li>\r\n          <span>内饰颜色</span>\r\n          <el-input type=\"text\" v-model=\"obj.innercolor\"></el-input>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"btn\">\r\n      <el-button type=\"success\" @click=\"submit\">保存</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 车型\r\n      title: '',\r\n      obj: {}\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取参数列表\r\n    async getTypeList () {\r\n      const id = this.$route.query.id\r\n      this.title = id\r\n      const { data: res } = await this.$http.post('/car/getConfig', {\r\n        c: id\r\n      })\r\n      this.obj = res\r\n    },\r\n    // 提交\r\n    async submit () {\r\n      const res = await this.$http.post('/car/update', this.obj)\r\n      if (res.status !== 200 || res.data !== true) return this.$message.error('修改失败')\r\n      this.$message.success('修改成功')\r\n    }\r\n  },\r\n  created () {\r\n    // 获取参数列表\r\n    this.getTypeList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n.main {\r\n  width: 750px;\r\n  margin: 0 auto;\r\n  li {\r\n    margin-bottom: 20px;\r\n  }\r\n  span {\r\n    display: inline-block;\r\n    width: 280px;\r\n  }\r\n  .el-input {\r\n    width: 400px;\r\n  }\r\n}\r\n.btn {\r\n  .el-button {\r\n    width: 750px;\r\n  }\r\n  width: 750px;\r\n  margin: 0 auto;\r\n}\r\n</style>\r\n"]}]}