 :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>营业执照副本</label>\r\n          <input type=\"file\" id=\"saveImage1\" name=\"myphoto1\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image1\">\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>经销商认证</label>\r\n          <el-select v-model=\"formLabelAlign.dealerRz\" placeholder=\"请选择\">\r\n            <el-option label=\"一级经销商\" value=\"一级经销商\"></el-option>\r\n            <el-option label=\"二级经销商\" value=\"二级经销商\"></el-option>\r\n            <el-option label=\"城市展厅\" value=\"城市展厅\"></el-option>\r\n          </el-select>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>厂商品牌授权书</label>\r\n          <input type=\"file\" id=\"saveImage2\" name=\"myphoto2\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image2\">\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>厂家门店编码</label>\r\n          <el-input v-model=\"formLabelAlign.cjmdbm\" placeholder=\"请输入厂家门店编码\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>店门口照片</label>\r\n          <input type=\"file\" id=\"saveImage3\" name=\"myphoto3\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image3\">\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>联系人电话</label>\r\n          <el-input v-model=\"formLabelAlign.phone\" placeholder=\"请输入联系人电话\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label></label>\r\n          <el-button type=\"primary\" style=\"width: 400px\" @click=\"submit\" :disabled=\"isDisabledForm\">提交审核</el-button>\r\n        </el-col>\r\n      </el-row>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 表单\r\n      formLabelAlign: {\r\n        yyzz: '',\r\n        csppsqs: '',\r\n        dmkzp: '',\r\n        id: '',\r\n        tyshxydm: '', // 统一社会信用代码\r\n        dealerRz: '', // 经销商认证\r\n        cjmdbm: '', // 门店编码\r\n        phone: '' // 联系人\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断提交按钮\r\n    isDisabledForm () {\r\n      if (this.formLabelAlign.tyshxydm !== '' && this.formLabelAlign.dealerRz !== '' && this.formLabelAlign.cjmdbm !== '' && this.formLabelAlign.phone !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  methods: {\r\n    // 提交表单\r\n    submit () {\r\n      if (this.$refs.new_image1.files[0] === undefined || this.$refs.new_image2.files[0] === undefined || this.$refs.new_image3.files[0] === undefined) {\r\n        return this.$message.warning('请上传照片')\r\n      }\r\n      const formData = new FormData()\r\n      formData.append('image_data', this.$refs.new_image1.files[0])\r\n      this.$http.post('/upload/addImage', formData, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data'\r\n        }\r\n      }).then(req => {\r\n        this.formLabelAlign.yyzz = req.data\r\n      })\r\n      formData.append('image_data', this.$refs.new_image2.files[0])\r\n      this.$http.post('/upload/addImage', formData, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data'\r\n        }\r\n      }).then(res => {\r\n        this.formLabelAlign.csppsqs = res.data\r\n      })\r\n      formData.append('image_data', this.$refs.new_image3.files[0])\r\n      this.$http.post('/upload/addImage', formData, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data'\r\n        }\r\n      }).then(rew => {\r\n        this.formLabelAlign.dmkzp = rew.data\r\n        if (rew.status !== 200) {\r\n          return\r\n        }\r\n        this.formLabelAlign.id = this.$route.query.id\r\n        console.log(this.formLabelAlign)\r\n        this.$http.post('/dealer/updateDealer', this.formLabelAlign).then(rey => {\r\n          // 提交信息\r\n          if (rey.data !== true) {\r\n            return\r\n          }\r\n          this.$router.push('/login')\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-header {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  padding: 0 100px;\r\n  background-color: #f6f6f6;\r\n  height: 100px!important;\r\n  line-height: 100px;\r\n  img {\r\n    width: 50px;\r\n    height: 50px;\r\n    vertical-align: middle;\r\n    margin-right: 40px;\r\n  }\r\n}\r\n.el-main {\r\n  margin-top: 40px;\r\n  .el-row {\r\n    margin-bottom: 20px;\r\n  }\r\n  label {\r\n    display: inline-block;\r\n    width: 150px;\r\n  }\r\n  .el-input {\r\n    width: 400px;\r\n  }\r\n  .el-select {\r\n    width: 400px;\r\n  }\r\n  // 图片上传\r\n}\r\n</style>\r\n"]}]}