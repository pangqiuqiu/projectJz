ow=\"item.tj === '1'\">下架推荐</el-button>\r\n      </el-col>\r\n      <el-col :span=\"17\" :offset=\"1\">\r\n        <el-button type=\"success\" @click=\"submit\" style=\"width: 86%\">确定提交</el-button>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      list: [],\r\n      // 报价\r\n      obj: {}\r\n    }\r\n  },\r\n  created () {\r\n    this.getList()\r\n  },\r\n  methods: {\r\n    // 经销商报价\r\n    async getList () {\r\n      const { data: res } = await this.$http.post('/dealer/Offer', {\r\n        token: window.sessionStorage.getItem('token')\r\n      })\r\n      this.list = res\r\n    },\r\n    // 提交\r\n    async submit () {\r\n      for (let i = 0; i < this.list.length; i++) {\r\n        var item = this.list[i]\r\n        this.obj[item.id] = item.offer\r\n        if (i === this.list.length-1) {\r\n          const res = await this.$http.post('/dealer/addOffer', {\r\n            token: window.sessionStorage.getItem('token'),\r\n            obj: JSON.stringify(this.obj)\r\n          })\r\n          if (res.status !== 200) {\r\n            return this.$message.error('修改失败')\r\n          }\r\n          if (res.data === true) {\r\n            this.$router.go(0)\r\n            this.$message.success('修改成功')\r\n          } else {\r\n            const ary = res.data\r\n            alert(ary + '不合法报价')\r\n          }\r\n        }\r\n      }\r\n    },\r\n    // 近期推荐(上)\r\n    async top (id, offer) {\r\n      const res = await this.$http.post('/dealer/tj', {\r\n        carId: id,\r\n        token: window.sessionStorage.getItem('token'),\r\n        tj: '1',\r\n        offer: offer\r\n      })\r\n      if (res.status !== 200 || res.data !== true) {\r\n        return this.$message.error('操作失败')\r\n      }\r\n      this.$router.go(0)\r\n    },\r\n    // 近期推荐(下)\r\n    async bottom (id) {\r\n      const res = await this.$http.post('/dealer/tj', {\r\n        carId: id,\r\n        token: window.sessionStorage.getItem('token'),\r\n        tj: '0'\r\n      })\r\n      if (res.status !== 200 || res.data !== true) {\r\n        return this.$message.error('操作失败')\r\n      }\r\n      this.$router.go(0)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.title {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  margin-bottom: 20px;\r\n  span {\r\n    font-size: 14px;\r\n    color: red;\r\n  }\r\n}\r\n.el-col {\r\n  margin-bottom: 20px\r\n}\r\nspan {\r\n  display: inline-block;\r\n  width: 435px;\r\n}\r\n.el-input {\r\n  width: 200px;\r\n  margin-right: 20px\r\n}\r\n</style>\r\n"]}]}