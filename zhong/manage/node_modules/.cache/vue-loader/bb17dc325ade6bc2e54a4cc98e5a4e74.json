tion label=\"地区论坛\" value=\"地区论坛\"></el-option>\r\n            <el-option label=\"主题论坛\" value=\"主题论坛\"></el-option>\r\n            <el-option label=\"车型论坛\" value=\"车型论坛\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <!-- 城市 -->\r\n        <el-form-item label=\"城市\" :label-width=\"formLabelWidth\" v-show=\"this.form.type === '地区论坛'\">\r\n          <el-select v-model=\"form.province\" placeholder=\"请输入省份\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n          <el-select v-model=\"form.city\" placeholder=\"请输入城市\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <!-- 主题 -->\r\n        <el-form-item label=\"主题\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.theme\" placeholder=\"请输入自定义主题\"></el-input>\r\n        </el-form-item>\r\n        <!-- 车型 -->\r\n        <el-form-item label=\"车型\" :label-width=\"formLabelWidth\">\r\n          <el-select v-model=\"form.brand\" placeholder=\"请选择品牌\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n          <el-select v-model=\"form.series\" placeholder=\"请选择车系\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogFormVisible = false\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 搜索条件 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select @change=\"changeProvince\" @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in provinceAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"城市\">\r\n          <el-select :disabled=\"disabledCity\" @visible-change=\"dropDownCity($event)\" v-model=\"formInline.city\" placeholder=\"请选择城市\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in cityAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"类型\">\r\n          <el-select v-model=\"formInline.type\" placeholder=\"请选择类型\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"主题论坛\" value=\"主题论坛\"></el-option>\r\n            <el-option label=\"地区论坛\" value=\"地区论坛\"></el-option>\r\n            <el-option label=\"车系论坛\" value=\"车系论坛\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"主题\">\r\n          <el-select @visible-change=\"dropDownTheme($event)\" v-model=\"formInline.theme\" placeholder=\"请选择主题\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in themeAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"品牌\">\r\n          <el-select @change=\"changeBrand\" @visible-change=\"dropDownBrand($event)\" v-model=\"formInline.brand\" placeholder=\"请选择品牌\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"(item,index) in brandAry\" :key=\"index\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"车系\">\r\n          <el-select :disabled=\"disabledSeries\" @visible-change=\"dropDownSeries($event)\" v-model=\"formInline.series\" placeholder=\"请选择车系\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"(item,index) in seriesAry\" :key=\"index\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" style=\"width: 100px\" size=\"mini\">搜索</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '', // 省份\r\n        city: '', // 城市\r\n        type: '',\r\n        theme: '',\r\n        brand: '',\r\n        series: ''\r\n      },\r\n      // 省份\r\n      provinceAry: [],\r\n      cityAry: [],\r\n      themeAry: [],\r\n      brandAry: [],\r\n      seriesAry: [],\r\n      // 弹框\r\n      dialogFormVisible: false,\r\n      formLabelWidth: '120px',\r\n      form: {\r\n        province: '',\r\n        city: '',\r\n        type: '',\r\n        theme: '',\r\n        brand: '',\r\n        series: ''\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断车系和城市下拉框\r\n    disabledCity () {\r\n      if (this.formInline.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    disabledSeries () {\r\n      if (this.formInline.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n  },\r\n  methods: {\r\n    // 省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getProvince')\r\n        this.provinceAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getCity', {\r\n          province: this.formInline.province\r\n        })\r\n        this.cityAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 主题\r\n    async dropDownTheme (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getTheme')\r\n        this.themeAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getBrand')\r\n        this.brandAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 车系\r\n    async dropDownSeries (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getSeries', {\r\n          brand: this.formInline.brand\r\n        })\r\n        this.seriesAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 改变省份和品牌\r\n    changeProvince () {\r\n      this.formInline.city = ''\r\n    },\r\n    changeBrand () {\r\n      this.formInline.series = ''\r\n    },\r\n    // 新增论坛\r\n    add () {\r\n    }\r\n  },\r\n  created () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  margin-bottom: 15px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n.el-dialog {\r\n  .el-select {\r\n    margin-bottom: 20px;\r\n    margin-right: 20px;\r\n  }\r\n}\r\n.el-form {\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n}\r\n</style>\r\n"]}]}