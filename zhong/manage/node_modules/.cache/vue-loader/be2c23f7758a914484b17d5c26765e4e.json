eader-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\">\r\n      <el-table-column prop=\"name\" label=\"课程名\"></el-table-column>\r\n      <el-table-column prop=\"nums\" label=\"观看数\"></el-table-column>\r\n      <el-table-column label=\"封面图\">\r\n        <template slot-scope=\"scope\">\r\n          <img :src=\"scope.row.coverPic\" alt=\"\">\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" size=\"mini\" @click=\"edit(scope.row.id)\">编辑</el-button>\r\n          <el-popconfirm\r\n            title=\"您确定要删除吗?\"\r\n            cancel-button-type=\"primary\"\r\n            confirm-button-type=\"danger\"\r\n            @onConfirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" size=\"mini\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"formAlign.pageNo\"\r\n      :page-size=\"formAlign.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 参数\r\n      formAlign: {\r\n        pageNo: 1,\r\n        pageSize: 15\r\n      },\r\n      // 课程列表\r\n      tableData: [],\r\n      total: 0\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取课程列表\r\n    async getTable () {\r\n      const { data: res } = await this.$http.post('/us/showCourse', this.formAlign)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.formAlign.pageNo = index\r\n      this.getTable()\r\n    },\r\n    // 新增课程\r\n    add () {\r\n      this.$router.push('/us/cooperation/curriculumdetails')\r\n    },\r\n    // 编辑\r\n    edit (id) {\r\n      this.$router.push(`/us/cooperation/curriculumdetails?id=${id}`)\r\n    },\r\n    // 删除\r\n    async del (id) {\r\n      const { data: res } = await this.$http.post('/us/delCourse', { id })\r\n      if (!res) return this.$message.error('删除失败')\r\n      this.$message.success('删除成功')\r\n      this.getTable()\r\n    }\r\n  },\r\n  created () {\r\n    this.getTable()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 新增按钮\r\n.btn {\r\n  margin: 10px 0;\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"]}]}