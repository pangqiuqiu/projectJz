CAAA,CAAA;AACA,4BAAA,MAAA,CAAA,IAAA,CAAA,aAAA,GAAA,MAAA,CAAA,cAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AACA,4BAAA,MAAA,CAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AAjBA;AAAA,mCAkBA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA,MAAA,CAAA,IAAA,CAlBA;;AAAA;AAkBA,4BAAA,GAlBA;;AAAA,kCAmBA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GAnBA;AAAA;AAAA;AAAA;;AAAA,8DAoBA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CApBA;;AAAA;AAsBA,4BAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;;AACA,4BAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AAvBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAAA;;AAAA;AAAA;AAAA;AAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2BA,KA/DA;AAgEA;AACA,IAAA,kBAjEA,gCAiEA;AACA,UAAA,IAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,KApEA;AAqEA;AACA,IAAA,UAtEA,wBAsEA;AACA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,KAAA,GAAA,KAAA,IAAA,MAAA,CAAA,KAAA;AACA,UAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,MAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,MAAA,GAAA,IAAA,UAAA,EAAA,CADA,CACA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,CAAA;;AACA,QAAA,MAAA,CAAA,aAAA,CAAA,IAAA,CAAA,CAAA,CAAA;;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,UAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,CAAA,MAAA,CAAA,MAAA,EADA,CACA;;AACA,SAFA;AAGA;AACA,KApFA;AAqFA,IAAA,GArFA,eAqFA,CArFA,EAqFA;AACA;AACA,WAAA,QAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA,WAAA,IAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AAzFA,GA/BA;AA0HA,EAAA,OA1HA,qBA0HA;AACA;AACA,SAAA,QAAA;AACA;AA7HA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 头部标题 -->\r\n    <div class=\"HeadTitle\">\r\n      <h2>新增特卖</h2>\r\n      <el-button type=\"text\" @click=\"back\">《《 返回</el-button>\r\n    </div>\r\n    <!-- 注意事项 -->\r\n    <div class=\"notice\">注：每次只发布一个品牌特卖系列</div>\r\n    <!-- 新增特卖表单 -->\r\n    <el-form label-position=\"right\" label-width=\"80px\" :model=\"form\" :rules=\"rules\" ref=\"rulesForm\">\r\n      <el-form-item label=\"品牌\" prop=\"brand\">\r\n        <el-select v-model=\"form.brand\" placeholder=\"请选择特卖品牌\" @change=\"changeBrand\">\r\n          <el-option v-for=\"item in brandAry\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"系列\" prop=\"series\" v-show=\"this.form.brand !== ''\">\r\n        <el-checkbox-group v-model=\"form.series\" @change=\"changeSeries\">\r\n          <el-checkbox v-for=\"(item,index) in seriesAry\" :key=\"index\" :label=\"item\"></el-checkbox>\r\n        </el-checkbox-group>\r\n      </el-form-item>\r\n      <el-form-item label=\"特卖时间\">\r\n        <div class=\"block\">\r\n          <el-date-picker v-model=\"time\" type=\"datetimerange\" range-separator=\"至\" start-placeholder=\"开始日期\" end-placeholder=\"结束日期\"></el-date-picker>\r\n        </div>\r\n      </el-form-item>\r\n      <el-form-item label=\"车型照片\">\r\n        <div style=\"color: #999\">每个系列一张即可，超出部分只截取符合条件的个数 (建议尺寸 270 x 200, 图片格式为png/gif/jpeg)</div>\r\n        <div v-if=\"imgs.length > 0\" class=\"flex\">\r\n          <div class=\"img-box\" v-for=\"(item, i) in imgs\" :key=\"i\">\r\n            <div>\r\n              <img :src=\"item\" alt />\r\n              <span @click=\"del(i)\"><i class=\"el-icon-delete\"></i></span>\r\n            </div>\r\n          </div>\r\n          <div style=\"clear: both;\"></div>\r\n        </div>\r\n        <div class=\"img-file\" v-if=\"imgs.length < this.form.series.length\">\r\n          <input type=\"file\" id=\"files\" @change=\"fileChange\" />\r\n        </div>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submit('rulesForm')\">确认发布</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 品牌\r\n      brandAry: [],\r\n      // 系列\r\n      seriesAry: [],\r\n      // 特卖时间\r\n      time: [],\r\n      // 表单数据\r\n      form: {\r\n        ary: [],\r\n        brand: '', // 品牌\r\n        series: [], // 系列\r\n        activityDateS: '', // 开始时间\r\n        activityDateE: '' // 结束时间\r\n      },\r\n      // 表单验证\r\n      rules: {\r\n        brand: [\r\n          { required: true, message: '请选择特卖品牌', trigger: 'change' }\r\n        ],\r\n        series: [\r\n          { type: 'array', required: true, message: '请至少选择一个系列', trigger: 'change' }\r\n        ]\r\n      },\r\n      // 图片上传\r\n      imgs: [], // 图片预览地址\r\n      imgfiles: [] // 图片原文件，上传到后台的数据\r\n    }\r\n  },\r\n  methods: {\r\n    // 返回上一页面\r\n    back () {\r\n      this.$router.go(-1)\r\n    },\r\n    // 获取品牌\r\n    async getBrand () {\r\n      const { data: res } = await this.$http.post('/sale/showAddSaleBrand', {\r\n        token: window.sessionStorage.getItem('token')\r\n      })\r\n      this.brandAry = res\r\n    },\r\n    // 获取系列\r\n    async changeBrand () {\r\n      const{ data: res } = await this.$http.post('/sale/showSaleSeries', {\r\n        brand: this.form.brand\r\n      })\r\n      this.seriesAry = res\r\n    },\r\n    // 改变系列\r\n    changeSeries () {\r\n      this.imgs = []\r\n    },\r\n    // 事件处理函数\r\n    dateConversion (value) {\r\n      function checkTime (i) {\r\n        if (i < 10) {\r\n          i = '0' + i\r\n        }\r\n        return i\r\n      }\r\n      var d = new Date(value)\r\n      var date = d.getFullYear() + '-' + checkTime(d.getMonth() + 1) + '-' + checkTime(d.getDate())\r\n      return date\r\n    },\r\n    // 提交特卖\r\n    async submit (formName) {\r\n      this.form.activityDateS = this.dateConversion(this.time[0])\r\n      this.$refs[formName].validate(async valid => {\r\n        if (!valid || this.form.activityDateS === 'NaN-NaN-NaN NaN:NaN:NaN') {\r\n          return\r\n        }\r\n        let ary = []\r\n        for(var i = 0; i < this.imgfiles.length; i++) {\r\n          const formData = new FormData()\r\n          formData.append('image_data', this.imgfiles[i])\r\n          const { data: res } = await this.$http.post('/upload/addImage', formData, {\r\n            headers: {\r\n              'Content-Type': 'multipart/form-data'\r\n            }\r\n          })\r\n          ary.push(res)\r\n        }\r\n        this.form.activityDateS = this.dateConversion(this.time[0])\r\n        this.form.activityDateE = this.dateConversion(this.time[1])\r\n        this.form.ary = ary\r\n        const rew = await this.$http.post('/sale/addSale', this.form)\r\n        if (rew.data !== true || rew.status !== 200) {\r\n          return this.$message.error('新增失败')\r\n        }\r\n        this.$router.go(0)\r\n        this.$message.success('新增成功')\r\n      })\r\n    },\r\n    // 获取资讯类型\r\n    getInformationType () {\r\n      const type = this.$route.query.type\r\n      this.title = type\r\n    },\r\n    // 上传图片\r\n    fileChange () {\r\n      // 加入图片\r\n      var _this = this\r\n      var event = event || window.event\r\n      var file = event.target.files\r\n      var leng = file.length\r\n      for (var i = 0; i < leng; i++) {\r\n        var reader = new FileReader() // 使用 FileReader 来获取图片路径及预览效果\r\n        _this.imgfiles.push(file[i])\r\n        reader.readAsDataURL(file[i])\r\n        reader.onload = function(e) {\r\n          _this.imgs.push(e.target.result) // base 64 图片地址形成预览\r\n        }\r\n      }\r\n    },\r\n    del (i) {\r\n      // 根据下标删除\r\n      this.imgfiles.splice(i, 1)\r\n      this.imgs.splice(i, 1)\r\n    }\r\n  },\r\n  created () {\r\n    // 获取品牌\r\n    this.getBrand()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 头部标题\r\n.HeadTitle {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n// 注意\r\n.notice {\r\n  color: red;\r\n  font-size: 18px;\r\n  height: 60px;\r\n  line-height: 60px;\r\n  padding-left: 20px;\r\n}\r\n// 新增表单\r\n.el-form {\r\n  margin-top: 30px;\r\n  .el-form-item {\r\n    margin-bottom: 20px;\r\n  }\r\n  .el-button {\r\n    margin-top: 20px;\r\n    width: 300px;\r\n  }\r\n  .img-box {\r\n    float: left;\r\n  }\r\n  img {\r\n    width: 270px;\r\n    height: 200px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/specialSale"}]}