CAAA,iBAAA,GAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAxDA;AAyDA;AACA,IAAA,UA1DA,sBA0DA,GA1DA,EA0DA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,kBAAA,QAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;AAIA,gBAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,iBAAA,EAAA,GAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAjEA;AAkEA;AACA,IAAA,WAnEA,yBAmEA;AAAA;;AACA,UAAA,MAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,KAAA,CAAA,UAAA,CAAA,KAAA,CAAA,CAAA,CADA;AAEA,QAAA,OAAA,EAAA,KAAA,KAAA,CAAA,UAAA,CAAA,KAAA,CAAA,CAAA,CAFA;AAGA,QAAA,KAAA,EAAA,KAAA,KAAA,CAAA,UAAA,CAAA,KAAA,CAAA,CAAA;AAHA,OAAA;;AAKA,WAAA,IAAA,CAAA,IAAA,MAAA,EAAA;AACA,YAAA,MAAA,CAAA,CAAA,CAAA,KAAA,SAAA,EAAA;AACA,iBAAA,MAAA,CAAA,CAAA,CAAA;AACA;AACA,OAVA,CAWA;;;AACA,UAAA,IAAA,CAAA,SAAA,CAAA,MAAA,MAAA,IAAA,EAAA;AACA,aAAA,IAAA,CAAA,EAAA,GAAA,KAAA,GAAA;AACA,YAAA,GAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,sBAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,mBAAA,KAAA;AACA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA,SANA,CAAA;AAOA,OATA,MASA;AACA,aAAA,IAAA,CAAA,IAAA,MAAA,EAAA;AACA,cAAA,QAAA,GAAA,IAAA,QAAA,EAAA;AACA,UAAA,QAAA,CAAA,MAAA,CAAA,YAAA,EAAA,MAAA,CAAA,CAAA,CAAA;AACA,eAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,QAAA,EAAA;AACA,YAAA,OAAA,EAAA;AACA,8BAAA;AADA;AADA,WAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA;AACA;;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,EAAA,GAAA,MAAA,CAAA,GAAA;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,CAAA,IAAA,GAAA,CAAA,IAAA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,sBAAA,EAAA,MAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,kBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,uBAAA,KAAA;AACA;;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA,aANA;AAOA,WAjBA;AAkBA;AACA;AACA,KAhHA;AAiHA;AACA,IAAA,UAlHA,sBAkHA,KAlHA,EAkHA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,MAAA;AACA;AArHA,GA5CA;AAmKA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,YAFA,0BAEA;AACA,UAAA,KAAA,UAAA,CAAA,QAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAPA,GAnKA;AA4KA,EAAA,OA5KA,qBA4KA;AACA;AACA,SAAA,cAAA;AACA,GA/KA;AAgLA,EAAA,OAhLA,qBAgLA,CACA;AAjLA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h2>经销商列表</h2>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\" @change=\"changeP\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.province\" :value=\"item.province\" v-for=\"item in provinceAry\" :key=\"item.province\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"城市\">\r\n          <el-select @change=\"ggg\" :disabled=\"disabledCity\" @visible-change=\"dropDownCity($event)\" v-model=\"formInline.city\" placeholder=\"请选择城市\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.city\" :value=\"item.city\" v-for=\"item in cityAry\" :key=\"item.cityId\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"品牌\">\r\n          <el-select @change=\"ggg\" v-model=\"formInline.brand\" placeholder=\"请选择品牌\" @visible-change=\"dropDownBrand($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"item in brandAry\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"4S店\">\r\n          <el-input @change=\"ggg\" v-model=\"formInline.name4s\" placeholder=\"店铺名称\" size=\"mini\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"id\"\r\n        label=\"编号\"\r\n        width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"city\"\r\n        label=\"城市\"\r\n        width=\"170\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"brand\"\r\n        label=\"品牌\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"name4s\"\r\n        label=\"店铺名称\"\r\n        >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"state4s\"\r\n        label=\"状态\"\r\n        width=\"150\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n          <el-button\r\n          size=\"mini\"\r\n          type=\"success\"\r\n          @click=\"handleSign(scope.row)\">模拟登录</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 对话框 -->\r\n    <el-dialog :info='msg' title=\"经销商信息\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form :model=\"form\">\r\n        <el-form-item label=\"公司名称\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.company\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"公司地址\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.companyAddress\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"门店地址\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"经销商认证\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.dealerRz\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"统一社会信用代码\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.tyshxydm\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"厂商门店编码\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.cjmdbm\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"营业执照副本\" :label-width=\"formLabelWidth\">\r\n          <img :src=\"form.yyzz\" alt=\"\">\r\n          <input type=\"file\" id=\"saveImage\" name=\"myphoto\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image1\">\r\n        </el-form-item>\r\n        <el-form-item label=\"厂商品牌授权书\" :label-width=\"formLabelWidth\">\r\n          <img :src=\"form.csppsqs\" alt=\"\">\r\n          <input type=\"file\" id=\"saveImage\" name=\"myphoto\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image2\">\r\n        </el-form-item>\r\n        <el-form-item label=\"店门口照片\" :label-width=\"formLabelWidth\">\r\n          <img :src=\"form.dmkzp\" alt=\"\">\r\n          <input type=\"file\" id=\"saveImage\" name=\"myphoto\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image3\">\r\n        </el-form-item>\r\n        <el-form-item label=\"所属品牌\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.brand\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"联系人\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.link\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机号\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogCheck\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      flag: true,\r\n      // 对话框\r\n      dialogFormVisible: false,\r\n      msg: '',\r\n      formLabelWidth: '150px',\r\n      form: {\r\n        company: '',\r\n        companyAddress: '',\r\n        address: '',\r\n        dealerRz: '',\r\n        tyshxydm: '',\r\n        cjmdbm: '',\r\n        brand: '',\r\n        link: '',\r\n        phone: '',\r\n        id: ''\r\n      },\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '',\r\n        city: '',\r\n        brand: '',\r\n        name4s: '',\r\n        state4s: '已上线',\r\n        pageNo: 1,\r\n        pageSize: 20\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 省份\r\n      provinceAry: [],\r\n      // 城市\r\n      cityAry: [],\r\n      // 品牌\r\n      brandAry: [],\r\n      // 表格\r\n      tableData: [],\r\n      // 图片数组\r\n      pic: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取经销商列表\r\n    async getSpecialSale () {\r\n      const { data: res } = await this.$http.post('/dealer/DealerList', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 获取省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getProvince')\r\n        this.provinceAry = res\r\n      }\r\n    },\r\n    changeP () {\r\n      this.flag = false\r\n      this.formInline.city = ''\r\n    },\r\n    // 获取城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getCityByPid', {\r\n          province: this.formInline.province\r\n        })\r\n        this.cityAry = res\r\n      }\r\n    },\r\n    // 获取品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleBrand')\r\n        this.brandAry = res\r\n      }\r\n    },\r\n    // 查询\r\n    ggg (index) {\r\n      if (index !== undefined) {\r\n        this.formInline.pageNo = 1\r\n      }\r\n    },\r\n    submit () {\r\n      if (this.flag === false) {\r\n        this.formInline.pageNo = 1\r\n      }\r\n      this.ggg()\r\n      this.getSpecialSale()\r\n      this.flag = true\r\n    },\r\n    // 编辑\r\n    async handleEdit (row) {\r\n      this.msg = row.id\r\n      const { data: res } = await this.$http.post('/dealer/getDealer', {\r\n        id: row.id\r\n      })\r\n      this.form = res\r\n      this.dialogFormVisible = true\r\n    },\r\n    // 模拟登录\r\n    async handleSign (row) {\r\n      const { data: res } = await this.$http.post('/user/mnLogin', {\r\n        dealerId: row.id\r\n      })\r\n      window.sessionStorage.setItem('simulationToken', res)\r\n      this.$router.push('/')\r\n      this.$router.go(0)\r\n    },\r\n    // 确定提交\r\n    dialogCheck () {\r\n      const picObj = {\r\n        yyzz: this.$refs.new_image1.files[0],\r\n        csppsqs: this.$refs.new_image2.files[0],\r\n        dmkzp: this.$refs.new_image3.files[0]\r\n      }\r\n      for (var i in picObj) {\r\n        if(picObj[i] === undefined) {\r\n          delete picObj[i]\r\n        }\r\n      }\r\n      // 判断是否修改了图片\r\n      if (JSON.stringify(picObj) === \"{}\") {\r\n        this.form.id = this.msg\r\n        const res = this.$http.post('/dealer/updateDealer', this.form).then(res => {\r\n          if (res.status !== 200 || res.data !== true) {\r\n            return false\r\n          }\r\n          this.$message.success('修改成功')\r\n          this.dialogFormVisible = false\r\n        })\r\n      } else {\r\n        for(var i in picObj) {\r\n          const formData = new FormData()\r\n          formData.append('image_data', picObj[i])\r\n          this.$http.post('/upload/addImage', formData, {\r\n            headers: {\r\n              'Content-Type': 'multipart/form-data'\r\n            }\r\n          }).then(res => {\r\n            if (res.status !== 200) {\r\n              return\r\n            }\r\n            this.form.id = this.msg\r\n            this.form[i] = res.data\r\n            this.$http.post('/dealer/updateDealer', this.form).then(rew => {\r\n              if (rew.status !== 200 || rew.data !== true) {\r\n                return false\r\n              }\r\n              this.$message.success('修改成功')\r\n              this.dialogFormVisible = false\r\n            })\r\n          })\r\n        }\r\n      }\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.submit()\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断城市下拉框\r\n    disabledCity () {\r\n      if (this.formInline.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  created () {\r\n    // 获取特卖列表\r\n    this.getSpecialSale()\r\n  },\r\n  mounted () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/distributor"}]}