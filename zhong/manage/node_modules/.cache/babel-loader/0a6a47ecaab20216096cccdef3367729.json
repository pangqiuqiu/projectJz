EAAA;AACA,eAAA,KAAA,QAAA,CAAA,KAAA,CAAA,WAAA,CAAA;AACA;;AACA,UAAA,KAAA,QAAA,GAAA,CAAA,EAAA;AACA;AACA;;AACA,WAAA,QAAA,GAAA,EAAA;AACA,UAAA,KAAA,GAAA,WAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA;;AACA,YAAA,KAAA,CAAA,QAAA,GAAA,CAAA,EAAA;AACA,UAAA,aAAA,CAAA,KAAA,CAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,CAAA;AACA;AACA,OANA,EAMA,IANA,CAAA;AAOA,KA1BA;AA2BA;AACA,IAAA,SA5BA,uBA4BA;AAAA,kCACA,KAAA,OAAA,CAAA,OAAA,CAAA,YAAA,CADA;AAAA,UACA,IADA,yBACA,IADA;;AAEA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,KA/BA;AAgCA;AACA,IAAA,MAjCA,oBAiCA;AACA,UAAA,CAAA,MAAA,CAAA,IAAA,CAAA,KAAA,cAAA,CAAA,KAAA,CAAA,EAAA;AACA,eAAA,KAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,cAAA,EAJA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,UAAA;AACA;AAxCA;AA3BA,CAAA","sourcesContent":["<template>\r\n  <el-container>\r\n    <el-header>\r\n      <!-- logo -->\r\n      <div><img src=\"../../assets/logo.png\" alt=\"\"></div>\r\n      <!-- 注册 -->\r\n      <h2>欢迎注册</h2>\r\n    </el-header>\r\n    <el-main>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>手机号</label>\r\n          <el-input @blur=\"judgeMobile\" v-model=\"formLabelAlign.phone\" placeholder=\"请输入手机号\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>设置密码</label>\r\n          <el-input v-model=\"formLabelAlign.psd1\" placeholder=\"请输入密码\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>确认密码</label>\r\n          <el-input v-model=\"formLabelAlign.psd2\" placeholder=\"请确认密码\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>验证码</label>\r\n          <el-input v-model=\"formLabelAlign.code\" placeholder=\"请输入验证码\" style=\"width: 270px;margin-right: 20px\"></el-input>\r\n          <el-button @click=\"code\" :disabled=\"isDisabled\" type=\"success\">{{!codetime?'发送验证码':codetime + 's后重新获取'}}</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <div>\r\n            <label></label>\r\n            <el-checkbox v-model=\"checked\">我已看过并同意 <el-button type=\"text\" @click=\"agreement\">《用户协议》</el-button></el-checkbox>\r\n          </div>\r\n          <div>\r\n            <label></label>\r\n            <el-button type=\"primary\" style=\"width: 400px\" @click=\"submit\" :disabled=\"isDisabledForm\">注册</el-button>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\nconst mobile = /^[1]([3-9])[0-9]{9}$/\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 复选框\r\n      checked: false,\r\n      // 验证码倒计时\r\n      codetime: 0,\r\n      // 发送验证码按钮\r\n      isDisabled: false,\r\n      // 表单\r\n      formLabelAlign: {\r\n        phone: '',\r\n        psd1: '',\r\n        psd2: '',\r\n        code: ''\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    isDisabledForm () {\r\n      // 判断提交按钮\r\n      if (this.checked === true && this.formLabelAlign.psd1.trim().length !== 0 && this.formLabelAlign.psd1 === this.formLabelAlign.psd2 && this.formLabelAlign.code.trim().length === 4) {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  methods: {\r\n    // 判断手机号是否注册\r\n    judgeMobile () {\r\n      if (this.formLabelAlign.phone.trim().length === 11) {\r\n        console.log('已被注册')\r\n        // this.isDisabled = true\r\n      } else {\r\n        this.isDisabled = false\r\n      }\r\n    },\r\n    // 获取验证码\r\n    code () {\r\n      if (!mobile.test(this.formLabelAlign.phone)) {\r\n        return this.$message.error('请输入正确的手机号')\r\n      }\r\n      if (this.codetime > 0) {\r\n        return\r\n      }\r\n      this.codetime = 60\r\n      const timer = setInterval(() => {\r\n        this.codetime--\r\n        if (this.codetime < 1) {\r\n          clearInterval(timer)\r\n          this.codetime = 0\r\n        }\r\n      }, 1000)\r\n    },\r\n    // 用户协议\r\n    agreement () {\r\n      const { href } = this.$router.resolve('/agreement')\r\n      window.open(href, '_blank')\r\n    },\r\n    // 提交表单\r\n    submit () {\r\n      if (!mobile.test(this.formLabelAlign.phone)) {\r\n        return this.$message.error('手机号不合法')\r\n      }\r\n      console.log(this.formLabelAlign)\r\n      // 提交成功\r\n      this.$router.push('/company')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-header {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  padding: 0 100px;\r\n  background-color: #f6f6f6;\r\n  height: 100px!important;\r\n  line-height: 100px;\r\n  img {\r\n    width: 50px;\r\n    height: 50px;\r\n    vertical-align: middle;\r\n    margin-right: 40px;\r\n  }\r\n}\r\n.el-main {\r\n  margin-top: 40px;\r\n  .el-row {\r\n    margin-bottom: 20px;\r\n  }\r\n  label {\r\n    display: inline-block;\r\n    width: 80px;\r\n  }\r\n  .el-input {\r\n    width: 400px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/settledIn"}]}