AA,EAFA;AAGA;AACA,MAAA,IAAA,EAAA;AACA,QAAA,KAAA,EAAA,EADA;AACA;AACA,QAAA,QAAA,EAAA,EAFA;AAEA;AACA,QAAA,MAAA,EAAA,EAHA;AAGA;AACA,QAAA,OAAA,EAAA,EAJA,CAIA;;AAJA,OAJA;AAUA;AACA,MAAA,GAAA,EAAA,EAXA;AAYA;AACA,MAAA,YAAA,EAAA;AACA,QAAA,OAAA,EAAA;AACA,UAAA,OAAA,EAAA;AACA,YAAA,SAAA,EAAA,cADA,CACA;;AADA;AADA;AADA;AAbA,KAAA;AAqBA,GAvBA;AAwBA,EAAA,QAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,aAAA,KAAA,KAAA,CAAA,aAAA,CAAA,KAAA;AACA;AAHA,GAxBA;AA6BA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,kBAFA,gCAEA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,EADA;AAAA;AAAA,uBAEA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA;AACA,kBAAA,EAAA,EAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,QAEA,IAFA;AAKA,gBAAA,KAAA,CAAA,GAAA,GAAA,GAAA,CAAA,QAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,gBAAA,KAAA,CAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,KAAA;AACA,gBAAA,KAAA,CAAA,IAAA,CAAA,MAAA,GAAA,GAAA,CAAA,QAAA;AACA,gBAAA,KAAA,CAAA,IAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAbA;AAcA;AACA,IAAA,IAfA,kBAeA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AAjBA,GA7BA;AAgDA,EAAA,OAhDA,qBAgDA;AACA;AACA,SAAA,kBAAA;AACA;AAnDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 头部标题 -->\r\n    <div class=\"HeadTitle\">\r\n      <h3>{{this.title}}</h3>\r\n      <el-button type=\"text\" @click=\"back\">《《 返回</el-button>\r\n    </div>\r\n    <!-- 新增资讯表单 -->\r\n    <el-form label-position=\"right\" label-width=\"80px\" :model=\"form\" ref=\"rulesForm\">\r\n      <el-form-item label=\"标题\">\r\n        <el-input v-model=\"form.title\" placeholder=\"请输入标题，不超过25个汉字\" disabled></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"摘要\">\r\n        <el-input style=\"margin-bottom: 10px;\" disabled placeholder=\"不超过120字\" type=\"textarea\" :rows=\"2\" v-model=\"form.abstract\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"作者\">\r\n        <el-input v-model=\"form.author\" disabled placeholder=\"请输入作者名称\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"封面图\">\r\n        <img :src=\"this.pic\" alt=\"\">\r\n      </el-form-item>\r\n      <!-- 富文本 -->\r\n      <div class=\"edit_container\">\r\n        <quill-editor\r\n          style=\"height: 400px\"\r\n          v-model='form.content'\r\n          ref='myQuillEditor'\r\n          :options=\"editorOption\"\r\n          >\r\n        </quill-editor>\r\n      </div>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst toolbarOptions = [\r\n  ['bold', 'italic', 'underline', 'strike'], // 加粗，斜体，下划线，删除\r\n  [{ 'header': 1 }, { 'header': 2 }], // 标题，键值对的形式；1、2表示字体大小\r\n  [{ 'script': 'sub' }, { 'script': 'super' }], // 上下标\r\n  [{ 'indent': '-1' }, { 'indent': '+1' }], // 缩进\r\n  [{ 'direction': 'rtl' }], // 文本方向\r\n  [{ 'size': ['small', false, 'large', 'huge'] }], // 字体大小\r\n  [{ 'header': [1, 2, 3, 4, 5, 6, false] }], // 几级标题\r\n  [{ 'color': [] }, { 'background': [] }], // 字体颜色，字体背景颜色\r\n  [{ 'font': [] }], // 字体\r\n  // [{ 'align': [] }], // 对齐方式\r\n  ['image'] // 上传图片、上传视频\r\n]\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 资讯标题\r\n      title: '',\r\n      // 表单数据\r\n      form: {\r\n        title: '', // 标题\r\n        abstract: '', // 摘要\r\n        author: '', // 作者\r\n        content: '' // 正文\r\n      },\r\n      // 图片\r\n      pic: '',\r\n      // 富文本\r\n      editorOption: {\r\n        modules: {\r\n          toolbar: {\r\n            container: toolbarOptions // 工具栏\r\n          }\r\n        }\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    editor () {\r\n      return this.$refs.myQuillEditor.quill\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取资讯\r\n    async getInformationType () {\r\n      const id = this.$route.query.id\r\n      const { data: res } = await this.$http.post('/Infor/getOneInfor', {\r\n        id: id\r\n      })\r\n      this.pic = res.coverPic\r\n      this.title = res.type\r\n      this.form.title = res.title\r\n      this.form.abstract = res.intro\r\n      this.form.author = res.username\r\n      this.form.content = res.text\r\n    },\r\n    // 返回上一页面\r\n    back () {\r\n      this.$router.go(-1)\r\n    }\r\n  },\r\n  created () {\r\n    // 获取资讯\r\n    this.getInformationType()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 头部标题\r\n.HeadTitle {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n// 新增表单\r\n.el-form {\r\n  margin-top: 30px;\r\n  .el-form-item {\r\n    margin-bottom: 0px;\r\n  }\r\n  .el-input {\r\n    margin-bottom: 10px;\r\n    width: 400px;\r\n  }\r\n  .el-button {\r\n    margin-top: 20px;\r\n  }\r\n}\r\n// 编辑器\r\n.edit_container {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  /* text-align: center; */\r\n  color: #2c3e50;\r\n  margin-top: 30px;\r\n  margin-bottom: 30px;\r\n  width: 873px\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/information"}]}