tion label=\"一级经销商\" value=\"一级经销商\"></el-option>\r\n            <el-option label=\"二级经销商\" value=\"二级经销商\"></el-option>\r\n            <el-option label=\"城市展厅\" value=\"城市展厅\"></el-option>\r\n          </el-select>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>厂商品牌授权书</label>\r\n          <el-input v-model=\"formLabelAlign.store\" placeholder=\"请输入厂商品牌授权书\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>厂家门店编码</label>\r\n          <el-input v-model=\"formLabelAlign.store\" placeholder=\"请输入厂家门店编码\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label>店门口照片</label>\r\n          <span>1</span>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"12\" :offset=\"6\">\r\n          <label></label>\r\n          <el-button type=\"primary\" style=\"width: 400px\" @click=\"submit\" :disabled=\"isDisabledForm\">提交审核</el-button>\r\n        </el-col>\r\n      </el-row>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 表单\r\n      formLabelAlign: {\r\n        company: '',\r\n        province: '',\r\n        city: '',\r\n        address: '',\r\n        store: '',\r\n        brand: []\r\n      },\r\n      // 省份城市品牌\r\n      provinceAry: [],\r\n      cityAry: [],\r\n      brandAry: []\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断城市下拉框\r\n    isDisabledCity () {\r\n      if (this.formLabelAlign.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    // 提交按钮\r\n    isDisabledForm () {\r\n      if (this.formLabelAlign.company.trim() !== '' && this.formLabelAlign.province !== '' && this.formLabelAlign.city !== '' && this.formLabelAlign.address.trim() !== '' && this.formLabelAlign.store.trim() !== '' && this.formLabelAlign.brand.length !== 0) {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getProvince')\r\n        this.provinceAry = res\r\n      }\r\n    },\r\n    // 获取城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getCityByPid', {\r\n          provinceid: this.formLabelAlign.province\r\n        })\r\n        this.cityAry = res\r\n      }\r\n    },\r\n    // 获取品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/Infor/getInforBrand')\r\n        this.brandAry = res\r\n      }\r\n    },\r\n    // 提交表单\r\n    submit () {\r\n      if (!mobile.test(this.formLabelAlign.phone)) {\r\n        return this.$message.error('手机号不合法')\r\n      }\r\n      console.log(this.formLabelAlign)\r\n      // 提交成功\r\n      this.$router.push('/all')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-header {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  padding: 0 100px;\r\n  background-color: #f6f6f6;\r\n  height: 100px!important;\r\n  line-height: 100px;\r\n  img {\r\n    width: 50px;\r\n    height: 50px;\r\n    vertical-align: middle;\r\n    margin-right: 40px;\r\n  }\r\n}\r\n.el-main {\r\n  margin-top: 40px;\r\n  .el-row {\r\n    margin-bottom: 20px;\r\n  }\r\n  label {\r\n    display: inline-block;\r\n    width: 150px;\r\n  }\r\n  .el-input {\r\n    width: 400px;\r\n  }\r\n  .el-select {\r\n    width: 400px;\r\n  }\r\n  // 图片上传\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/settledIn"}]}