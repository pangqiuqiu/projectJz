A;AAFA,OAAA,EAGA,KAHA,CAGA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,OALA,EAKA,IALA;AAAA,4EAKA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBAAA,KAAA,SAAA,KAAA;AAAA;AAAA,yBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA;AACA,oBAAA,IAAA,EAAA;AADA,mBAAA,CADA;;AAAA;AACA,kBAAA,GADA;;AAAA,wBAIA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GAJA;AAAA;AAAA;AAAA;;AAAA,oDAIA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAJA;;AAAA;AAKA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,QAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SALA;;AAAA;AAAA;AAAA;AAAA;AAaA,KAtBA;AAuBA;AACA,IAAA,QAxBA,sBAwBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,mBAAA;AACA,KA1BA;AA2BA;AACA,IAAA,eA5BA,6BA4BA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,oBAAA,MAAA,CAAA,IAAA,KAAA,IAAA,IAAA,MAAA,CAAA,IAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,IAAA,GAAA,EAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,UAAA,CAAA,WAAA,GAAA,cAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AACA,kBAAA,MAAA,CAAA,UAAA,CAAA,WAAA,GAAA,cAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AACA,iBANA,CAOA;;;AAPA;AAAA,uBAQA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,wBAAA,EAAA,MAAA,CAAA,UAAA,CARA;;AAAA;AAAA;AAQA,gBAAA,GARA,SAQA,IARA;AASA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,CAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,SAAA,CAAA,MAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAvCA;AAwCA,IAAA,WAxCA,yBAwCA;AACA,WAAA,OAAA,CAAA,IAAA;AACA,KA1CA;AA2CA;AACA,IAAA,QA5CA,sBA4CA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;AAEA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KA/CA;AAgDA;AACA,IAAA,GAjDA,eAiDA,KAjDA,EAiDA;AACA,UAAA,KAAA,KAAA,SAAA,EAAA;AACA,aAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA;AACA,KArDA;AAsDA,IAAA,MAtDA,oBAsDA;AACA,UAAA,KAAA,IAAA,KAAA,KAAA,EAAA;AACA,aAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA;;AACA,WAAA,GAAA;AACA,WAAA,eAAA;AACA,KA5DA;AA6DA;AACA;AACA,IAAA,UA/DA,sBA+DA,KA/DA,EA+DA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,eAAA;AACA;AAlEA,GA5BA;AAgGA,EAAA,QAAA,EAAA,EAhGA;AAiGA,EAAA,OAjGA,qBAiGA;AACA;AACA,SAAA,eAAA,GAFA,CAGA;;AACA,SAAA,QAAA;AACA,GAtGA;AAuGA,EAAA,OAvGA,qBAuGA,CAAA;AAvGA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h3>推广设置</h3>\r\n    <!-- 账号余额 -->\r\n    <div class=\"account\">\r\n      <div class=\"left\">\r\n        <h4>账户余额: <span> {{count}} </span>元</h4>\r\n        <span class=\"limit\">(每日限额: {{limit}}元)</span>\r\n        <el-button type=\"text\" @click=\"everyDay\">修改每日限额</el-button>\r\n      </div>\r\n      <div>\r\n        <el-button type=\"primary\" size=\"small\" @click=\"recharge\">账号充值</el-button>\r\n      </div>\r\n    </div>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item v-if=\"level === 2\" label=\"经销商名称\">\r\n          <el-input v-model=\"formInline.dealerName\" placeholder=\"经销商名称\" size=\"mini\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"合同类型\">\r\n          <el-select @change=\"ggg\" v-model=\"formInline.type\" placeholder=\"请选择合同类型\" size=\"mini\">\r\n            <el-option label=\"软件使用\" value=\"软件使用\"></el-option>\r\n            <el-option label=\"充值\" value=\"充值\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"合同创建时间\">\r\n          <div class=\"block\">\r\n            <el-date-picker size=\"mini\" v-model=\"time\" type=\"datetimerange\" :picker-options=\"pickerOptions\" range-separator=\"至\" start-placeholder=\"开始日期\" end-placeholder=\"结束日期\" align=\"right\"></el-date-picker>\r\n          </div>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{\r\n        'text-align': 'center',\r\n        'font-size': '14px',\r\n        color: '#000',\r\n      }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\"\r\n    >\r\n      <el-table-column v-if=\"level === 2\" prop=\"dealerName\" label=\"经销商名称\" ></el-table-column>\r\n      <el-table-column prop=\"type\" label=\"合同类型\" ></el-table-column>\r\n      <el-table-column prop=\"edition\" label=\"版本\" ></el-table-column>\r\n      <el-table-column prop=\"price\" label=\"价格\"></el-table-column>\r\n      <el-table-column prop=\"contacts\" label=\"联系人\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"联系电话\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"联系地址\"></el-table-column>\r\n      <el-table-column prop=\"startDate\" label=\"合同开始时间\"></el-table-column>\r\n      <el-table-column prop=\"endDate\" label=\"合同结束时间\"></el-table-column>\r\n      <el-table-column prop=\"payDate\" label=\"付款时间\"></el-table-column>\r\n      <!-- <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"mini\" type=\"primary\" @click=\"handleEdit(scope.row)\"\r\n            >编辑</el-button\r\n          >\r\n        </template>\r\n      </el-table-column> -->\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\"\r\n    >\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { pickerOptions, dateConversion } from '../../assets/js/timer'\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 账号余额\r\n      count: 0,\r\n      // 每日限额\r\n      limit: 0,\r\n      // 当前人等级\r\n      level: 0,\r\n      // 时间快捷键\r\n      pickerOptions: pickerOptions,\r\n      time: [],\r\n      // 搜索条件\r\n      formInline: {\r\n        createDateS:'',\r\n        createDateE:'',\r\n        type: '',\r\n        dealerName: '',\r\n        //token:'',\r\n        pageNo: 1,\r\n        pageSize: 20,\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 表格\r\n      tableData: [],\r\n    }\r\n  },\r\n  methods: {\r\n      // 获取账户余额和限额\r\n    async getCount () {\r\n      const { data: res } = await this.$http.post('/money/getOneConsume')\r\n      this.count = res.balance\r\n      this.limit = res.norm\r\n    },\r\n    // 修改每日限额\r\n    everyDay () {\r\n      this.$prompt('每日限额', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消'\r\n      }).catch(() => {\r\n        console.log('取消')\r\n      }).then(async ({ value }) => {\r\n        const res = await this.$http.post('/money/norm', {\r\n          norm: value\r\n        })\r\n        if (res.data !== true || res.status !== 200) return this.$message.error('修改失败')\r\n        this.$message.success('修改成功')\r\n        this.getCount()\r\n      })\r\n    },\r\n    // 账户充值\r\n    recharge () {\r\n      this.$router.push('/finance/recharge')\r\n    },\r\n    // 获取经销商列表\r\n    async getContractList() {\r\n      if (this.time === null || this.time.length===0 ) {\r\n        this.time = []\r\n      }else{\r\n        this.formInline.createDateS = dateConversion(this.time[0])\r\n        this.formInline.createDateE = dateConversion(this.time[1])\r\n      }      \r\n      //this.formInline.token = window.sessionStorage.getItem('token')\r\n      const { data: res } = await this.$http.post('/contract/showContract',this.formInline)\r\n      this.tableData = res.l\r\n      this.total = this.tableData.length\r\n    },\r\n    addContract() {\r\n      this.$router.push(`/contract/ContractDetail`)\r\n    },\r\n    // 获取当前登陆人的等级\r\n    async getLevel() {\r\n      const { data: res } = await this.$http.post('/user/level')\r\n      this.level = res\r\n    },\r\n    // // 查询\r\n    ggg (index) {\r\n      if (index !== undefined) {\r\n        this.formInline.pageNo = 1\r\n      }\r\n    },\r\n    submit () {\r\n      if (this.flag === false) {\r\n        this.formInline.pageNo = 1\r\n      }\r\n      this.ggg()\r\n      this.getContractList()\r\n    },\r\n    // 编辑\r\n    // 切换页数\r\n    changePage(index) {\r\n      this.formInline.pageNo = index\r\n      this.getContractList()\r\n    },\r\n  },\r\n  computed: {},\r\n  created() {\r\n    // 获取特卖列表\r\n    this.getContractList()\r\n    // 获取账户余额和限额\r\n    this.getCount()\r\n  },\r\n  mounted() {},\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 账户余额\r\n.account {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  border: 1px solid #ccc;\r\n  padding: 0 20px;\r\n  margin-top: 20px;\r\n  .left {\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    .limit {\r\n      margin: 20px;\r\n    }\r\n  }\r\n}\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.img {\r\n  width: 500px;\r\n}\r\n.admin {\r\n  ul {\r\n    overflow: hidden;\r\n  }\r\n  li {\r\n    float: left;\r\n    .el-checkbox {\r\n      margin-right: 7px;\r\n    }\r\n    span {\r\n      margin-right: 20px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/extension"}]}