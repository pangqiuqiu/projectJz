;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;;AAAA,oBAEA,GAFA;AAAA;AAAA;AAAA;;AAAA,kDAEA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AA1BA,GAbA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,SAAA,QAAA;AACA;AA3CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h3>优秀老师</h3>\r\n    <!-- 新增老师 -->\r\n    <el-button class=\"btn\" @click=\"add\" type=\"primary\" size=\"mini\">新增优秀老师</el-button>\r\n    <!-- 列表 -->\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\"\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\">\r\n      <el-table-column prop=\"name\" label=\"老师姓名\"></el-table-column>\r\n      <el-table-column prop=\"intro\" label=\"老师简介\"></el-table-column>\r\n      <el-table-column label=\"头像\">\r\n        <template slot-scope=\"scope\">\r\n          <img :src=\"scope.row.headPic\" alt=\"\" style=\"width: 130px\">\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" size=\"mini\" @click=\"edit(scope.row.id)\">编辑</el-button>\r\n          <el-popconfirm\r\n            title=\"您确定要删除吗?\"\r\n            cancel-button-type=\"primary\"\r\n            confirm-button-type=\"danger\"\r\n            @onConfirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" size=\"mini\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"formAlign.pageNo\"\r\n      :page-size=\"formAlign.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 参数\r\n      formAlign: {\r\n        pageNo: 1,\r\n        pageSize: 15\r\n      },\r\n      // 老师列表\r\n      tableData: [],\r\n      total: 0\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取老师列表\r\n    async getTable () {\r\n      const { data: res } = await this.$http.post('/us/showTeachers', this.formAlign)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.formAlign.pageNo = index\r\n      this.getTable()\r\n    },\r\n    // 新增老师\r\n    add () {\r\n      this.$router.push('/us/cooperation/teacherdetails')\r\n    },\r\n    // 编辑\r\n    edit (id) {\r\n      this.$router.push(`/us/cooperation/teacherdetails?id=${id}`)\r\n    },\r\n    // 删除\r\n    async del (id) {\r\n      const { data: res } = await this.$http.post('/us/delTeachers', { id })\r\n      if (!res) return this.$message.error('删除失败')\r\n      this.$message.success('删除成功')\r\n      this.getTable()\r\n    }\r\n  },\r\n  created () {\r\n    this.getTable()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 新增按钮\r\n.btn {\r\n  margin: 10px 0;\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/us/cooperation"}]}