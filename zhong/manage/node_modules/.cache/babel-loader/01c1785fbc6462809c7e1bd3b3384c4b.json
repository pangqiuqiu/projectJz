A,IAAA,CAAA,kBAAA,EAAA,MAAA,CAAA,UAAA,CAJA;;AAAA;AAAA;AAIA,gBAAA,GAJA,SAIA,IAJA;AAKA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,CAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAhCA;AAiCA;AACA,IAAA,UAlCA,sBAkCA,KAlCA,EAkCA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,MAAA;AACA,KArCA;AAsCA;AACA,IAAA,YAvCA,wBAuCA,GAvCA,EAuCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA;AACA,gBAAA,MAAA,CAAA,GAAA,GAAA,GAAA,CAAA,UAAA;AAFA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CAHA;;AAAA;AAAA;AAGA,gBAAA,GAHA,SAGA,IAHA;AAMA;AACA,gBAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAhDA;AAiDA;AACA,IAAA,SAlDA,uBAkDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAIA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GAJA;AAAA;AAAA;AAAA;;AAAA,kDAKA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CALA;;AAAA;AAOA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AA5DA,GApDA;AAkHA,EAAA,OAlHA,qBAkHA;AACA;AACA,SAAA,cAAA;AACA,GArHA;AAsHA,EAAA,OAtHA,qBAsHA,CACA;AAvHA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h2>评论列表</h2>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"用户\">\r\n          <el-input size=\"mini\" v-model=\"formInline.userId\" placeholder=\"输入评论人ID\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"回帖时间\">\r\n          <div class=\"block\">\r\n            <el-date-picker size=\"mini\" v-model=\"time\" type=\"datetimerange\" :picker-options=\"pickerOptions\" range-separator=\"至\" start-placeholder=\"开始日期\" end-placeholder=\"结束日期\" align=\"right\"></el-date-picker>\r\n          </div>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <template>\r\n      <el-table\r\n        :default-sort = \"{prop: 'issueDate', order: 'descending'}\"\r\n        :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\"\r\n        :cell-style=\"{ 'text-align': 'center' }\"\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\">\r\n        <el-table-column\r\n          prop=\"userId\"\r\n          label=\"评论人ID\"\r\n          width=\"150\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"invitation\"\r\n          label=\"帖子标题\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          sortable\r\n          prop=\"replyDate\"\r\n          label=\"回帖时间\"\r\n          width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\">审核</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </template>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n    <!-- 对话框 -->\r\n    <el-dialog title=\"评论审核\" :visible.sync=\"dialogFormVisible\" :info=\"msg\">\r\n      <el-form>\r\n        <el-form-item label=\"帖子标题\" :label-width=\"formLabelWidth\">\r\n          <el-input disabled v-model=\"this.msg\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"评论内容\" :label-width=\"formLabelWidth\">\r\n          <div v-html=\"this.commentObj.reply\"></div>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"warning\" @click=\"deleteBtn\">删除</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 时间快捷键\r\n      pickerOptions: {\r\n        shortcuts: [{\r\n          text: '最近一周',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 7)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近一个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 30)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近三个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 90)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }]\r\n      },\r\n      time: [],\r\n      // 搜索条件\r\n      formInline: {\r\n        replyDateS: '',\r\n        replyDateE: '',\r\n        userId: '',\r\n        invitId: '', // 从帖子页跳转过来\r\n        pageNo: 1,\r\n        pageSize: 20\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 表格\r\n      tableData: [],\r\n      // 对话框\r\n      dialogFormVisible: false,\r\n      commentObj: {},\r\n      msg: '',\r\n      formLabelWidth: '120px'\r\n    }\r\n  },\r\n  methods: {\r\n    // 事件处理函数\r\n    dateConversion (value) {\r\n      function checkTime (i) {\r\n        if (i < 10) {\r\n          i = '0' + i\r\n        }\r\n        return i\r\n      }\r\n      var d = new Date(value)\r\n      var date = d.getFullYear() + '-' + checkTime(d.getMonth() + 1) + '-' + checkTime(d.getDate()) + ' ' + checkTime(d.getHours()) + ':' + checkTime(d.getMinutes()) + ':' + checkTime(d.getSeconds())\r\n      return date\r\n    },\r\n    // 获取评论列表\r\n    async getCommentList () {\r\n      const id = this.$route.query.id\r\n      this.formInline.replyDateS = this.dateConversion(this.time[0])\r\n      this.formInline.replyDateE = this.dateConversion(this.time[1])\r\n      this.formInline.invitId = id\r\n      const { data: res } = await this.$http.post('/invit/showReply', this.formInline)\r\n      // console.log(res)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 查询\r\n    async submit () {\r\n      this.formInline.replyDateS = this.dateConversion(this.time[0])\r\n      this.formInline.replyDateE = this.dateConversion(this.time[1])\r\n      this.formInline.invitId = ''\r\n      const { data: res } = await this.$http.post('/invit/showReply', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.submit()\r\n    },\r\n    // 审核\r\n    async handleDelete (row) {\r\n      // console.log(row.id)\r\n      this.msg = row.invitation\r\n      const { data: res } = await this.$http.post('/invit/getReply', {\r\n        id: row.id\r\n      })\r\n      // console.log(res)\r\n      this.commentObj = res\r\n      this.dialogFormVisible = true\r\n    },\r\n    // 删除评论\r\n    async deleteBtn () {\r\n      const res = await this.$http.post('/invit/delReply', {\r\n        id: this.commentObj.id\r\n      })\r\n      if (res.data !== true || res.status !== 200) {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.dialogFormVisible = false\r\n      this.submit()\r\n    }\r\n  },\r\n  created () {\r\n    // 获取评论列表\r\n    this.getCommentList()\r\n  },\r\n  mounted () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n// 对话框\r\n.el-dialog {\r\n  div {\r\n    width: 92%;\r\n  }\r\n  .el-input {\r\n    width: 92%;\r\n  }\r\n  .dialog-footer {\r\n    display: flex;\r\n    justify-content: center;\r\n    .el-button {\r\n      width: 100px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/forum"}]}