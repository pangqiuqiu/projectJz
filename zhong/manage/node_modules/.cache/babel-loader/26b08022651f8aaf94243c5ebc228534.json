,GAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAbA;AAcA,IAAA,WAdA,yBAcA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,EAAA;AACA,KAhBA;AAiBA;AACA,IAAA,cAlBA,0BAkBA,QAlBA,EAkBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,sBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAKA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAzBA;AA0BA;AACA,IAAA,MA3BA,oBA2BA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA,WAAA,cAAA;AACA,KA9BA;AA+BA;AACA,IAAA,UAhCA,sBAgCA,GAhCA,EAgCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,GAAA,CAAA,IAAA,KAAA,CADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,IAAA,EAAA,CADA;AAEA,kBAAA,EAAA,EAAA,GAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAAA,sBAMA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IANA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AASA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAVA;AAAA;;AAAA;AAAA;AAAA,uBAYA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,IAAA,EAAA,CADA;AAEA,kBAAA,EAAA,EAAA,GAAA,CAAA;AAFA,iBAAA,CAZA;;AAAA;AAYA,gBAAA,IAZA;;AAAA,sBAgBA,IAAA,CAAA,MAAA,KAAA,GAAA,IAAA,IAAA,CAAA,IAAA,KAAA,IAhBA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAmBA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KAtDA;AAuDA,IAAA,YAvDA,wBAuDA,GAvDA,EAuDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAIA,oBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,MAAA;AACA,iBAHA,MAGA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA;AACA,oBAAA,OAAA,EAAA,MADA;AAEA,oBAAA,IAAA,EAAA;AAFA,mBAAA;AAIA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KApEA;AAqEA;AACA,IAAA,UAtEA,sBAsEA,KAtEA,EAsEA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,cAAA;AACA;AAzEA,GArBA;AAgGA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,cAFA,4BAEA;AACA,UAAA,KAAA,UAAA,CAAA,KAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAPA,GAhGA;AAyGA,EAAA,OAzGA,qBAyGA;AACA;AACA,SAAA,cAAA;AACA;AA5GA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h2>推广审核</h2>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"品牌\">\r\n          <el-select @change=\"changeBrand\" v-model=\"formInline.brand\" placeholder=\"请选择品牌\" @visible-change=\"dropDownBrand($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"item in brandAry\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"车系\">\r\n          <el-select :disabled=\"disabledSeries\" v-model=\"formInline.series\" placeholder=\"请选择车系\" @visible-change=\"dropDownSeries($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"(item,index) in seriesAry\" :key=\"index\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"id\"\r\n        label=\"编号\"\r\n        width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"specialSale.dealer\"\r\n        label=\"发布经销商\"\r\n        width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"specialSale.brand\"\r\n        label=\"品牌\"\r\n        width>\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"series\"\r\n        label=\"车系\"\r\n        width>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"250\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"success\"\r\n            @click=\"handleEdit(scope.row)\">审核</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 搜索条件\r\n      formInline: {\r\n        brand: '',\r\n        series: '',\r\n        token: window.sessionStorage.getItem('token'),\r\n        pageNo: 1,\r\n        pageSize: 10\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 品牌\r\n      brandAry: [],\r\n      // 车系\r\n      seriesAry: [],\r\n      // 表格\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取特卖列表\r\n    async getSpecialSale () {\r\n      const { data: res } = await this.$http.post('/sale/getTg', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 获取品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleBrand')\r\n        this.brandAry = res\r\n      }\r\n    },\r\n    changeBrand () {\r\n      this.formInline.series = ''\r\n    },\r\n    // 获取车系\r\n    async dropDownSeries (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleSeries', {\r\n          brand: this.formInline.brand\r\n        })\r\n        this.seriesAry = res\r\n      }\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.formInline.pageNo = 1\r\n      this.getSpecialSale()\r\n    },\r\n    // 表格操作按钮\r\n    async handleEdit (row) { // 上首页\r\n      if (row.home === 0) {\r\n        const res = await this.$http.post('/sale/updateHome', {\r\n          home: 1,\r\n          id: row.id\r\n        })\r\n        if (res.status !== 200 || res.data !== true) {\r\n          return\r\n        }\r\n        this.$message.success('上首页成功')\r\n        this.submit()\r\n      } else {\r\n        const res = await this.$http.post('/sale/updateHome', {\r\n          home: 0,\r\n          id: row.id\r\n        })\r\n        if (res.status !== 200 || res.data !== true) {\r\n          return\r\n        }\r\n        this.$message.success('下首页成功')\r\n        this.submit()\r\n      }\r\n    },\r\n    async handleDelete (row) { // 删除\r\n      const res = await this.$http.post('/sale/delTg', {\r\n        id: row.id\r\n      })\r\n      if (res.status === 200 && res.data === true) {\r\n        this.$message.success('删除成功')\r\n        this.submit()\r\n      } else {\r\n        this.$message({\r\n          message: '删除失败',\r\n          type: 'error'\r\n        })\r\n      }\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getSpecialSale()\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断车系下拉框\r\n    disabledSeries () {\r\n      if (this.formInline.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  created () {\r\n    // 获取特卖列表\r\n    this.getSpecialSale()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/specialSale"}]}