 label=\"类型\">\r\n          <el-select v-model=\"formInline.type\" placeholder=\"请选择类型\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"获取底价\" value=\"获取底价\"></el-option>\r\n            <el-option label=\"分期购车\" value=\"分期购车\"></el-option>\r\n            <el-option label=\"预约试驾\" value=\"预约试驾\"></el-option>\r\n            <el-option label=\"置地换车\" value=\"置地换车\"></el-option>\r\n            <el-option label=\"客户留言\" value=\"客户留言\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"时间\">\r\n          <div class=\"block\">\r\n            <el-date-picker size=\"mini\" v-model=\"time\" type=\"datetimerange\" :picker-options=\"pickerOptions\" range-separator=\"至\" start-placeholder=\"开始日期\" end-placeholder=\"结束日期\" align=\"right\"></el-date-picker>\r\n          </div>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"id\"\r\n        label=\"编号\"\r\n        width=\"70\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"type\"\r\n        label=\"类型\"\r\n        width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"xsDate\"\r\n        label=\"时间\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"name\"\r\n        label=\"用户\"\r\n        width=\"140\">\r\n      </el-table-column>\r\n       <el-table-column\r\n        prop=\"phone\"\r\n        label=\"手机号\"\r\n        width=\"130\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"series\"\r\n        label=\"车系\"\r\n        width=\"300\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        :show-overflow-tooltip='true'\r\n        prop=\"other\"\r\n        label=\"留言\">\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 搜索条件\r\n      formInline: {\r\n        token: window.sessionStorage.getItem('token'),\r\n        type: '',\r\n        xsDateS: '',\r\n        xsDateE: '',\r\n        pageNo: 1,\r\n        pageSize: 20\r\n      },\r\n      // 时间快捷键\r\n      pickerOptions: {\r\n        shortcuts: [{\r\n          text: '最近一周',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 7)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近一个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 30)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近三个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 90)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }]\r\n      },\r\n      time: [],\r\n      // 总数\r\n      total: 0,\r\n      // 表格\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 时间处理函数\r\n    dateConversion (value) {\r\n      function checkTime (i) {\r\n        if (i < 10) {\r\n          i = '0' + i\r\n        }\r\n        return i\r\n      }\r\n      var d = new Date(value)\r\n      var date = d.getFullYear() + '-' + checkTime(d.getMonth() + 1) + '-' + checkTime(d.getDate()) + ' ' + checkTime(d.getHours()) + ':' + checkTime(d.getMinutes()) + ':' + checkTime(d.getSeconds())\r\n      return date\r\n    },\r\n    // 获取用户留言列表\r\n    async getMseeage () {\r\n      this.formInline.xsDateS = this.dateConversion(this.time[0])\r\n      this.formInline.xsDateE = this.dateConversion(this.time[1])\r\n      const { data: res } = await this.$http.post('/dealer/showTab', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.formInline.pageNo = 1\r\n      this.getMseeage()\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getMseeage()\r\n    }\r\n  },\r\n  created () {\r\n    // 获取用户留言列表\r\n    this.getMseeage()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/distributor"}]}