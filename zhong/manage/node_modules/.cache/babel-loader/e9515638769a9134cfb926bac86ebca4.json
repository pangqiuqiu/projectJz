AAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,gBAAA;AACA,KAjBA;AAkBA,IAAA,GAlBA,eAkBA,KAlBA,EAkBA;AACA,UAAA,KAAA,KAAA,SAAA,EAAA;AACA,aAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA;AACA,KAtBA;AAuBA;AACA,IAAA,QAxBA,sBAwBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;AAEA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KA3BA;AA4BA,IAAA,YA5BA,0BA4BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,oBAAA,GAAA,IAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,KA9BA;AA+BA,IAAA,WA/BA,uBA+BA,GA/BA,EA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,mBAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAlCA;AAmCA,IAAA,aAnCA,2BAmCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA;AACA,kBAAA,OAAA,EAAA,MAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;;AAIA,oBAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,oBAAA,GAAA,KAAA;AACA,kBAAA,MAAA,CAAA,UAAA,CAAA,MAAA,GAAA,CAAA;;AACA,kBAAA,MAAA,CAAA,gBAAA;AACA,iBALA,MAKA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KA/CA;AAgDA,IAAA,YAhDA,0BAgDA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,EADA;AAEA,kBAAA,MAAA,EAAA,MAAA,CAAA;AAFA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;;AAKA,oBAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,mBAAA,GAAA,KAAA;AACA,kBAAA,MAAA,CAAA,UAAA,CAAA,MAAA,GAAA,CAAA;;AACA,kBAAA,MAAA,CAAA,gBAAA;AACA,iBALA,MAKA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA;AA7DA,GAvBA;AAsFA,EAAA,QAAA,EAAA,EAtFA;AAuFA,EAAA,OAvFA,qBAuFA;AACA;AACA,SAAA,gBAAA,GAFA,CAGA;;AACA,SAAA,QAAA;AACA,GA5FA;AA6FA,EAAA,OA7FA,qBA6FA,CAAA;AA7FA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h2>问题反馈列表</h2>\r\n    <!-- 搜索区域 -->\r\n     <el-card v-if=\"level===2\" body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"问题状态\">\r\n          <el-select @change=\"ggg\" v-model=\"formInline.isAnswer\" placeholder=\"请选择问题状态\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"未答复\" value=\"0\"></el-option>\r\n            <el-option label=\"已答复\" value=\"1\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"经销商\">\r\n           <el-input @change=\"ggg\" v-model=\"formInline.dealerName\" placeholder=\"店铺名称\" size=\"mini\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <el-button v-if=\"level===5\" size=\"mini\" type=\"primary\" @click=\"creatProblem\" style=\"margin-top: 15px;margin-bottom: 15px;\">新建问题</el-button>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{'text-align': 'center', 'font-size': '14px', color: '#000', }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\"\r\n    >\r\n      <el-table-column prop=\"id\" label=\"编号\"></el-table-column>\r\n      <el-table-column v-if=\"level===2\" prop=\"dealerName\" label=\"店铺名称\"></el-table-column>\r\n      <el-table-column prop=\"problem\" label=\"问题\" ></el-table-column>\r\n      <el-table-column prop=\"answer\" label=\"答复\" ></el-table-column>\r\n      <el-table-column prop=\"problemDate\" label=\"提出时间\" ></el-table-column>\r\n      <el-table-column prop=\"answerDate\" label=\"答复时间\" ></el-table-column>\r\n      <el-table-column v-if=\"level===2\" label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button  size=\"mini\" type=\"primary\" @click=\"creatAnswer(scope.row)\">答复</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\"\r\n    >\r\n    </el-pagination>\r\n    <el-dialog title=\"问题反馈\" :visible.sync=\"dialogProblemVisible\">\r\n        <el-input v-model=\"txtproblem\" style=\"width: 100%\" placeholder=\"请填写问题\"></el-input>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"dialogProblemVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"submitProblem()\">确 定</el-button>\r\n        </div>\r\n    </el-dialog>\r\n    <el-dialog title=\"问题答复\" :visible.sync=\"dialogAnswerVisible\">\r\n        <el-input v-model=\"txtanswer\" style=\"width: 100%\"></el-input>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"dialogAnswerVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"submitAnswer()\">确 定</el-button>\r\n        </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { pickerOptions, dateConversion } from '../../assets/js/timer'\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 当前人等级\r\n      level: 0,\r\n      // 搜索条件\r\n      formInline: {\r\n        isAnswer:'',\r\n        dealerName:'',\r\n        pageNo: 1,\r\n        pageSize: 20,\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 表格\r\n      tableData: [],\r\n      dialogProblemVisible: false,\r\n      dialogAnswerVisible: false,\r\n      txtproblem:'',\r\n      txtanswer:'',\r\n      id:''\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取经销商列表\r\n    async getExtensionList() { \r\n      const { data: res } = await this.$http.post('/extension/problemList',this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    submit () {\r\n\r\n      this.formInline.pageNo = 1\r\n\r\n      this.getExtensionList()\r\n    },\r\n    // 切换页数\r\n    changePage(index) {\r\n      this.formInline.pageNo = index\r\n      this.getExtensionList()\r\n    },\r\n    ggg (index) {\r\n      if (index !== undefined) {\r\n        this.formInline.pageNo = 1\r\n      }\r\n    },\r\n    // 获取当前登陆人的等级\r\n    async getLevel() {\r\n      const { data: res } = await this.$http.post('/user/level')\r\n      this.level = res\r\n    },\r\n    async creatProblem () {\r\n        this.dialogProblemVisible = true;\r\n    },\r\n    async creatAnswer (row) {\r\n        this.dialogAnswerVisible = true;\r\n        this.id = row.id;\r\n    },\r\n    async submitProblem() {\r\n        const { data: res } = await this.$http.post('/extension/problem', {\r\n            problem:this.txtproblem\r\n        })\r\n        if(res){\r\n          this.$message.success('提交成功')\r\n          this.dialogProblemVisible = false;\r\n          this.formInline.pageNo = 1\r\n          this.getExtensionList()\r\n        }else{\r\n          this.$message.success('提交失败')\r\n        }\r\n    },\r\n    async submitAnswer() {\r\n        const { data: res } = await this.$http.post('/extension/answer', {\r\n            id: this.id,\r\n            answer:this.txtanswer\r\n        })\r\n        if(res){\r\n          this.$message.success('提交成功')\r\n          this.dialogAnswerVisible = false;\r\n          this.formInline.pageNo = 1\r\n          this.getExtensionList()\r\n        }else{\r\n          this.$message.success('提交失败')\r\n        }\r\n    },\r\n  },\r\n  computed: {},\r\n  created() {\r\n    // 获取特卖列表\r\n    this.getExtensionList()\r\n    // 等级\r\n    this.getLevel()\r\n  },\r\n  mounted() {},\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.img {\r\n  width: 500px;\r\n}\r\n.admin {\r\n  ul {\r\n    overflow: hidden;\r\n  }\r\n  li {\r\n    float: left;\r\n    .el-checkbox {\r\n      margin-right: 7px;\r\n    }\r\n    span {\r\n      margin-right: 20px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/specialExtension"}]}