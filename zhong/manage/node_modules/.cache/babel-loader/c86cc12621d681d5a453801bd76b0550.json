AAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,KAAA,CAAA,UAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,QACA,IADA;AAEA,gBAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,CAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KANA;AAOA;AACA,IAAA,gBARA,4BAQA,QARA,EAQA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAGA,gBAAA,MAAA,CAAA,WAAA,GAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAbA;AAcA,IAAA,OAdA,qBAcA;AACA,WAAA,UAAA,CAAA,IAAA,GAAA,EAAA;AACA,KAhBA;AAiBA;AACA,IAAA,YAlBA,wBAkBA,QAlBA,EAkBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAKA,gBAAA,MAAA,CAAA,OAAA,GAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAzBA;AA0BA;AACA,IAAA,MA3BA,oBA2BA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA,WAAA,kBAAA;AACA,KA9BA;AA+BA;AACA,IAAA,UAhCA,sBAgCA,EAhCA,EAgCA;AAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,mBADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAFA,OAAA;AAIA,KArCA;AAsCA,IAAA,YAtCA,wBAsCA,GAtCA,EAsCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAIA,oBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,kBAAA,MAAA,CAAA,MAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA;AACA,oBAAA,OAAA,EAAA,MADA;AAEA,oBAAA,IAAA,EAAA;AAFA,mBAAA;AAIA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAlDA;AAmDA,IAAA,WAnDA,uBAmDA,GAnDA,EAmDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,GAAA,CAAA,MAAA,KAAA,GADA;AAAA;AAAA;AAAA;;AAAA,kDAEA,MAAA,CAAA,QAAA,CAAA;AACA,kBAAA,OAAA,EAAA,UADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA,CAFA;;AAAA;AAAA;AAAA,uBAOA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CAPA;;AAAA;AAOA,gBAAA,GAPA;;AAUA,oBAAA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,kBAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA;AACA,oBAAA,OAAA,EAAA,MADA;AAEA,oBAAA,IAAA,EAAA;AAFA,mBAAA;AAIA;;AAjBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KArEA;AAsEA;AACA,IAAA,UAvEA,sBAuEA,KAvEA,EAuEA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,kBAAA;AACA,KA1EA;AA2EA;AACA,IAAA,oBA5EA,kCA4EA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,WAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,OAAA;AADA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAKA,oBAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,kBAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KArFA;AAsFA;AACA,IAAA,kBAvFA,gCAuFA;AACA,WAAA,iBAAA,GAAA,KAAA,CADA,CAEA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,oBADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,IAAA,EAAA,KAAA;AAAA;AAFA,OAAA;AAIA;AA9FA,GA/BA;AA+HA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,YAFA,0BAEA;AACA,UAAA,KAAA,UAAA,CAAA,UAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAPA,GA/HA;AAwIA,EAAA,OAxIA,qBAwIA;AACA;AACA,SAAA,kBAAA;AACA;AA3IA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h3>资讯列表</h3>\r\n    <!-- 新建资讯 -->\r\n    <el-button type=\"primary\" class=\"addInformation\" @click=\"dialogFormVisibleing\">新建资讯</el-button>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select @change=\"changeP\" @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\">\r\n            <el-option :label=\"item.province\" :value=\"item.province\" v-for=\"item in provinceAry\" :key=\"item.provinceid\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"城市\">\r\n          <el-select :disabled=\"disabledCity\" @visible-change=\"dropDownCity($event)\" v-model=\"formInline.area\" placeholder=\"请选择城市\" size=\"mini\">\r\n            <el-option :label=\"item.city\" :value=\"item.city\" v-for=\"item in cityAry\" :key=\"item.cityid\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"资讯类型\">\r\n          <el-select v-model=\"formInline.type\" placeholder=\"选择资讯类型\" size=\"mini\">\r\n            <el-option label=\"车讯\" value=\"车讯\"></el-option>\r\n            <el-option label=\"试驾评测\" value=\"试驾评测\"></el-option>\r\n            <el-option label=\"技术\" value=\"技术\"></el-option>\r\n            <el-option label=\"导购\" value=\"导购\"></el-option>\r\n            <el-option label=\"行业文化\" value=\"行业文化\"></el-option>\r\n            <el-option label=\"店铺活动\" value=\"店铺活动\"></el-option>\r\n            <el-option label=\"团购活动\" value=\"团购活动\"></el-option>\r\n            <el-option label=\"品牌新闻\" value=\"品牌新闻\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\">\r\n          <el-select v-model=\"formInline.state\" placeholder=\"选择状态\" size=\"mini\">\r\n            <el-option label=\"待审核\" value=\"待审核\"></el-option>\r\n            <el-option label=\"已上线\" value=\"已上线\"></el-option>\r\n            <el-option label=\"未通过\" value=\"未通过\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"文章标题\">\r\n          <el-input style=\"width: 300px;\" v-model=\"formInline.title\" size=\"mini\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <template>\r\n      <el-table\r\n        :default-sort = \"{prop: 'issueDate', order: 'descending'}\"\r\n        :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n        :cell-style=\"{ 'text-align': 'center' }\"\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\">\r\n        <el-table-column\r\n          prop=\"id\"\r\n          label=\"编号\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"title\"\r\n          label=\"标题\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"type\"\r\n          label=\"类型\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"state\"\r\n          label=\"状态\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"申请上首页\"\r\n          width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-show=\"scope.row.ishome === '1'\">已申请</span>\r\n            <span v-show=\"scope.row.ishome === '0'\">未申请</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"通过上首页\"\r\n          prop=\"home\"\r\n          width=\"100\">\r\n          <!-- <template slot-scope=\"scope\">\r\n            <span v-show=\"scope.row.home === '1'\">是</span>\r\n            <span v-show=\"scope.row.home === '0'\">否</span>\r\n          </template> -->\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"260\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleEdit(scope.row.id)\">编辑</el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\">删除</el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"success\"\r\n              @click=\"handleApply(scope.row)\">申请上首页</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </template>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n    <!-- 选择资讯类型 -->\r\n    <el-dialog title=\"请选择新增资讯类型\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form>\r\n        <el-radio-group v-model=\"radio\">\r\n          <el-radio label=\"车讯\"></el-radio>\r\n          <el-radio label=\"试驾评测\"></el-radio>\r\n          <el-radio label=\"技术\"></el-radio>\r\n          <el-radio label=\"导购\"></el-radio>\r\n          <el-radio label=\"行业文化\"></el-radio>\r\n          <el-radio label=\"店铺活动\" v-show=\"isInfor\"></el-radio>\r\n          <el-radio label=\"团购活动\" v-show=\"isInfor\"></el-radio>\r\n          <el-radio label=\"品牌新闻\" v-show=\"isInfor\"></el-radio>\r\n        </el-radio-group>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogFormVisibled\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 判断是否是经销商\r\n      isInfor: false, // 假设不是经销商\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '',\r\n        area: '',\r\n        state: '',\r\n        title: '',\r\n        type: '',\r\n        pageNo: 1,\r\n        pageSize: 20\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 省份\r\n      provinceAry: [],\r\n      // 城市\r\n      cityAry: [],\r\n      // 品牌\r\n      brandList: [],\r\n      // 表格\r\n      tableData: [],\r\n      // 弹出框\r\n      dialogFormVisible: false,\r\n      // 新增资讯类型\r\n      radio: '汽车资讯'\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取资讯列表\r\n    async getInformationList () {\r\n      const { data: res } = await this.$http.post('/Infor/inforList', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 获取省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getProvince')\r\n        this.provinceAry = res\r\n      }\r\n    },\r\n    changeP () {\r\n      this.formInline.area = ''\r\n    },\r\n    // 获取城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getCityByPid', {\r\n          province: this.formInline.province\r\n        })\r\n        this.cityAry = res\r\n      }\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.formInline.pageNo = 1\r\n      this.getInformationList()\r\n    },\r\n    // 表格操作按钮\r\n    handleEdit (id) { // 编辑\r\n      this.$router.push({\r\n        path: '/information/edit',\r\n        query: { id: id }\r\n      })\r\n    },\r\n    async handleDelete (row) { // 删除\r\n      const res = await this.$http.post('/Infor/delInfor', {\r\n        id: row.id\r\n      })\r\n      if (res.status === 200 && res.data === true) {\r\n        this.submit()\r\n      } else {\r\n        this.$message({\r\n          message: '删除失败',\r\n          type: 'error'\r\n        })\r\n      }\r\n    },\r\n    async handleApply (row) { // 申请上首页\r\n      if (row.ishome === '1') {\r\n        return this.$message({\r\n          message: '您已经申请过了哦',\r\n          type: 'warning'\r\n        })\r\n      }\r\n      const res = await this.$http.post('/Infor/ishome', {\r\n        id: row.id\r\n      })\r\n      if (res.data === true && res.status === 200) {\r\n        this.getInformationList()\r\n      } else {\r\n        this.$message({\r\n          message: '申请失败',\r\n          type: 'error'\r\n        })\r\n      }\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getInformationList()\r\n    },\r\n    // 新建资讯 显示弹框\r\n    async dialogFormVisibleing () {\r\n      // 判断是否是经销商\r\n      const res = await this.$http.post('/Infor/dj', {\r\n        token: window.sessionStorage.getItem('token')\r\n      })\r\n      if (res.data === true) {\r\n        this.isInfor = true\r\n      }\r\n      this.dialogFormVisible = true\r\n    },\r\n    // 新建资讯 确定按钮\r\n    dialogFormVisibled () {\r\n      this.dialogFormVisible = false\r\n      // console.log(this.radio)\r\n      this.$router.push({\r\n        path: '/information/newly',\r\n        query: { type: this.radio }\r\n      })\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断城市下拉框\r\n    disabledCity () {\r\n      if (this.formInline.provinceId !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  created () {\r\n    // 获取资讯列表\r\n    this.getInformationList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh3 {\r\n  margin-bottom: 20px;\r\n}\r\n// 新建资讯\r\n.addInformation {\r\n  margin-bottom: 20px;\r\n  border-radius: 10px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n.el-radio {\r\n  margin-bottom: 20px;\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/information"}]}