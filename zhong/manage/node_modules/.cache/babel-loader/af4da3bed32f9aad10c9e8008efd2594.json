OAhDA,qBAgDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,IAAA,CAAA,KAAA,KAAA,SADA;AAAA;AAAA;AAAA;;AAAA,kDAEA,KAAA,CAAA,OAAA,CAFA;;AAAA;AAAA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,gBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,GADA;AAEA,kBAAA,KAAA,EAAA,MAAA,CAAA,IAAA,CAAA,KAFA;AAGA,kBAAA,OAAA,EAAA;AAHA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,GAJA;;AAAA,sBASA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GATA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAYA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KA/DA;AAgEA;AACA,IAAA,OAjEA,qBAiEA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,uBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,GADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAKA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GALA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA5EA;AA6EA;AACA,IAAA,UA9EA,sBA8EA,KA9EA,EA8EA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,cAAA;AACA;AAjFA,GA5CA;AA+HA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,YAFA,0BAEA;AACA,UAAA,KAAA,UAAA,CAAA,QAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAPA,GA/HA;AAwIA,EAAA,OAxIA,qBAwIA;AACA;AACA,SAAA,cAAA;AACA,GA3IA;AA4IA,EAAA,OA5IA,qBA4IA,CACA;AA7IA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h2>经销商入驻审核</h2>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\" @change=\"changeP\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.province\" :value=\"item.province\" v-for=\"item in provinceAry\" :key=\"item.province\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"城市\">\r\n          <el-select :disabled=\"disabledCity\" @visible-change=\"dropDownCity($event)\" v-model=\"formInline.city\" placeholder=\"请选择城市\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.city\" :value=\"item.city\" v-for=\"item in cityAry\" :key=\"item.cityId\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"品牌\">\r\n          <el-select v-model=\"formInline.brand\" placeholder=\"请选择品牌\" @visible-change=\"dropDownBrand($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"item in brandAry\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"4S店\">\r\n          <el-input v-model=\"formInline.name4s\" placeholder=\"店铺名称\" size=\"mini\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"id\"\r\n        label=\"编号\"\r\n        width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"city\"\r\n        label=\"城市\"\r\n        width=\"130\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"brand\"\r\n        label=\"品牌\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"name4s\"\r\n        label=\"店铺名称\"\r\n        >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"state4s\"\r\n        label=\"状态\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleEdit(scope.row)\">审核</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 对话框 -->\r\n    <el-dialog :info='msg' title=\"经销商信息\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form :model=\"form\">\r\n        <el-form-item label=\"公司名称\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.company\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"公司地址\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.companyAddress\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"门店地址\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"经销商认证\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.dealerRz\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"统一社会信用代码\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.tyshxydm\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"厂商门店编码\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.cjmdbm\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"营业执照副本\" :label-width=\"formLabelWidth\">\r\n          <img :src=\"form.yyzz\" alt=\"\">\r\n        </el-form-item>\r\n        <el-form-item label=\"厂商品牌授权书\" :label-width=\"formLabelWidth\">\r\n          <img :src=\"form.csppsqs\" alt=\"\">\r\n        </el-form-item>\r\n        <el-form-item label=\"店门口照片\" :label-width=\"formLabelWidth\">\r\n          <img :src=\"form.dmkzp\" alt=\"\">\r\n        </el-form-item>\r\n        <el-form-item label=\"所属品牌\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.brand\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"联系人\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.link\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机号\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"\" :label-width=\"formLabelWidth\">\r\n          <textarea v-model=\"form.cause\" autocomplete=\"off\" placeholder=\"请输入拒绝原因\"></textarea>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogB\">不通过</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogT\">通过</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 对话框\r\n      dialogFormVisible: false,\r\n      msg: '',\r\n      formLabelWidth: '150px',\r\n      form: {\r\n        company: '',\r\n        companyAddress: '',\r\n        address: '',\r\n        dealerRz: '',\r\n        tyshxydm: '',\r\n        cjmdbm: '',\r\n        brand: '',\r\n        link: '',\r\n        phone: '',\r\n        id: '',\r\n        cause: '',\r\n        token: window.sessionStorage.getItem('token')\r\n      },\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '',\r\n        city: '',\r\n        brand: '',\r\n        name4s: '',\r\n        token: window.sessionStorage.getItem('token'),\r\n        state4s: '未审核',\r\n        pageNo: 1,\r\n        pageSize: 10\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 省份\r\n      provinceAry: [],\r\n      // 城市\r\n      cityAry: [],\r\n      // 品牌\r\n      brandAry: [],\r\n      // 表格\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取经销商列表\r\n    async getSpecialSale () {\r\n      const { data: res } = await this.$http.post('/dealer/DealerList', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 获取省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getProvince')\r\n        this.provinceAry = res\r\n      }\r\n    },\r\n    changeP () {\r\n      this.formInline.city = ''\r\n    },\r\n    // 获取城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getCityByPid', {\r\n          province: this.formInline.province\r\n        })\r\n        this.cityAry = res\r\n      }\r\n    },\r\n    // 获取品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleBrand')\r\n        this.brandAry = res\r\n      }\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.formInline.pageNo = 1\r\n      this.getSpecialSale()\r\n    },\r\n    // 编辑\r\n    async handleEdit (row) {\r\n      this.msg = row.id\r\n      const { data: res } = await this.$http.post('/dealer/getDealer', {\r\n        id: row.id\r\n      })\r\n      this.form = res\r\n      this.dialogFormVisible = true\r\n    },\r\n    // 不通过\r\n    async dialogB () {\r\n      if (this.form.cause === undefined) {\r\n        return alert('请输入原因')\r\n      }\r\n      const res = await this.$http.post('/dealer/addBtg', {\r\n        id: this.msg,\r\n        cause: this.form.cause,\r\n        state4s: '不通过'\r\n      })\r\n      if (res.data !== true || res.status !== 200) {\r\n        return\r\n      }\r\n      this.$message.success('审核成功')\r\n      this.dialogFormVisible = false\r\n      this.submit()\r\n    },\r\n    // 通过\r\n    async dialogT () {\r\n      const res = await this.$http.post('/dealer/updateState4s', {\r\n        id: this.msg,\r\n        state4s: '已上线'\r\n      })\r\n      if (res.data !== true || res.status !== 200) {\r\n        return\r\n      }\r\n      this.$message.success('审核成功')\r\n      this.dialogFormVisible = false\r\n      this.submit()\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getSpecialSale()\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断城市下拉框\r\n    disabledCity () {\r\n      if (this.formInline.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  created () {\r\n    // 获取特卖列表\r\n    this.getSpecialSale()\r\n  },\r\n  mounted () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\ntextarea {\r\n  width: 100%;\r\n  height: 300px;\r\n  resize: none;\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/distributor"}]}