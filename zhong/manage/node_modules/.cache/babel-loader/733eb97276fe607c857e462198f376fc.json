A;AAAA;AAAA;AAAA;AAAA;AAIA,KANA;AAOA;AACA,IAAA,QARA,sBAQA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;AAEA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAXA;AAYA;AACA,IAAA,QAbA,oBAaA,QAbA,EAaA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAKA,gBAAA,MAAA,CAAA,UAAA,GAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KApBA;AAqBA,IAAA,WArBA,yBAqBA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,EAAA;AACA,KAvBA;AAwBA;AACA,IAAA,GAzBA,iBAyBA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KA3BA;AA4BA,IAAA,YA5BA,0BA4BA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,UADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,KAAA;AAAA;AAFA,OAAA;AAIA,KAlCA;AAmCA;AACA,IAAA,QApCA,sBAoCA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA,WAAA,QAAA;AACA,KAvCA;AAwCA;AACA,IAAA,UAzCA,sBAyCA,KAzCA,EAyCA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,QAAA;AACA,KA5CA;AA6CA;AACA,IAAA,UA9CA,sBA8CA,GA9CA,EA8CA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,WAAA,EAAA;AACA,kBAAA,MAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;AAIA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA;AACA,gBAAA,MAAA,CAAA,kBAAA,GAAA,IAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KApDA;AAqDA;AACA,IAAA,UAtDA,sBAsDA,GAtDA,EAsDA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,gBADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,OAAA;AAIA;AA3DA,GArBA;AAkFA,EAAA,OAlFA,qBAkFA;AACA;AACA,SAAA,QAAA,GAFA,CAGA;;AACA,SAAA,QAAA;AACA;AAvFA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h3>车辆列表</h3>\r\n    <!-- 新增 -->\r\n    <el-button size=\"small\" @click=\"add\" type=\"primary\" style=\"margin-top: 15px\">新增车辆</el-button>\r\n    <!-- 新增对话框 -->\r\n    <el-dialog title=\"新增车辆\" :visible.sync=\"addCar\">\r\n      <el-radio-group v-model=\"radio\">\r\n        <el-radio :label=\"1\">新增品牌</el-radio>\r\n        <el-radio :label=\"2\">新增车系</el-radio>\r\n        <el-radio :label=\"3\">新增车型</el-radio>\r\n      </el-radio-group>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addCar = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addCarSubmit\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 搜索条件 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\">\r\n        <el-form-item label=\"品牌\">\r\n          <el-select filterable  v-model=\"formInline.brand\" placeholder=\"请选择品牌\" size=\"mini\" @change=\"changeBrand\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in brandList\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"车系\">\r\n          <el-select filterable @visible-change=\"dropDown($event)\" v-model=\"formInline.series\" placeholder=\"请选择车系\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in seriesList\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" size=\"mini\" @click=\"onSubmit\">搜索</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 表格 -->\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\" :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\" :cell-style=\"{ 'text-align': 'center' }\">\r\n      <el-table-column\r\n        prop=\"brand\"\r\n        label=\"品牌\"\r\n        width=\"400\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"series\"\r\n        label=\"车系\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleEdit(scope.row)\">车型</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 查看车型 -->\r\n    <el-dialog width=\"60%\" title=\"车型展示\" :visible.sync=\"dialogTableVisible\">\r\n      <el-table :data=\"typeList\" border :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\" :cell-style=\"{ 'text-align': 'center' }\">\r\n        <el-table-column property=\"c\" label=\"车型\"></el-table-column>\r\n        <el-table-column label=\"操作\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleType(scope.row)\">参数配置</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 新增对话框\r\n      radio: 3,\r\n      addCar: false,\r\n      brandList: [],\r\n      seriesList: [],\r\n      tableData: [],\r\n      formInline: {\r\n        brand: '',\r\n        series: '',\r\n        pageSize: 20,\r\n        pageNo: 1\r\n      },\r\n      total: 0,\r\n      // 车型展示弹框\r\n      dialogTableVisible: false,\r\n      typeList: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取表格信息\r\n    async getTable () {\r\n      const { data: res } = await this.$http.post('/car/showBrandSeries', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 品牌\r\n    async getBrand () {\r\n      const { data: res } = await this.$http.post('/car/showAllBrand')\r\n      this.brandList = res\r\n    },\r\n    // 车系\r\n    async dropDown (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/car/showSeries', {\r\n          brand: this.formInline.brand\r\n        })\r\n        this.seriesList = res\r\n      }\r\n    },\r\n    changeBrand () {\r\n      this.formInline.series = ''\r\n    },\r\n    // 新增车辆\r\n    add () {\r\n      this.addCar = true\r\n    },\r\n    addCarSubmit () {\r\n      this.addCar = false\r\n      this.$router.push({\r\n        path: '/car/add',\r\n        query: { id: this.radio }\r\n      })\r\n    },\r\n    // 查询\r\n    onSubmit () {\r\n      this.formInline.pageNo = 1\r\n      this.getTable()\r\n    },\r\n    // 分页\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getTable()\r\n    },\r\n    // 车型展示\r\n    async handleEdit (row) {\r\n      const { data: res } = await this.$http.post('/car/getC', {\r\n        series: row.series\r\n      })\r\n      this.typeList = res\r\n      this.dialogTableVisible = true\r\n    },\r\n    // 跳转参数配置页面\r\n    handleType (row) {\r\n      this.$router.push({\r\n        path: '/car/parameter',\r\n        query: { id: row.c }\r\n      })\r\n    }\r\n  },\r\n  created () {\r\n    // 获取表格信息\r\n    this.getTable()\r\n    // 获取品牌数据\r\n    this.getBrand()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-card {\r\n  margin: 15px 0;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/car"}]}