,CAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAbA;AAcA;AACA,IAAA,IAfA,kBAeA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AAjBA,GAhBA;AAmCA,EAAA,OAnCA,qBAmCA;AACA;AACA,SAAA,kBAAA;AACA,GAtCA;AAuCA,EAAA,OAvCA,qBAuCA;AAAA;;AACA,SAAA,MAAA,GAAA,IAAA,CAAA,CAAA,KAAA,KAAA,CAAA,UAAA,CAAA,CADA,CAEA;;AACA,SAAA,MAAA,CAAA,YAAA,CAAA,mBAAA,GAAA,IAAA,CAHA,CAIA;;AACA,SAAA,MAAA,CAAA,YAAA,CAAA,QAAA,GAAA,UAAA,IAAA,EAAA;AACA,MAAA,MAAA,CAAA,aAAA,GAAA,IAAA,CADA,CAEA;AACA,KAHA;;AAIA,SAAA,MAAA,CAAA,YAAA,CAAA,KAAA,GAAA,CACA;AACA,UAFA,EAEA;AACA,UAHA,EAGA;AACA,cAJA,EAIA;AACA,cALA,EAKA;AACA,YANA,EAMA;AACA,eAPA,EAOA;AACA,mBARA,EAQA;AACA,eATA,EASA;AACA,eAVA,EAUA;AACA,UAXA,EAWA;AACA,UAZA,EAYA;AACA,aAbA,EAaA;AACA,WAdA,EAcA;AACA;AACA,WAhBA,EAgBA;AACA,WAjBA,EAiBA;AACA,UAlBA,EAkBA;AACA,UAnBA,EAmBA;AACA,UApBA,CAoBA;AApBA,KAAA;;AAsBA,SAAA,MAAA,CAAA,YAAA,CAAA,SAAA,GAAA,UAAA,IAAA,EAAA,IAAA,EAAA;AACA,UAAA,IAAA,KAAA,EAAA,EAAA;AACA,YAAA,GAAA,GAAA,uFAAA;;AACA,YAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,iBAAA,+BAAA;AACA,SAFA,MAEA;AACA,iBAAA,IAAA;AACA;AACA;AACA,KATA;;AAUA,SAAA,MAAA,CAAA,MAAA,GAzCA,CAyCA;AACA;AAjFA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 头部标题 -->\r\n    <div class=\"HeadTitle\">\r\n      <h3>{{this.title}}</h3>\r\n      <el-button type=\"text\" @click=\"back\">《《 返回</el-button>\r\n    </div>\r\n    <!-- 新增资讯表单 -->\r\n    <el-form label-position=\"right\" label-width=\"80px\" :model=\"form\" ref=\"rulesForm\">\r\n      <el-form-item label=\"标题\">\r\n        <el-input v-model=\"form.title\" placeholder=\"请输入标题，不超过25个汉字\" disabled></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"摘要\">\r\n        <el-input style=\"margin-bottom: 10px;\" disabled placeholder=\"不超过120字\" type=\"textarea\" :rows=\"2\" v-model=\"form.abstract\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"作者\">\r\n        <el-input v-model=\"form.author\" disabled placeholder=\"请输入作者名称\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"封面图\">\r\n        <img :src=\"this.pic\" alt=\"\">\r\n      </el-form-item>\r\n      <!-- 富文本 -->\r\n      <div class=\"edit_container\">\r\n        <div ref=\"editorElem\" style=\"text-align: left; width: 1000px;\"></div>\r\n      </div>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 资讯标题\r\n      title: '',\r\n      // 表单数据\r\n      form: {\r\n        title: '', // 标题\r\n        abstract: '', // 摘要\r\n        author: '', // 作者\r\n        content: '' // 正文\r\n      },\r\n      // 图片\r\n      pic: ''\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取资讯\r\n    async getInformationType () {\r\n      const id = this.$route.query.id\r\n      const { data: res } = await this.$http.post('/Infor/getOneInfor', {\r\n        id: id\r\n      })\r\n      this.pic = res.coverPic\r\n      this.title = res.type\r\n      this.form.title = res.title\r\n      this.form.abstract = res.intro\r\n      this.form.author = res.username\r\n      this.form.content = this.editor.txt.html(res.text)\r\n    },\r\n    // 返回上一页面\r\n    back () {\r\n      this.$router.go(-1)\r\n    }\r\n  },\r\n  created () {\r\n    // 获取资讯\r\n    this.getInformationType()\r\n  },\r\n  mounted () {\r\n    this.editor = new E(this.$refs.editorElem)\r\n    // 上传本地base64格式图片\r\n    this.editor.customConfig.uploadImgShowBase64 = true\r\n    // 编辑器的事件，每次改变会获取其html内容\r\n    this.editor.customConfig.onchange = html => {\r\n      this.editorContent = html\r\n      // this.catchData(this.editorContent) // 把这个html通过catchData的方法传入父组件\r\n    }\r\n    this.editor.customConfig.menus = [\r\n      // 菜单配置\r\n      'head', // 标题\r\n      'bold', // 粗体\r\n      'fontSize', // 字号\r\n      'fontName', // 字体\r\n      'italic', // 斜体\r\n      'underline', // 下划线\r\n      'strikeThrough', // 删除线\r\n      'foreColor', // 文字颜色\r\n      'backColor', // 背景颜色\r\n      'link', // 插入链接\r\n      'list', // 列表\r\n      'justify', // 对齐方式\r\n      'quote', // 引用\r\n      // 'emoticon', // 表情\r\n      'image', // 插入图片\r\n      'table', // 表格\r\n      'code', // 插入代码\r\n      'undo', // 撤销\r\n      'redo' // 重复\r\n    ]\r\n    this.editor.customConfig.linkCheck = function (text, link) {\r\n    if (link !== \"\") {\r\n      const reg = /(http|ftp|https):\\/\\/[\\w\\-_]+(\\.[\\w\\-_]+)+([\\w\\-\\.,@?^=%&:/~\\+#]*[\\w\\-\\@?^=%&/~\\+#])?/\r\n        if (!reg.test(link)) {\r\n          return '请输入正确的网址(例如 http://baidu.com)'\r\n        } else{\r\n          return true\r\n        }\r\n      }\r\n    }\r\n    this.editor.create() // 创建富文本实例\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 头部标题\r\n.HeadTitle {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n// 新增表单\r\n.el-form {\r\n  margin-top: 30px;\r\n  .el-form-item {\r\n    margin-bottom: 0px;\r\n  }\r\n  .el-input {\r\n    margin-bottom: 10px;\r\n    width: 400px;\r\n  }\r\n  .el-button {\r\n    margin-top: 20px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/information"}]}