,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,WAAA;AACA,KA3BA;AA4BA;AACA,IAAA,YA7BA,wBA6BA,GA7BA,EA6BA;AAAA;;AACA,WAAA,QAAA,CAAA,cAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA;AAFA,OAAA,EAGA,IAHA,uEAGA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAIA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAJA;AAAA;AAAA;AAAA;;AAAA,kDAKA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CALA;;AAAA;AAOA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAHA,IAYA,KAZA,CAYA,YAAA,CACA,CAbA;AAcA,KA5CA;AA6CA;AACA,IAAA,UA9CA,sBA8CA,GA9CA,EA8CA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,kBADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,GAAA,EAAA,KAAA,OAAA,CAAA,MAAA,CAAA,IAAA,CAAA,SAAA,CAAA;AACA,YAAA,EAAA,EAAA,GAAA,CAAA;AADA,WAAA,CAAA;AADA;AAFA,OAAA;AAQA;AAvDA,GAdA;AAuEA,EAAA,OAvEA,qBAuEA;AACA;AACA,SAAA,WAAA;AACA;AA1EA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h3>员工管理</h3>\r\n    <!-- 新增员工 -->\r\n    <el-button size=\"small\" style=\"margin-bottom: 15px;\" type=\"primary\" class=\"addInformation\" @click=\"addUser\">新增员工</el-button>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"id\"\r\n        label=\"编号\"\r\n        width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"name\"\r\n        label=\"员工姓名\"\r\n        width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"phone\"\r\n        label=\"手机号\"\r\n        width=\"150\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"role\"\r\n        label=\"职位\"\r\n        >\r\n      </el-table-column>\r\n       <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 搜索条件\r\n      formInline: {\r\n        pageNo: 1,\r\n        pageSize: 20\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 表格\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 新增员工\r\n    addUser () {\r\n      this.$router.push({\r\n        path: '/personnel/admin',\r\n        query: {\r\n          key: this.$Base64.encode(JSON.stringify({\r\n            title: '新增员工'\r\n          }))\r\n        }\r\n      })\r\n    },\r\n    // 获取员工列表\r\n    async getUserList () {\r\n      const { data: res } = await this.$http.post('/user/showUser', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.formInline.pageNo = 1\r\n      this.getUserList()\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getUserList()\r\n    },\r\n    // 删除\r\n    handleDelete (row) {\r\n      this.$confirm('确定要删除该员工信息吗?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消'\r\n      }).then(async () => {\r\n        const res = await this.$http.post('/user/delUser', {\r\n          id: row.id\r\n        })\r\n        if (res.status !== 200 || res.data !== true) {\r\n          return this.$message.error('操作失败')\r\n        }\r\n        this.$message.success('操作成功')\r\n        this.submit()\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    // 编辑\r\n    handleEdit (row) {\r\n      this.$router.push({\r\n        path: \"/personnel/admin\",\r\n        query: {\r\n          key: this.$Base64.encode(JSON.stringify({\r\n            id: row.id\r\n          }))\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created () {\r\n    // 获取用户列表\r\n    this.getUserList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh3 {\r\n  margin-bottom: 20px;\r\n}\r\n.el-dialog {\r\n  .el-input {\r\n    width: 300px;\r\n  }\r\n  .el-select {\r\n    width: 300px;\r\n  }\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/personnel"}]}