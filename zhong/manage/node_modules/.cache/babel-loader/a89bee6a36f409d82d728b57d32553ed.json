sBAiCA,KAjCA,EAiCA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,MAAA;AACA,KApCA;AAqCA;AACA,IAAA,YAtCA,wBAsCA,GAtCA,EAsCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA,CADA,CAEA;;AAFA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CAHA;;AAAA;AAAA;AAGA,gBAAA,GAHA,SAGA,IAHA;AAMA,gBAAA,MAAA,CAAA,WAAA,GAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KA7CA;AA8CA;AACA,IAAA,MA/CA,oBA+CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,gBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA,EADA;AAEA,kBAAA,KAAA,EAAA;AAFA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAKA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GALA;AAAA;AAAA;AAAA;;AAAA,kDAMA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CANA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA1DA;AA2DA;AACA,IAAA,OA5DA,qBA4DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,gBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA,EADA;AAEA,kBAAA,KAAA,EAAA;AAFA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;AAKA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AALA,sBAMA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GANA;AAAA;AAAA;AAAA;;AAAA,kDAOA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAPA;;AAAA;AASA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAxEA;AAyEA;AACA,IAAA,SA1EA,qBA0EA,GA1EA,EA0EA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,gBADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,OAAA;AAIA;AA/EA,GAxDA;AAyIA,EAAA,OAzIA,qBAyIA;AACA;AACA,SAAA,WAAA;AACA,GA5IA;AA6IA,EAAA,OA7IA,qBA6IA,CACA;AA9IA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h2>帖子列表</h2>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"论坛\">\r\n          <el-select filterable @visible-change=\"dropDownType($event)\" v-model=\"formInline.forumId\" placeholder=\"请选择\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"item in type\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"时间\">\r\n          <div class=\"block\">\r\n            <el-date-picker size=\"mini\" v-model=\"time\" type=\"datetimerange\" :picker-options=\"pickerOptions\" range-separator=\"至\" start-placeholder=\"开始日期\" end-placeholder=\"结束日期\" align=\"right\"></el-date-picker>\r\n          </div>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\">\r\n          <el-select v-model=\"formInline.state\" placeholder=\"选择状态\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"未审核\" value=\"未审核\"></el-option>\r\n            <el-option label=\"已审核\" value=\"已审核\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"帖子标题\">\r\n          <el-input style=\"width: 300px;\" v-model=\"formInline.title\" size=\"mini\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <template>\r\n      <el-table\r\n        :default-sort = \"{prop: 'issueDate', order: 'descending'}\"\r\n        :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\"\r\n        :cell-style=\"{ 'text-align': 'center' }\"\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\">\r\n        <el-table-column\r\n          prop=\"forum\"\r\n          label=\"论坛\"\r\n          width=\"190\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"username\"\r\n          label=\"用户\"\r\n          width=\"150\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"title\"\r\n          label=\"标题\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"state\"\r\n          label=\"状态\"\r\n          width=\"120\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          sortable\r\n          prop=\"postDate\"\r\n          label=\"发布日期\"\r\n          width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"160\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\">审核</el-button>\r\n            <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleSee(scope.row)\">评论</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </template>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n    <!-- 审核对话框 -->\r\n    <el-dialog title=\"帖子审核\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form :model=\"form\">\r\n        <el-form-item label=\"帖子标题\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"this.postDetails.title\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"帖子正文\" :label-width=\"formLabelWidth\">\r\n          <div v-html=\"this.postDetails.text\" class=\"all\"></div>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"success\" @click=\"passed\">通过审核</el-button>\r\n        <el-button type=\"danger\" @click=\"notpass\">未通过审核</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 时间快捷键\r\n      pickerOptions: {\r\n        shortcuts: [{\r\n          text: '最近一周',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 7)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近一个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 30)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近三个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 90)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }]\r\n      },\r\n      time: [],\r\n      // 搜索条件\r\n      formInline: {\r\n        postDateS: '',\r\n        postDateE: '',\r\n        state: '',\r\n        title: '',\r\n        forumId: '',\r\n        pageNo: 1,\r\n        pageSize: 20\r\n      },\r\n      // 类型\r\n      type: [],\r\n      // 总数\r\n      total: 0,\r\n      // 表格\r\n      tableData: [],\r\n      // 对话框\r\n      dialogFormVisible: false,\r\n      postDetails: {},\r\n      form: {\r\n      },\r\n      formLabelWidth: '120px'\r\n    }\r\n  },\r\n  methods: {\r\n    // 事件处理函数\r\n    dateConversion (value) {\r\n      function checkTime (i) {\r\n        if (i < 10) {\r\n          i = '0' + i\r\n        }\r\n        return i\r\n      }\r\n      var d = new Date(value)\r\n      var date = d.getFullYear() + '-' + checkTime(d.getMonth() + 1) + '-' + checkTime(d.getDate()) + ' ' + checkTime(d.getHours()) + ':' + checkTime(d.getMinutes()) + ':' + checkTime(d.getSeconds())\r\n      return date\r\n    },\r\n    // 获取帖子列表\r\n    async getPostList () {\r\n      this.formInline.postDateS = this.dateConversion(this.time[0])\r\n      this.formInline.postDateE = this.dateConversion(this.time[1])\r\n      const { data: res } = await this.$http.post('/invit/showInvit', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 类型\r\n    async dropDownType (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getForumInvit')\r\n        this.type = res\r\n      }\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.getPostList()\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.submit()\r\n    },\r\n    // 审核\r\n    async handleDelete (row) {\r\n      this.dialogFormVisible = true\r\n      // console.log(row.id)\r\n      const { data: res } = await this.$http.post('/invit/getInvit', {\r\n        id: row.id\r\n      })\r\n      this.postDetails = res\r\n    },\r\n    // 通过\r\n    async passed () {\r\n      const res = await this.$http.post('/invit/invitSh', {\r\n        id: this.postDetails.id,\r\n        state: '已审核'\r\n      })\r\n      if (res.data !== true || res.status !== 200) {\r\n        return this.$message.error('操作失败')\r\n      }\r\n      this.$message.success('审核成功')\r\n      this.dialogFormVisible = false\r\n      this.submit()\r\n    },\r\n    // 不通过\r\n    async notpass () {\r\n      const res = await this.$http.post('/invit/invitSh', {\r\n        id: this.postDetails.id,\r\n        state: '未通过'\r\n      })\r\n      console.log(res)\r\n      if (res.data !== true || res.status !== 200) {\r\n        return this.$message.error('操作失败')\r\n      }\r\n      this.$message.success('审核成功')\r\n      this.dialogFormVisible = false\r\n      this.submit()\r\n    },\r\n    // 跳转评论\r\n    handleSee (row) {\r\n      this.$router.push({\r\n        path: '/forum/comment',\r\n        query: { id: row.id }\r\n      })\r\n    }\r\n  },\r\n  created () {\r\n    // 获取资讯列表\r\n    this.getPostList()\r\n  },\r\n  mounted () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n// 对话框\r\n.el-dialog {\r\n  .el-input {\r\n    width: 91%;\r\n  }\r\n  .all {\r\n    width: 90%;\r\n    img {\r\n      width: 100%;\r\n    }\r\n  }\r\n  .dialog-footer {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/forum"}]}