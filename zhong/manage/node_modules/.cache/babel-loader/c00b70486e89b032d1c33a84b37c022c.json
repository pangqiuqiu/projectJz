A,gBAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,CAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KANA;AAOA;AACA,IAAA,GARA,iBAQA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,kBAAA;AACA,KAVA;AAWA;AACA,IAAA,gBAZA,4BAYA,QAZA,EAYA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAGA,gBAAA,MAAA,CAAA,WAAA,GAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAjBA;AAkBA,IAAA,OAlBA,qBAkBA;AACA,WAAA,UAAA,CAAA,IAAA,GAAA,EAAA;AACA,KApBA;AAqBA;AACA,IAAA,YAtBA,wBAsBA,QAtBA,EAsBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA;AACA,kBAAA,UAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAKA,gBAAA,MAAA,CAAA,OAAA,GAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KA7BA;AA8BA;AACA,IAAA,aA/BA,yBA+BA,QA/BA,EA+BA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,qBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KApCA;AAqCA;AACA,IAAA,cAtCA,0BAsCA,QAtCA,EAsCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,sBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAKA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KA7CA;AA8CA;AACA,IAAA,MA/CA,oBA+CA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA,WAAA,cAAA;AACA,KAlDA;AAmDA;AACA,IAAA,UApDA,sBAoDA,GApDA,EAoDA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,GAAA,CAAA,IAAA,KAAA,CADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,IAAA,EAAA,CADA;AAEA,kBAAA,EAAA,EAAA,GAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAAA,sBAMA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IANA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AASA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAVA;AAAA;;AAAA;AAAA;AAAA,uBAYA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,IAAA,EAAA,CADA;AAEA,kBAAA,EAAA,EAAA,GAAA,CAAA;AAFA,iBAAA,CAZA;;AAAA;AAYA,gBAAA,IAZA;;AAAA,sBAgBA,IAAA,CAAA,MAAA,KAAA,GAAA,IAAA,IAAA,CAAA,IAAA,KAAA,IAhBA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAmBA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KA1EA;AA2EA,IAAA,YA3EA,wBA2EA,GA3EA,EA2EA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAIA,oBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,kBAAA,MAAA,CAAA,MAAA;AACA,iBAHA,MAGA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA;AACA,oBAAA,OAAA,EAAA,MADA;AAEA,oBAAA,IAAA,EAAA;AAFA,mBAAA;AAIA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAxFA;AAyFA;AACA,IAAA,eA1FA,2BA0FA,GA1FA,EA0FA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,GAAA,GAAA,GAAA,CAAA,EAAA;AADA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAKA,gBAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAjGA;AAkGA,IAAA,WAlGA,yBAkGA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,SAAA,CAAA,MAAA,KAAA,CADA;AAAA;AAAA;AAAA;;AAAA,kDAEA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CAFA;;AAAA;AAAA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,SADA;AAEA,kBAAA,EAAA,EAAA,MAAA,CAAA,GAFA;AAGA,kBAAA,KAAA,EAAA;AAHA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,GAJA;;AAAA,sBASA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IATA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAYA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KAjHA;AAkHA;AACA,IAAA,UAnHA,sBAmHA,KAnHA,EAmHA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,cAAA;AACA;AAtHA,GAjCA;AAyJA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,YAFA,0BAEA;AACA,UAAA,KAAA,UAAA,CAAA,QAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA,KAPA;AAQA;AACA,IAAA,cATA,4BASA;AACA,UAAA,KAAA,UAAA,CAAA,KAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAdA,GAzJA;AAyKA,EAAA,OAzKA,qBAyKA;AACA;AACA,SAAA,cAAA;AACA,GA5KA;AA6KA,EAAA,OA7KA,qBA6KA,CACA;AA9KA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h2>特卖列表</h2>\r\n    <!-- 新建资讯 -->\r\n    <el-button size=\"small\" type=\"primary\" class=\"addInformation\" @click=\"add\">新建特卖</el-button>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select @change=\"changeP\" @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.province\" :value=\"item.provinceid\" v-for=\"item in provinceAry\" :key=\"item.provinceid\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"城市\">\r\n          <el-select :disabled=\"disabledCity\" @visible-change=\"dropDownCity($event)\" v-model=\"formInline.area\" placeholder=\"请选择城市\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.city\" :value=\"item.city\" v-for=\"item in cityAry\" :key=\"item.cityid\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"品牌\">\r\n          <el-select v-model=\"formInline.brand\" placeholder=\"请选择品牌\" @visible-change=\"dropDownBrand($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"item in brandAry\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"车系\">\r\n          <el-select :disabled=\"disabledSeries\" v-model=\"formInline.series\" placeholder=\"请选择车系\" @visible-change=\"dropDownSeries($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"(item,index) in seriesAry\" :key=\"index\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"id\"\r\n        label=\"编号\"\r\n        width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"brand\"\r\n        label=\"品牌\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"dealer\"\r\n        label=\"发布经销商\"\r\n        >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"activityDate\"\r\n        label=\"特卖时间\"\r\n        width=\"190\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"home\"\r\n        label=\"是否在首页\"\r\n        width=\"130\">\r\n        <template slot-scope=\"scope\">\r\n          <span v-show=\"scope.row.home === 0\">否</span>\r\n          <span v-show=\"scope.row.home === 1\">是</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"state\"\r\n        label=\"推广状态\"\r\n        width=\"130\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"250\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            :disabled=\"scope.row.state === '未申请'?false:true\"\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleExtension(scope.row)\">申请推广</el-button>\r\n          <el-button\r\n            v-if=\"scope.row.bol === 1\"\r\n            size=\"mini\"\r\n            type=\"success\"\r\n            @click=\"handleEdit(scope.row)\">\r\n            <span v-show=\"scope.row.home === 1\" style=\"color: red\">下首页</span>\r\n            <span v-show=\"scope.row.home === 0\">上首页</span>\r\n            </el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 对话框 -->\r\n    <el-dialog :info='msg' title=\"选择推广的车系\" :visible.sync=\"dialogFormVisible\">\r\n      <h2 style=\"color: red;\">每个品牌只能推广一次，请谨慎选择推广的车系</h2>\r\n      <el-checkbox-group v-model=\"checkList\">\r\n        <el-checkbox v-for=\"(item,index) in list\" :key=\"index\" :label=\"item\"></el-checkbox>\r\n      </el-checkbox-group>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogCheck\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 对话框\r\n      dialogFormVisible: false,\r\n      msg: '',\r\n      formLabelWidth: '120px',\r\n      checkList: [], // 选中的\r\n      list: [],\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '',\r\n        area: '',\r\n        brand: '',\r\n        series: '',\r\n        token: window.sessionStorage.getItem('token'),\r\n        pageNo: 1,\r\n        pageSize: 20\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 省份\r\n      provinceAry: [],\r\n      // 城市\r\n      cityAry: [],\r\n      // 品牌\r\n      brandAry: [],\r\n      // 车系\r\n      seriesAry: [],\r\n      // 表格\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取特卖列表\r\n    async getSpecialSale () {\r\n      const { data: res } = await this.$http.post('/sale/showSale', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 新增特卖\r\n    add () {\r\n      this.$router.push('/specialSale/add')\r\n    },\r\n    // 获取省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getProvince')\r\n        this.provinceAry = res\r\n      }\r\n    },\r\n    changeP () {\r\n      this.formInline.area = ''\r\n    },\r\n    // 获取城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getCityByPid', {\r\n          provinceId: this.formInline.province\r\n        })\r\n        this.cityAry = res\r\n      }\r\n    },\r\n    // 获取品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleBrand')\r\n        this.brandAry = res\r\n      }\r\n    },\r\n    // 获取车系\r\n    async dropDownSeries (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleSeries', {\r\n          brand: this.formInline.brand\r\n        })\r\n        this.seriesAry = res\r\n      }\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.formInline.pageNo = 1\r\n      this.getSpecialSale()\r\n    },\r\n    // 表格操作按钮\r\n    async handleEdit (row) { // 上首页\r\n      if (row.home === 0) {\r\n        const res = await this.$http.post('/sale/updateHome', {\r\n          home: 1,\r\n          id: row.id\r\n        })\r\n        if (res.status !== 200 || res.data !== true) {\r\n          return\r\n        }\r\n        this.$message.success('上首页成功')\r\n        this.submit()\r\n      } else {\r\n        const res = await this.$http.post('/sale/updateHome', {\r\n          home: 0,\r\n          id: row.id\r\n        })\r\n        if (res.status !== 200 || res.data !== true) {\r\n          return\r\n        }\r\n        this.$message.success('下首页成功')\r\n        this.submit()\r\n      }\r\n    },\r\n    async handleDelete (row) { // 删除\r\n      const res = await this.$http.post('/sale/delSale', {\r\n        id: row.id\r\n      })\r\n      if (res.status === 200 && res.data === true) {\r\n        this.$message.success('删除成功')\r\n        this.submit()\r\n      } else {\r\n        this.$message({\r\n          message: '删除失败',\r\n          type: 'error'\r\n        })\r\n      }\r\n    },\r\n    // 推广\r\n    async handleExtension (row) {\r\n      this.msg = row.id\r\n      const { data: res } = await this.$http.post('/sale/getSeriesT', {\r\n        id: row.id\r\n      })\r\n      this.list = res\r\n      this.dialogFormVisible = true\r\n    },\r\n    async dialogCheck () {\r\n      if (this.checkList.length === 0) {\r\n        return this.$message.error('至少选择一个车系')\r\n      }\r\n      const res = await this.$http.post('/sale/Tg', {\r\n        serie: this.checkList,\r\n        id: this.msg,\r\n        state: '已申请未推广'\r\n      })\r\n      if (res.status !== 200 || res.data !== true) {\r\n        return\r\n      }\r\n      this.dialogFormVisible = false\r\n      this.$message.success('推广申请成功')\r\n      this.submit()\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getSpecialSale()\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断城市下拉框\r\n    disabledCity () {\r\n      if (this.formInline.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    // 判断车系下拉框\r\n    disabledSeries () {\r\n      if (this.formInline.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  created () {\r\n    // 获取特卖列表\r\n    this.getSpecialSale()\r\n  },\r\n  mounted () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 新建资讯\r\n.addInformation {\r\n  margin-bottom: 20px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/specialSale"}]}