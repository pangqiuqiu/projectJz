AAKA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAzCA;AA0CA;AACA,IAAA,WA3CA,uBA2CA,CA3CA,EA2CA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,EAAA;;AACA,UAAA,CAAA,KAAA,SAAA,EAAA;AACA,aAAA,IAAA,GAAA,KAAA;AACA;AACA,KAhDA;AAiDA,IAAA,QAjDA,sBAiDA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA,WAAA,YAAA;AACA,KApDA;AAqDA;AACA;AACA,IAAA,iBAvDA,6BAuDA,QAvDA,EAuDA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,gBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAGA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KA5DA;AA6DA;AACA,IAAA,kBA9DA,8BA8DA,QA9DA,EA8DA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,IAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAKA,gBAAA,MAAA,CAAA,UAAA,GAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KArEA;AAsEA,IAAA,eAtEA,6BAsEA;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,EAAA;AACA,KAxEA;AAyEA;AACA,IAAA,QA1EA,sBA0EA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,IAAA,CAAA,IAAA,KAAA,MAAA,IAAA,MAAA,CAAA,IAAA,CAAA,KAAA,KAAA,EAAA,IAAA,MAAA,CAAA,IAAA,CAAA,IAAA,KAAA,MAAA,KAAA,MAAA,CAAA,IAAA,CAAA,KAAA,KAAA,EAAA,IAAA,MAAA,CAAA,IAAA,CAAA,MAAA,KAAA,EAAA,CADA;AAAA;AAAA;AAAA;;AAAA,kDAEA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA,CAFA;;AAAA;AAAA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA,MAAA,CAAA,IAAA,CAJA;;AAAA;AAIA,gBAAA,GAJA;;AAAA,sBAKA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GALA;AAAA;AAAA;AAAA;;AAAA,kDAMA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CANA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,YAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KArFA;AAsFA;AACA,IAAA,UAvFA,sBAuFA,GAvFA,EAuFA;AACA,WAAA,QAAA,GAAA,GAAA,CAAA,KAAA;AACA,WAAA,MAAA,GAAA,GAAA,CAAA,EAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KA3FA;AA4FA;AACA,IAAA,YA7FA,0BA6FA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QADA;AAEA,kBAAA,EAAA,EAAA,MAAA,CAAA;AAFA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAKA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IALA;AAAA;AAAA;AAAA;;AAAA,kDAMA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CANA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,YAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AAxGA,GAvDA;AAiKA,EAAA,OAjKA,qBAiKA;AACA;AACA,SAAA,YAAA;AACA;AApKA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 头部标题 -->\r\n    <h2>论坛</h2>\r\n    <!-- 新增论坛 -->\r\n    <el-button type=\"primary\" @click=\"dialogFormVisible = true\" style=\"margin-bottom: 15px;\">新增论坛</el-button>\r\n    <!-- 新增论坛弹框 -->\r\n    <el-dialog title=\"论坛类型\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form :model=\"form\">\r\n        <el-form-item label=\"新增论坛类型\" :label-width=\"formLabelWidth\">\r\n          <el-select v-model=\"form.type\" placeholder=\"请选择论坛类型\">\r\n            <el-option label=\"主题论坛\" value=\"主题论坛\"></el-option>\r\n            <el-option label=\"车型论坛\" value=\"车型论坛\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <!-- 主题 -->\r\n        <el-form-item label=\"主题\" :label-width=\"formLabelWidth\" v-show=\"this.form.type === '主题论坛'\">\r\n          <el-input v-model=\"form.theme\" placeholder=\"请输入自定义主题\"></el-input>\r\n        </el-form-item>\r\n        <!-- 车型 -->\r\n        <el-form-item label=\"车型\" :label-width=\"formLabelWidth\" v-show=\"this.form.type === '车型论坛'\">\r\n          <el-select filterable v-model=\"form.brand\" placeholder=\"请选择品牌\" @change=\"changeBrandForm\" @visible-change=\"dropDownBrandForm($event)\">\r\n            <el-option v-for=\"item in brandList\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n          <el-select filterable :disabled=\"seriesForm\" v-model=\"form.series\" placeholder=\"请选择车系\" @visible-change=\"dropDownSeriesForm($event)\">\r\n            <el-option v-for=\"item in seriesList\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"newForum\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 搜索条件 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select filterable @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in provinceAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"类型\">\r\n          <el-select v-model=\"formInline.type\" placeholder=\"请选择类型\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"主题论坛\" value=\"主题论坛\"></el-option>\r\n            <el-option label=\"地区论坛\" value=\"地区论坛\"></el-option>\r\n            <el-option label=\"车型论坛\" value=\"车型论坛\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"主题\">\r\n          <el-select filterable @visible-change=\"dropDownTheme($event)\" v-model=\"formInline.theme\" placeholder=\"请选择主题\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in themeAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"品牌\">\r\n          <el-select filterable @change=\"changeBrand\" @visible-change=\"dropDownBrand($event)\" v-model=\"formInline.brand\" placeholder=\"请选择品牌\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"(item,index) in brandAry\" :key=\"index\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"车系\">\r\n          <el-select :disabled=\"disabledSeries\" @visible-change=\"dropDownSeries($event)\" v-model=\"formInline.series\" placeholder=\"请选择车系\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"(item,index) in seriesAry\" :key=\"index\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" style=\"width: 100px\" size=\"mini\" @click=\"onSubmit\">搜索</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果区域 -->\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\" :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\" :cell-style=\"{ 'text-align': 'center' }\">\r\n      <el-table-column prop=\"name\" label=\"论坛\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"type\" label=\"类型\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"brand\" label=\"品牌\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"series\" label=\"车系\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"theme\" label=\"主题\"></el-table-column>\r\n      <el-table-column prop=\"province\" label=\"地区\" width=\"100\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"80\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            @click=\"handleEdit(scope.row)\" v-show=\"scope.row.type === '主题论坛'\">修改</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 修改论坛对话框 -->\r\n    <el-dialog title=\"主题论坛\" :visible.sync=\"editForum\">\r\n      <el-form>\r\n        <el-form-item label=\"主题名称\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"editName\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editForum = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editForumAdd\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '', // 省份\r\n        city: '', // 城市\r\n        type: '',\r\n        theme: '',\r\n        brand: '',\r\n        series: '',\r\n        pageSize: 20,\r\n        pageNo: 1\r\n      },\r\n      // 省份\r\n      provinceAry: [],\r\n      cityAry: [],\r\n      themeAry: [],\r\n      brandAry: [],\r\n      seriesAry: [],\r\n      // 表格\r\n      tableData: [],\r\n      total: 0,\r\n      // 新增论坛弹框\r\n      dialogFormVisible: false,\r\n      formLabelWidth: '120px',\r\n      form: {\r\n        type: '',\r\n        theme: '',\r\n        brand: '',\r\n        series: ''\r\n      },\r\n      brandList: [],\r\n      seriesList: [],\r\n      // 修改对话框\r\n      editForum: false,\r\n      editName: '',\r\n      demoId: ''\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断车系下拉框\r\n    disabledSeries () {\r\n      if (this.formInline.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    seriesForm () {\r\n      if (this.form.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取论坛列表\r\n    async getForumList () {\r\n      const { data: res } = await this.$http.post('/invit/forumList', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getForumList()\r\n    },\r\n    // 省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getProvince')\r\n        this.provinceAry = res\r\n      }\r\n    },\r\n    // 主题\r\n    async dropDownTheme (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getTheme')\r\n        this.themeAry = res\r\n      }\r\n    },\r\n    // 品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getBrand')\r\n        this.brandAry = res\r\n      }\r\n    },\r\n    // 车系\r\n    async dropDownSeries (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getSeries', {\r\n          brand: this.formInline.brand\r\n        })\r\n        this.seriesAry = res\r\n      }\r\n    },\r\n    // 改变品牌\r\n    changeBrand (a) {\r\n      this.formInline.series = ''\r\n      if (a !== undefined) {\r\n        this.flag = false\r\n      }\r\n    },\r\n    onSubmit () {\r\n      this.formInline.pageNo = 1\r\n      this.getForumList()\r\n    },\r\n    // 弹框部分-------\r\n    // 品牌\r\n    async dropDownBrandForm (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/nobrand')\r\n        this.brandList = res\r\n      }\r\n    },\r\n    // 车系\r\n    async dropDownSeriesForm (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/noseries', {\r\n          brand: this.form.brand\r\n        })\r\n        this.seriesList = res\r\n      }\r\n    },\r\n    changeBrandForm () {\r\n      this.form.series = ''\r\n    },\r\n    // 新增论坛\r\n    async newForum () {\r\n      if ((this.form.type === '主题论坛' && this.form.theme === '') || (this.form.type === '车型论坛' && (this.form.brand === '' || this.form.series === ''))) {\r\n        return this.$message.error('请完善内容')\r\n      }\r\n      const res = await this.$http.post('/invit/addForum', this.form)\r\n      if (res.data !== true || res.status !== 200) {\r\n        return this.$message.error('新增失败')\r\n      }\r\n      this.$message.success('新增成功')\r\n      this.dialogFormVisible = false\r\n      this.getForumList()\r\n    },\r\n    // 操作\r\n    handleEdit (row) {\r\n      this.editName = row.theme\r\n      this.demoId = row.id\r\n      this.editForum = true\r\n    },\r\n    // 提交修改后的主题论坛\r\n    async editForumAdd () {\r\n      const res = await this.$http.post('/invit/updateForum', {\r\n        theme: this.editName,\r\n        id: this.demoId\r\n      })\r\n      if (res.status !== 200 || res.data !== true) {\r\n        return this.$message.error('修改失败')\r\n      }\r\n      this.$message.success('修改成功')\r\n      this.editForum = false\r\n      this.getForumList()\r\n    }\r\n  },\r\n  created () {\r\n    // 获取论坛列表\r\n    this.getForumList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  margin-bottom: 15px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n.el-dialog {\r\n  .el-select {\r\n    margin-bottom: 20px;\r\n    margin-right: 20px;\r\n  }\r\n}\r\n.el-form {\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n}\r\n.el-table {\r\n  margin-top: 20px;\r\n}\r\n.el-pagination {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/forum"}]}