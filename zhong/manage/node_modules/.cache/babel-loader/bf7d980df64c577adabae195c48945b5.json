,CAAA,MAAA,GAAA,IAAA,CAbA,CAcA;;AACA,qBAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,CAAA,KAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,kBAAA,IADA,GACA,KAAA,CAAA,KAAA,CAAA,CAAA,EAAA,IADA;;AAEA,uBAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,wBAAA,IAAA,CAAA,CAAA,CAAA,CAAA,EAAA,KAAA,IAAA,EAAA,KAAA,CAAA,SAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA,EAAA,IAAA,CAAA,CAAA,CAAA,CAAA,EAAA,EADA,CAEA;AACA;AACA;AACA;AACA;AACA;AACA,iBAzBA,CA0BA;;;AA1BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2BA,KA7BA;AA8BA;AACA,IAAA,MA/BA,kBA+BA,KA/BA,EA+BA,CA/BA,EA+BA;AACA,UAAA,CAAA,KAAA,IAAA,EAAA;AACA,aAAA,SAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA,EAAA,KAAA,EADA,CAEA;AACA,OAHA,MAGA;AACA,aAAA,SAAA,GAAA,KAAA,SAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,iBAAA,KAAA,KAAA,KAAA;AACA,SAFA,CAAA,CADA,CAIA;AACA;AACA,KAzCA;AA0CA;AACA,IAAA,UA3CA,wBA2CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA,EAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,SADA;AAEA,kBAAA,MAAA,EAAA,MAAA,CAAA,GAAA,CAAA,EAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,OAAA;AAHA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAAA,sBAOA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAPA;AAAA;AAAA;AAAA;;AAAA,kDAOA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAPA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AArDA;AAfA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h2>权限管理</h2>\r\n    <!-- 搜索条件 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\">\r\n        <el-form-item label=\"手机号\">\r\n          <el-input v-model=\"phone\" size=\"mini\" placeholder=\"请输入手机号\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 权限 -->\r\n    <div class=\"admin\">\r\n      <!-- 用户信息 -->\r\n      <div class=\"left\">\r\n        <div>\r\n          <label>员工ID</label>\r\n          <span>{{this.obj.id}}</span>\r\n        </div>\r\n        <div>\r\n          <label>姓名</label>\r\n          <span>{{this.obj.name}}</span>\r\n        </div>\r\n        <div>\r\n          <label>手机号</label>\r\n          <span>{{this.obj.phone}}</span>\r\n        </div>\r\n        <div>\r\n          <label>性别</label>\r\n          <span>{{this.obj.sex}}</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"right\">\r\n        <div v-for=\"item in admin\" :key=\"item.token\">\r\n          <h2>{{item.token}}</h2>\r\n          <ul>\r\n            <li v-for=\"items in item.list\" :key=\"items.id\">\r\n              <el-checkbox :label=\"items.name\" @change=\"change(items.id, items.is)\" v-model=\"items.is\"></el-checkbox>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n        <el-button type=\"success\" @click=\"submitForm\" style=\"width: 100%\" v-show=\"isShow\">提交</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 手机号\r\n      phone: '',\r\n      // 员工信息\r\n      obj: {},\r\n      // 权限列表\r\n      admin: [],\r\n      // 发送后台的ID数据\r\n      adminList: [],\r\n      // 提交按钮\r\n      isShow: false\r\n    }\r\n  },\r\n  methods: {\r\n    // 查找\r\n    async submit () {\r\n      // 根据手机号获取员工信息\r\n      if (!(/^1[3456789]\\d{9}$/.test(this.phone))) return this.$message.error('手机号码有误，请重新填写')\r\n      const { data: res } = await this.$http.post('/user/getUserByPhone', { phone: this.phone })\r\n      this.obj = res\r\n      // 获取权限列表\r\n      const { data: rew } = await this.$http.post('/user/ShowQx', {\r\n        phone: this.phone,\r\n        userId: this.obj.id\r\n      })\r\n      this.admin = rew\r\n      // console.log(rew)\r\n      // 显示提交按钮\r\n      this.isShow = true\r\n      // 获取数组中已有权限\r\n      for (let i = 0; i < this.admin.length; i++) {\r\n        const item = this.admin[i].list\r\n        for (let j = 0; j < item.length; j++) {\r\n          if (item[j].is === true) this.adminList.splice(1, 0, item[j].id)\r\n          // if (item.indexOf(item[j].id) !== -1) {\r\n          //   item[j].z = true\r\n          // } else {\r\n          //   console.log('111')\r\n          // }\r\n        }\r\n      }\r\n      // console.log(this.adminList)\r\n    },\r\n    // 改变复选框\r\n    change (index, z) {\r\n      if (z === true) {\r\n        this.adminList.splice(1, 0, index)\r\n        // console.log(this.adminList)\r\n      } else {\r\n        this.adminList = this.adminList.filter(value => {\r\n          return value !== index\r\n        })\r\n        // console.log(this.adminList)\r\n      }\r\n    },\r\n    // 提交\r\n    async submitForm () {\r\n      // console.log(this.adminList)\r\n      const res = await this.$http.post('/user/sq', {\r\n        list: this.adminList,\r\n        userId: this.obj.id,\r\n        token: window.sessionStorage.getItem('token')\r\n      })\r\n      if (res.status !== 200 || res.data !== true) return this.$message.error('设置失败')\r\n      this.$message.success('授权成功')\r\n      this.submit()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n.admin {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  margin-top: 20px;\r\n}\r\n.left {\r\n  width: 300px;\r\n  border: 1px solid #ccc;\r\n  padding: 0 10px;\r\n  margin-right: 20px;\r\n  div {\r\n    height: 50px;\r\n    line-height: 50px;\r\n  }\r\n  label {\r\n    display: inline-block;\r\n    width: 100px;\r\n  }\r\n  span {\r\n    color: blue;\r\n  }\r\n}\r\n.right {\r\n  ul {\r\n    overflow: hidden;\r\n  }\r\n  li {\r\n    float: left;\r\n    .el-checkbox {\r\n      margin-right: 7px;\r\n    }\r\n    span {\r\n      margin-right: 20px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/jurisdiction"}]}