KApBA;AAqBA;AACA,IAAA,YAtBA,wBAsBA,GAtBA,EAsBA;AAAA;;AACA,WAAA,QAAA,CAAA,YAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA;AAFA,OAAA,EAGA,IAHA,uEAGA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AAAA,kBAAA,EAAA,EAAA,GAAA,CAAA;AAAA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAEA,oBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,iBAHA,MAGA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAHA,IAWA,KAXA,CAWA,YAAA,CACA,CAZA;AAaA,KApCA;AAqCA;AACA,IAAA,UAtCA,sBAsCA,KAtCA,EAsCA;AACA,WAAA,SAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,QAAA;AACA;AAzCA,GAZA;AAuDA,EAAA,OAvDA,qBAuDA;AACA;AACA,SAAA,QAAA;AACA;AA1DA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h3>招贤纳士</h3>\r\n    <!-- 新增职位 -->\r\n    <el-button style=\"margin: 10px 0\" type=\"primary\" size=\"small\" @click=\"addWork\">新增职位</el-button>\r\n    <!-- 职位表格 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '16px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"name\"\r\n        label=\"岗位\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"city\"\r\n        label=\"工作地点\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"type\"\r\n        label=\"性质\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"years\"\r\n        label=\"年限\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"formAlign.pageNo\"\r\n      :page-size=\"formAlign.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 表格数据\r\n      tableData: [],\r\n      total: 0,\r\n      formAlign: {\r\n        pageSize: 20,\r\n        pageNo: 1\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取表格数据\r\n    async getTable () {\r\n      const { data: res } = await this.$http.post('/us/job', this.formAlign)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 新增职位\r\n    addWork () {\r\n      this.$router.push({\r\n        path: '/us/joinDetails',\r\n        query: { row: JSON.stringify({}) }\r\n      })\r\n    },\r\n    // 编辑\r\n    handleEdit (row) {\r\n      this.$router.push({\r\n        path: '/us/joinDetails',\r\n        query: { row: JSON.stringify(row) }\r\n      })\r\n    },\r\n    // 删除\r\n    handleDelete(row) {\r\n      this.$confirm('确定要删除此职位吗?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消'\r\n      }).then(async () => {\r\n        const res = await this.$http.post('/us/delJob', { id: row.id })\r\n        if (res.status === 200 && res.data === true) {\r\n          this.getTable()\r\n          this.$message.success('删除成功')\r\n        } else {\r\n          this.$message.error('删除失败')\r\n        }\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    // 分页\r\n    changePage (index) {\r\n      this.formAlign.pageNo = index\r\n      this.getTable()\r\n    }\r\n  },\r\n  created () {\r\n    // 获取表格数据\r\n    this.getTable()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/us"}]}