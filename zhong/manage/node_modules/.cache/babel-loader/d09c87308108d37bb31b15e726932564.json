AA,EAAA,2BADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,OAAA;AAIA,KArCA;AAsCA;AACA,IAAA,YAvCA,wBAuCA,GAvCA,EAuCA;AACA,WAAA,GAAA,GAAA,GAAA,CAAA,EAAA;AACA,WAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAFA,CAGA;AACA;AACA;AACA;AACA;;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,KAhDA;AAiDA,IAAA,UAjDA,wBAiDA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAMA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA,GADA;AAEA,kBAAA,IAAA,EAAA,MAAA,CAAA;AAFA,iBAAA,CANA;;AAAA;AAAA;AAMA,gBAAA,GANA,SAMA,IANA;;AAAA,sBAUA,GAAA,KAAA,IAVA;AAAA;AAAA;AAAA;;AAAA,kDAWA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAXA;;AAAA;AAaA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,MAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA;AAjEA,GAlDA;AAqHA,EAAA,OArHA,qBAqHA;AACA;AACA,SAAA,QAAA;AACA;AAxHA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h3>焦点资讯</h3>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input style=\"width: 300px;\" v-model=\"formInline.title\" size=\"mini\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"时间\">\r\n          <div class=\"block\">\r\n            <el-date-picker size=\"mini\" v-model=\"time\" type=\"datetimerange\" :picker-options=\"pickerOptions\" range-separator=\"至\" start-placeholder=\"开始日期\" end-placeholder=\"结束日期\" align=\"right\"></el-date-picker>\r\n          </div>\r\n        </el-form-item>\r\n        <el-form-item label=\"资讯位置\">\r\n          <el-select v-model=\"formInline.home\" placeholder=\"查看焦点图位置\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"首页资讯\" value=\"首页资讯\"></el-option>\r\n            <el-option label=\"导购\" value=\"导购\"></el-option>\r\n            <el-option label=\"车讯\" value=\"车讯\"></el-option>\r\n            <el-option label=\"技术\" value=\"技术\"></el-option>\r\n            <el-option label=\"行业文化\" value=\"行业文化\"></el-option>\r\n            <el-option label=\"试驾评测\" value=\"试驾评测\"></el-option>\r\n            <el-option label=\"首页焦点图\" value=\"焦点图\"></el-option>\r\n            <el-option label=\"首页焦点资讯\" value=\"焦点资讯\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 数据 -->\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\" :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        prop=\"id\"\r\n        label=\"编号\"\r\n        width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"type\"\r\n        label=\"类型\"\r\n        width=\"110\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"title\"\r\n        label=\"标题\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"home\"\r\n        label=\"焦点图位置\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"issueDate\"\r\n        label=\"发布时间\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleEdit(scope.row)\">查看</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.row)\">修改</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n    <!-- 修改位置 -->\r\n    <el-dialog title=\"修改资讯位置\" :visible.sync=\"dialogFormVisible\" :info=\"msg\">\r\n      <el-radio-group v-model=\"radio\">\r\n        <el-radio label=\"首页资讯\"></el-radio>\r\n        <el-radio label=\"焦点图\"></el-radio>\r\n        <el-radio label=\"焦点资讯\"></el-radio>\r\n        <el-radio label=\"导购\"></el-radio>\r\n        <el-radio label=\"车讯\"></el-radio>\r\n        <el-radio label=\"技术\"></el-radio>\r\n        <el-radio label=\"行业文化\"></el-radio>\r\n        <el-radio label=\"试驾评测\"></el-radio>\r\n        <el-radio label=\"普通资讯\"></el-radio>\r\n      </el-radio-group>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"edtionInfo\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 时间快捷键\r\n      pickerOptions: {\r\n        shortcuts: [{\r\n          text: '最近一周',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 7)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近一个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 30)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }, {\r\n          text: '最近三个月',\r\n          onClick(picker) {\r\n            const end = new Date();\r\n            const start = new Date();\r\n            start.setTime(start.getTime() - 3600 * 1000 * 24 * 90)\r\n            picker.$emit('pick', [start, end]);\r\n          }\r\n        }]\r\n      },\r\n      // 搜索条件\r\n      time: [],\r\n      formInline: {\r\n        home: '',\r\n        issueDateS: '',\r\n        issueDateE: '',\r\n        title: '',\r\n        pageSize: 15,\r\n        pageNo: 1\r\n      },\r\n      // 表格\r\n      tableData: [],\r\n      total: 0,\r\n      // 弹框\r\n      dialogFormVisible: false,\r\n      msg: '',\r\n      radio: ''\r\n    }\r\n  },\r\n  methods: {\r\n    // 时间处理函数\r\n    dateConversion (value) {\r\n      function checkTime (i) {\r\n        if (i < 10) {\r\n          i = '0' + i\r\n        }\r\n        return i\r\n      }\r\n      var d = new Date(value)\r\n      var date = d.getFullYear() + '-' + checkTime(d.getMonth() + 1) + '-' + checkTime(d.getDate()) + ' ' + checkTime(d.getHours()) + ':' + checkTime(d.getMinutes()) + ':' + checkTime(d.getSeconds())\r\n      return date\r\n    },\r\n    // 获取焦点资讯\r\n    async getToday () {\r\n      this.formInline.postDateS = this.dateConversion(this.time[0])\r\n      this.formInline.postDateE = this.dateConversion(this.time[1])\r\n      const { data: res } = await this.$http.post('/Infor/jd', this.formInline)\r\n      this.tableData = res.l\r\n      this.total = res. num\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.formInline.pageNo = 1\r\n      this.getToday()\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.getToday()\r\n    },\r\n    // 查看详情\r\n    handleEdit (row) {\r\n      this.$router.push({\r\n        path: '/information/inforDetails',\r\n        query: { id: row.id }\r\n      })\r\n    },\r\n    // 修改\r\n    handleDelete (row) {\r\n      this.msg = row.id\r\n      this.radio = row.home\r\n      // if (row.home === '1') {\r\n      //   this.radio = '首页资讯'\r\n      // } else {\r\n      //   this.radio = row.home\r\n      // }\r\n      this.dialogFormVisible = true\r\n    },\r\n    async edtionInfo () {\r\n      // if (this.radio === '普通资讯') {\r\n      //   this.radio = '0'\r\n      // } else if (this.radio === '首页资讯') {\r\n      //   this.radio = '1'\r\n      // }\r\n      const { data: res } = await this.$http.post('/Infor/updateHome', {\r\n        id: this.msg,\r\n        home: this.radio\r\n      })\r\n      if (res !== true) {\r\n        return this.$message.error('修改失败')\r\n      }\r\n      this.$message.success('修改成功')\r\n      this.dialogFormVisible = false\r\n      this.submit()\r\n    }\r\n  },\r\n  created () {\r\n    // 获取焦点资讯\r\n    this.getToday()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh3 {\r\n  margin-bottom: 15px;\r\n}\r\n.el-card {\r\n  margin-bottom: 15px;\r\n}\r\n.el-pagination {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n.el-radio {\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/information"}]}