A,CAAA,MAAA,CAAA,YAAA,EAAA,KAAA,CAAA,KAAA,CAAA,SAAA,CAAA,KAAA,CAAA,CAAA,CAAA,EADA,CAEA;;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,QAAA,EAAA;AACA,cAAA,OAAA,EAAA;AACA,gCAAA;AADA;AADA,aAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,kBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA;AACA;;AACA,kBAAA,GAAA,GAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,gBAAA,KAAA,EAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,OAAA,CADA;AAEA,gBAAA,KAAA,EAAA,KAAA,CAAA,IAAA,CAAA,KAFA;AAGA,gBAAA,IAAA,EAAA,KAAA,CAAA,KAHA;AAIA,gBAAA,QAAA,EAAA,KAAA,CAAA,IAAA,CAAA,MAJA;AAKA,gBAAA,IAAA,EAAA,KAAA,CAAA,IAAA,CAAA,OALA;AAMA,gBAAA,KAAA,EAAA,KAAA,CAAA,IAAA,CAAA,QANA;AAOA,gBAAA,QAAA,EAAA,GAAA,CAAA;AAPA,eAAA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,oBAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;AACA,iBAFA,MAEA;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,eAdA,CAAA;AAeA,aAvBA;AAwBA;AACA;AACA,OArDA;AAsDA;AArEA,GAzCA;AAgHA,EAAA,OAhHA,qBAgHA;AACA;AACA,SAAA,kBAAA;AACA;AAnHA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 头部标题 -->\r\n    <div class=\"HeadTitle\">\r\n      <h3>{{this.title}}</h3>\r\n      <el-button type=\"text\" @click=\"back\">《《 返回</el-button>\r\n    </div>\r\n    <!-- 新增资讯表单 -->\r\n    <el-form label-position=\"right\" label-width=\"80px\" :model=\"form\" :rules=\"rules\" ref=\"rulesForm\">\r\n      <el-form-item label=\"标题\" prop=\"title\">\r\n        <el-input v-model=\"form.title\" placeholder=\"请输入标题，不超过25个汉字\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"摘要\" prop=\"abstract\">\r\n        <el-input style=\"margin-bottom: 10px;\" placeholder=\"不超过120字\" type=\"textarea\" :rows=\"2\" v-model=\"form.abstract\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"作者\" prop=\"author\">\r\n        <el-input v-model=\"form.author\" placeholder=\"请输入作者名称\"></el-input>\r\n      </el-form-item>\r\n      <!-- 富文本 -->\r\n      <div class=\"edit_container\">\r\n        <quill-editor\r\n          v-model='form.content'\r\n          ref='myQuillEditor'\r\n          :options=\"editorOption\"\r\n          >\r\n        </quill-editor>\r\n      </div>\r\n      <!-- 上传封面图 -->\r\n      <el-form-item label=\"封面图\">\r\n        <input type=\"file\" id=\"saveImage\" name=\"myphoto\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image\">\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button style=\"width: 400px;\" type=\"primary\" @click=\"submit()\">提交</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst toolbarOptions = [\r\n  ['bold', 'italic', 'underline', 'strike'], // 加粗，斜体，下划线，删除\r\n  [{ 'header': 1 }, { 'header': 2 }], // 标题，键值对的形式；1、2表示字体大小\r\n  [{ 'script': 'sub' }, { 'script': 'super' }], // 上下标\r\n  [{ 'indent': '-1' }, { 'indent': '+1' }], // 缩进\r\n  [{ 'direction': 'rtl' }], // 文本方向\r\n  [{ 'size': ['small', false, 'large', 'huge'] }], // 字体大小\r\n  [{ 'header': [1, 2, 3, 4, 5, 6, false] }], // 几级标题\r\n  [{ 'color': [] }, { 'background': [] }], // 字体颜色，字体背景颜色\r\n  [{ 'font': [] }], // 字体\r\n  [{ 'align': [] }], // 对齐方式\r\n  ['clean'], // 清除字体样式\r\n  ['image'] // 上传图片、上传视频\r\n]\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 资讯标题\r\n      title: '',\r\n      // 表单数据\r\n      form: {\r\n        title: '', // 标题\r\n        abstract: '', // 摘要\r\n        author: '', // 作者\r\n        content: '' // 正文\r\n      },\r\n      // 富文本\r\n      editorOption: {\r\n        modules: {\r\n          toolbar: {\r\n            container: toolbarOptions // 工具栏\r\n          }\r\n        }\r\n      },\r\n      // 表单验证\r\n      rules: {\r\n        title: [\r\n          { required: true, message: '请输入标题', trigger: 'blur' },\r\n          { min: 1, max: 25, message: '长度在 1 到 25 个字符', trigger: 'blur' }\r\n        ],\r\n        abstract: [\r\n          { required: true, message: '请输入摘要', trigger: 'blur' },\r\n          { min: 1, max: 120, message: '不超过120字', trigger: 'blur' }\r\n        ],\r\n        author: [\r\n          { required: true, message: '请输入作者', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    editor () {\r\n      return this.$refs.myQuillEditor.quill\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取资讯类型\r\n    getInformationType () {\r\n      const type = this.$route.query.type\r\n      this.title = type\r\n    },\r\n    // 返回上一页面\r\n    back () {\r\n      this.$router.go(-1)\r\n    },\r\n    // 提交表单 \r\n    submit () {\r\n      if (this.content === '') {\r\n        return\r\n      }\r\n      this.$refs.rulesForm.validate((valid) => {\r\n        if (!valid) {\r\n          this.$message({\r\n            showClose: true,\r\n            message: '请完善表单内容',\r\n            type: 'warning'\r\n          })\r\n        } else {\r\n          const formData = new FormData()\r\n          if (this.$refs.new_image.files[0] === undefined) {\r\n            const rew = this.$http.post('/Infor/addInfor', {\r\n              token: window.sessionStorage.getItem('token'),\r\n              title: this.form.title,\r\n              type: this.title,\r\n              username: this.form.author,\r\n              text: this.form.content,\r\n              intro: this.form.abstract\r\n            }).then(rew => {\r\n              if (rew.data === true) {\r\n                this.$message.success('新建资讯成功')\r\n              } else {\r\n                this.$message.error('新建失败')\r\n              }\r\n            })\r\n          } else {\r\n            formData.append('image_data', this.$refs.new_image.files[0])\r\n            // 单个文件进行上传\r\n            this.$http.post('/upload/addImage', formData, {\r\n              headers: {\r\n                'Content-Type': 'multipart/form-data'\r\n              }\r\n            }).then(res => {\r\n              if (res.status !== 200) {\r\n                return\r\n              }\r\n              const rew = this.$http.post('/Infor/addInfor', {\r\n                token: window.sessionStorage.getItem('token'),\r\n                title: this.form.title,\r\n                type: this.title,\r\n                username: this.form.author,\r\n                text: this.form.content,\r\n                intro: this.form.abstract,\r\n                coverPic: res.data\r\n              }).then(rew => {\r\n                if (rew.data === true) {\r\n                  this.$message.success('新建资讯成功')\r\n                } else {\r\n                  this.$message.error('新建失败')\r\n                }\r\n              })\r\n            })\r\n          }\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created () {\r\n    // 获取资讯类型\r\n    this.getInformationType()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 头部标题\r\n.HeadTitle {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n// 新增表单\r\n.el-form {\r\n  margin-top: 30px;\r\n  .el-form-item {\r\n    margin-bottom: 0px;\r\n  }\r\n  .el-input {\r\n    margin-bottom: 10px;\r\n    width: 400px;\r\n  }\r\n  .el-button {\r\n    margin-top: 20px;\r\n  }\r\n}\r\n// 编辑器\r\n.edit_container {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  /* text-align: center; */\r\n  color: #2c3e50;\r\n  margin-top: 30px;\r\n  margin-bottom: 30px;\r\n}\r\n/* 头部 */\r\n.ql-toolbar {\r\n  width: 1000px;\r\n}\r\n/* 输入区域 */\r\n.ql-container {\r\n  width: 1000px;\r\n  height: 300px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/information"}]}