option>\r\n            <el-option label=\"车型论坛\" value=\"车型论坛\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <!-- 城市 -->\r\n        <el-form-item label=\"城市\" :label-width=\"formLabelWidth\" v-show=\"this.form.type === '地区论坛'\">\r\n          <el-select @change=\"changeProvinceForm\" v-model=\"form.province\" placeholder=\"请输入省份\" @visible-change=\"dropDownProvinceForm($event)\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n          <el-select :disabled=\"cityForm\" v-model=\"form.city\" placeholder=\"请输入城市\" @visible-change=\"dropDownCityForm($event)\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <!-- 主题 -->\r\n        <el-form-item label=\"主题\" :label-width=\"formLabelWidth\" v-show=\"this.form.type === '主题论坛'\">\r\n          <el-input v-model=\"form.theme\" placeholder=\"请输入自定义主题\"></el-input>\r\n        </el-form-item>\r\n        <!-- 车型 -->\r\n        <el-form-item label=\"车型\" :label-width=\"formLabelWidth\" v-show=\"this.form.type === '车型论坛'\">\r\n          <el-select v-model=\"form.brand\" placeholder=\"请选择品牌\" @change=\"changeBrandForm\" @visible-change=\"dropDownBrandForm($event)\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n          <el-select :disabled=\"seriesForm\" v-model=\"form.series\" placeholder=\"请选择车系\" @visible-change=\"dropDownSeriesForm($event)\">\r\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"newForum\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 搜索条件 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select filterable @change=\"changeProvince\" @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in provinceAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"城市\">\r\n          <el-select :disabled=\"disabledCity\" @visible-change=\"dropDownCity($event)\" v-model=\"formInline.city\" placeholder=\"请选择城市\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in cityAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"类型\">\r\n          <el-select v-model=\"formInline.type\" placeholder=\"请选择类型\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"主题论坛\" value=\"主题论坛\"></el-option>\r\n            <el-option label=\"地区论坛\" value=\"地区论坛\"></el-option>\r\n            <el-option label=\"车型论坛\" value=\"车型论坛\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"主题\">\r\n          <el-select filterable @visible-change=\"dropDownTheme($event)\" v-model=\"formInline.theme\" placeholder=\"请选择主题\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"item in themeAry\" :key=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"品牌\">\r\n          <el-select filterable @change=\"changeBrand\" @visible-change=\"dropDownBrand($event)\" v-model=\"formInline.brand\" placeholder=\"请选择品牌\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"(item,index) in brandAry\" :key=\"index\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"车系\">\r\n          <el-select :disabled=\"disabledSeries\" @visible-change=\"dropDownSeries($event)\" v-model=\"formInline.series\" placeholder=\"请选择车系\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item\" :value=\"item\" v-for=\"(item,index) in seriesAry\" :key=\"index\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" style=\"width: 100px\" size=\"mini\" @click=\"onSubmit\">搜索</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果区域 -->\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\" :header-cell-style=\"{ 'text-align': 'center', 'font-size': '20px', 'color': '#000' }\" :cell-style=\"{ 'text-align': 'center' }\">\r\n      <el-table-column prop=\"name\" label=\"论坛\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"type\" label=\"类型\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"brand\" label=\"品牌\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"series\" label=\"车系\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"theme\" label=\"主题\"></el-table-column>\r\n      <el-table-column prop=\"province\" label=\"省份\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"city\" label=\"城市\" width=\"100\"></el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '', // 省份\r\n        city: '', // 城市\r\n        type: '',\r\n        theme: '',\r\n        brand: '',\r\n        series: '',\r\n        pageSize: 20,\r\n        pageNo: 1\r\n      },\r\n      // 省份\r\n      provinceAry: [],\r\n      cityAry: [],\r\n      themeAry: [],\r\n      brandAry: [],\r\n      seriesAry: [],\r\n      // 表格\r\n      tableData: [],\r\n      total: 0,\r\n      // 弹框\r\n      dialogFormVisible: false,\r\n      formLabelWidth: '120px',\r\n      form: {\r\n        province: '',\r\n        city: '',\r\n        type: '',\r\n        theme: '',\r\n        brand: '',\r\n        series: ''\r\n      },\r\n      provinceList: [],\r\n      cityList: [],\r\n      brandList: [],\r\n      seriesList: []\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断车系和城市下拉框\r\n    disabledCity () {\r\n      if (this.formInline.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    disabledSeries () {\r\n      if (this.formInline.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    cityForm () {\r\n      if (this.form.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    seriesForm () {\r\n      if (this.form.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取论坛列表\r\n    async getForumList () {\r\n      const { data: res } = await this.$http.post('/invit/forumList', this.formInline)\r\n      console.log(res)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.onSubmit()\r\n    },\r\n    // 省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getProvince')\r\n        this.provinceAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getCity', {\r\n          province: this.formInline.province\r\n        })\r\n        this.cityAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 主题\r\n    async dropDownTheme (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getTheme')\r\n        this.themeAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getBrand')\r\n        this.brandAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 车系\r\n    async dropDownSeries (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/invit/getSeries', {\r\n          brand: this.formInline.brand\r\n        })\r\n        this.seriesAry = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 改变省份和品牌\r\n    changeProvince () {\r\n      this.formInline.city = ''\r\n    },\r\n    changeBrand () {\r\n      this.formInline.series = ''\r\n    },\r\n    // 搜索\r\n    onSubmit () {\r\n      this.getForumList()\r\n    },\r\n    // 弹框部分-------\r\n    // 省份\r\n    async dropDownProvinceForm (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('')\r\n        this.provinceList = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 城市\r\n    async dropDownCityForm (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('', {\r\n          province: this.form.province\r\n        })\r\n        this.cityList = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 品牌\r\n    async dropDownBrandForm (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('')\r\n        this.brandList = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 车系\r\n    async dropDownSeriesForm (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('', {\r\n          brand: this.form.brand\r\n        })\r\n        this.seriesList = res\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 改变省份和品牌\r\n    changeProvinceForm () {\r\n      this.form.city = ''\r\n    },\r\n    changeBrandForm () {\r\n      this.form.series = ''\r\n    },\r\n    // 新增论坛\r\n    async newForum () {\r\n      if ((this.form.type === '地区论坛' && (this.form.province === '' || this.form.city === '')) || (this.form.type === '主题论坛' && this.form.theme === '') || (this.form.type === '车型论坛' && (this.form.brand === '' || this.form.series === ''))) {\r\n        return\r\n      }\r\n      const res = await this.$http.post('/invit/addForum', this.form)\r\n      if (res.data !== true || res.status !== 200) {\r\n        return this.$message.error('新增失败')\r\n      }\r\n      this.$message.success('新增成功')\r\n      this.dialogFormVisible = false\r\n    }\r\n  },\r\n  created () {\r\n    // 获取论坛列表\r\n    this.getForumList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  margin-bottom: 15px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n.el-dialog {\r\n  .el-select {\r\n    margin-bottom: 20px;\r\n    margin-right: 20px;\r\n  }\r\n  .el-input {\r\n    width: 300px;\r\n  }\r\n}\r\n.el-form {\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n}\r\n.el-table {\r\n  margin-top: 20px;\r\n}\r\n.el-pagination {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/forum"}]}