A,IAAA,CAAA,EAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAhBA;AAiBA;AACA,IAAA,cAlBA,0BAkBA,QAlBA,EAkBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,QAAA,KAAA,IADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,EAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAvBA;AAwBA;AACA,IAAA,MAzBA,oBAyBA;AACA,WAAA,OAAA;AACA,KA3BA;AA4BA;AACA,IAAA,UA7BA,sBA6BA,KA7BA,EA6BA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,MAAA;AACA;AAhCA,GAvBA;AAyDA,EAAA,OAzDA,qBAyDA;AACA;AACA,SAAA,OAAA;AACA;AA5DA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 头部标题 -->\r\n    <div class=\"HeadTitle\">\r\n      <h2>特卖咨询</h2>\r\n      <el-button type=\"text\" @click=\"back\">《《 返回</el-button>\r\n    </div>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"品牌\">\r\n          <el-select @visible-change=\"dropDownBrand($event)\" v-model=\"formInline.brand\" placeholder=\"请选择品牌\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"\" value=\"\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"系列\">\r\n          <el-select :disabled=\"disabled\" @visible-change=\"dropDownSeries($event)\" v-model=\"formInline.series\" placeholder=\"请选择系列\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option label=\"\" value=\"\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 表格展示 -->\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\">\r\n      <el-table-column prop=\"date\" label=\"日期\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"date\" label=\"日期\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"date\" label=\"日期\" width=\"180\"></el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 表单\r\n      formInline: {\r\n        brand: '',\r\n        series: '',\r\n        pageSize: 20,\r\n        pageNo: 1\r\n      },\r\n      // 表格\r\n      tableData: [],\r\n      total: 0\r\n    }\r\n  },\r\n  computed: {\r\n    disabled () {\r\n      if (this.formInline.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  methods: {\r\n    // 返回上一页面\r\n    back () {\r\n      this.$router.go(-1)\r\n    },\r\n    // 获取用户咨询列表\r\n    async getList () {\r\n      const { data: res} = await this.$http.post('', this.formInline)\r\n      console.log(res)\r\n    },\r\n    // 品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('')\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 系列\r\n    async dropDownSeries (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('')\r\n        console.log(res)\r\n      }\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.getList()\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.submit()\r\n    },\r\n  },\r\n  created () {\r\n    // 获取用户咨询列表\r\n    this.getList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 头部标题\r\n.HeadTitle {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  margin-bottom: 20px;\r\n}\r\n// 表格\r\n.el-table {\r\n  margin: 20px 0;\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/specialSale"}]}