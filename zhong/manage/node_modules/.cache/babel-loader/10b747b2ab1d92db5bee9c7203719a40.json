AA,WAdA,yBAcA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,uBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,MAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,SACA,IADA;AAIA,gBAAA,MAAA,CAAA,SAAA,GAAA,CAAA;AAAA,kBAAA,EAAA,EAAA,GAAA,CAAA,QAAA;AAAA,kBAAA,MAAA,EAAA,GAAA,CAAA;AAAA,iBAAA,CAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,OAAA,GAAA,GAAA,CAAA,OAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,UAAA,GAAA,GAAA,CAAA,UAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,OAAA,GAAA,GAAA,CAAA,OAAA;AACA,gBAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,GAAA;;AAbA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KA7BA;AA8BA;AACA,IAAA,IA/BA,kBA+BA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,KAjCA;AAkCA,IAAA,GAlCA,eAkCA,KAlCA,EAkCA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,SAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA,KAAA,SAAA,CAAA,CAAA,CAAA;;AACA,YAAA,IAAA,CAAA,EAAA,KAAA,KAAA,EAAA;AACA,eAAA,IAAA,CAAA,UAAA,GAAA,IAAA,CAAA,MAAA;AACA,eAAA,IAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA;AACA,eAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,eAAA,IAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA;AACA;AACA,KA5CA;AA6CA,IAAA,IA7CA,kBA6CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,MAAA,EAAA,QAAA;AADA,kDAEA,KAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAhDA;AAiDA;AACA,IAAA,MAlDA,oBAkDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,uBAAA,EAAA,MAAA,CAAA,IAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAEA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAFA;AAAA;AAAA;AAAA;;AAAA,kDAGA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAHA;;AAAA;AAKA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA;AAzDA,GAlBA;AA6EA,EAAA,OA7EA,qBA6EA;AACA,SAAA,EAAA,GAAA,MAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,EAAA,CAAA;;AACA,QAAA,KAAA,EAAA,GAAA,CAAA,EAAA;AACA,WAAA,WAAA;AACA,KAFA,MAEA;AACA;AACA,WAAA,OAAA;AACA;AACA;AArFA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"HeadTitle\">\r\n      <h2>合同信息</h2>\r\n      <el-button type=\"text\" @click=\"back\">《《 返回</el-button>\r\n    </div>\r\n    <div class=\"account\">\r\n      <div class=\"left\">\r\n        <el-button type=\"primary\" @click=\"look()\">合同预览</el-button>\r\n      </div>\r\n    </div>\r\n    <div class=\"main\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">合同类型</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-select  :disabled=\"id>0\" v-model=\"shop.type\" placeholder=\"请选择合同类型\" style=\"width:50%\">\r\n            <el-option label=\"充值\" value=\"充值\"></el-option>\r\n            <el-option label=\"软件使用\" value=\"软件使用\"></el-option>\r\n          </el-select>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">充值金额</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-input :readonly=\"id>0\" v-model=\"shop.price\" placeholder=\"请输入金额\" style=\"width:50%\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">关联店方名称</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-select :disabled=\"id>0\" @change=\"ggg\" v-model=\"shop.dealerId\" placeholder=\"请选店方名称\" style=\"width:50%\">\r\n            <el-option v-for=\"item in dealerAry\" :key=\"item.id\" :label=\"item.name4s\" :value=\"item.id\"></el-option>\r\n          </el-select>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">甲方签约主体</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-input :readonly=\"id>0\" v-model=\"shop.dealerName\" placeholder=\"请输入内容\" style=\"width:50%\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">甲方联系地址</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-input :readonly=\"id>0\" v-model=\"shop.address\" placeholder=\"请输入内容\" style=\"width:50%\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">甲方联系人</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-input :readonly=\"id>0\" v-model=\"shop.contacts\" placeholder=\"请输入内容\" style=\"width:50%\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">甲方联系电话</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-input :readonly=\"id>0\" v-model=\"shop.phone\" placeholder=\"请输入内容\" style=\"width:50%\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"4\" class=\"col\">预计付款时间</el-col>\r\n        <el-col :span=\"16\">\r\n          <el-date-picker :readonly=\"id>0\" style=\"width:50%\" v-model=\"shop.payDate\" type=\"datetime\" value-format=\"yyyy-MM-dd HH:mm:ss\"  placeholder=\"选择日期\"/>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"16\" :offset=\"4\">\r\n          <el-button type=\"primary\" @click=\"submit\" style=\"width:50%\">提交</el-button>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      id:'',\r\n      // 店铺信息\r\n      shop: {\r\n        type: '',\r\n        dealerId: '',\r\n        price: '',\r\n        contacts: '',\r\n        phone: '',\r\n        address: '',\r\n        payDate: ''\r\n      },\r\n      dealerAry:[],\r\n      pdfsrc: 'https://jskjcontract.oss-cn-zhangjiakou.aliyuncs.com/testout.pdf',  // 文件地址\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取经销商信息\r\n    async getList () {\r\n      const { data: res } = await this.$http.post('/dealer/dealerByContract', {\r\n        token: window.sessionStorage.getItem('token')\r\n      })\r\n      this.dealerAry = res\r\n      this.shop.type = \"充值\"\r\n      if(this.dealerAry.length>0){\r\n        this.shop.dealerId = this.dealerAry[0].id\r\n        this.ggg(this.shop.dealerId)\r\n      }\r\n      \r\n    },\r\n    async getContract () {\r\n      const { data: res } = await this.$http.post('/contract/getContract', {\r\n        id: this.id\r\n      })\r\n      this.dealerAry = [{id:res.dealerId,name4s:res.dealerName}]\r\n      this.shop.type = res.type\r\n      this.shop.address = res.address\r\n      this.shop.phone = res.phone\r\n      this.shop.contacts = res.contacts\r\n      this.shop.dealerName = res.dealerName\r\n      this.shop.dealerId = res.dealerId\r\n      this.shop.price = res.price\r\n      this.shop.payDate = res.payDate\r\n      this.pdfsrc = res.url\r\n      \r\n    },\r\n    // 返回上一页面\r\n    back () {\r\n      this.$router.go(-1)\r\n    },\r\n    ggg (index) {\r\n       for (let i = 0; i < this.dealerAry.length; i++) {\r\n            const item = this.dealerAry[i]\r\n            if (item.id === index) {\r\n              this.shop.dealerName = item.name4s\r\n              this.shop.contacts = item.link\r\n              this.shop.phone = item.phone\r\n              this.shop.address = item.address\r\n          }\r\n        }\r\n    },\r\n    async look () {\r\n      window.open(this.pdfsrc,\"_blank\");\r\n      return false;\r\n    },\r\n    // 提交\r\n    async submit () {\r\n      const rew = await this.$http.post('/contract/addContract', this.shop)\r\n      if (rew.status !== 200 || rew.data !== true) {\r\n        return this.$message.error('提交失败')\r\n      }\r\n      this.$message.success('新建成功')\r\n      this.$router.go(0)\r\n    }\r\n  },\r\n  created () {\r\n    this.id = Number(this.$route.query.id)\r\n    if (this.id > 0) {\r\n      this.getContract()\r\n    }else{\r\n      // 获取经销商信息\r\n      this.getList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 账户余额\r\n.account {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n\r\n  padding: 0 20px;\r\n  margin-top: 20px;\r\n  margin-bottom: 30px;\r\n  .left {\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    .limit {\r\n      margin: 20px;\r\n    }\r\n  }\r\n}\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 头部标题\r\n.HeadTitle {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background-color: #f7f8f9;\r\n  padding-left: 30px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  margin-bottom: 30px;\r\n}\r\nh2 {\r\n  margin-bottom: 30px;\r\n}\r\n.el-row {\r\n  height: 40px;\r\n  margin-bottom: 20px;\r\n  .col {\r\n    line-height: 40px;\r\n  }\r\n}\r\n.main {\r\n  margin-left: 60px;\r\n  .el-input {\r\n    width: 500px;\r\n  }\r\n  textarea {\r\n    resize: none;\r\n    height: 300px;\r\n    width: 100%;\r\n  }\r\n  .el-button {\r\n    margin-top: 30px;\r\n    width: 100%;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/contract"}]}