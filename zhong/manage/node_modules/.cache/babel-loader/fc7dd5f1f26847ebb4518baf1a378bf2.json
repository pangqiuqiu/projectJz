AA;AAAA;AAAA;AAAA;AAYA,KAnEA;AAoEA,IAAA,WApEA,uBAoEA,GApEA,EAoEA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AADA,sBAEA,GAAA,CAAA,MAAA,KAAA,GAFA;AAAA;AAAA;AAAA;;AAAA,kDAGA,MAAA,CAAA,QAAA,CAAA;AACA,kBAAA,OAAA,EAAA,UADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA,CAHA;;AAAA;AAAA;AAAA,uBAQA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CARA;;AAAA;AAQA,gBAAA,GARA;;AAWA,oBAAA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA;AACA,oBAAA,OAAA,EAAA,MADA;AAEA,oBAAA,IAAA,EAAA;AAFA,mBAAA;AAIA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KAvFA;AAwFA;AACA,IAAA,UAzFA,sBAyFA,KAzFA,EAyFA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,KAAA;AACA,WAAA,MAAA;AACA;AA5FA,GA1BA;AAwHA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,YAFA,0BAEA;AACA,UAAA,KAAA,UAAA,CAAA,QAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA,KAPA;AAQA;AACA,IAAA,cATA,4BASA;AACA,UAAA,KAAA,UAAA,CAAA,KAAA,KAAA,EAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAdA,GAxHA;AAwIA,EAAA,OAxIA,qBAwIA;AACA;AACA,SAAA,cAAA;AACA,GA3IA;AA4IA,EAAA,OA5IA,qBA4IA,CACA;AA7IA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑 -->\r\n    <h2>特卖列表</h2>\r\n    <!-- 新建资讯 -->\r\n    <el-button type=\"primary\" class=\"addInformation\" @click=\"add\">新建特卖</el-button>\r\n    <!-- 搜索区域 -->\r\n    <el-card body-style=\"padding: 10px;\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"省份\">\r\n          <el-select @visible-change=\"dropDownProvince($event)\" v-model=\"formInline.province\" placeholder=\"请选择省份\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.province\" :value=\"item.provinceid\" v-for=\"item in provinceAry\" :key=\"item.provinceid\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"城市\">\r\n          <el-select :disabled=\"disabledCity\" @visible-change=\"dropDownCity($event)\" v-model=\"formInline.area\" placeholder=\"请选择城市\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option :label=\"item.city\" :value=\"item.city\" v-for=\"item in cityAry\" :key=\"item.cityid\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"品牌\">\r\n          <el-select v-model=\"formInline.brand\" placeholder=\"请选择品牌\" @visible-change=\"dropDownBrand($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"item in brandAry\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"车系\">\r\n          <el-select :disabled=\"disabledSeries\" v-model=\"formInline.series\" placeholder=\"请选择车系\" @visible-change=\"dropDownSeries($event)\" size=\"mini\">\r\n            <el-option label=\"不限\" value=\"\"></el-option>\r\n            <el-option v-for=\"(item,index) in seriesAry\" :key=\"index\" :label=\"item\" :value=\"item\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" size=\"mini\">查询</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <!-- 搜索结果 -->\r\n    <template>\r\n      <el-table\r\n        :default-sort = \"{prop: 'issueDate', order: 'descending'}\"\r\n        :header-cell-style=\"{ 'text-align': 'center', 'font-size': '14px', 'color': '#000' }\"\r\n        :cell-style=\"{ 'text-align': 'center' }\"\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\">\r\n        <el-table-column\r\n          prop=\"id\"\r\n          label=\"编号\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <!-- <el-table-column\r\n          prop=\"title\"\r\n          label=\"标题\">\r\n        </el-table-column> -->\r\n        <el-table-column\r\n          prop=\"brand\"\r\n          label=\"品牌\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"state\"\r\n          label=\"状态\"\r\n          width=\"160\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          sortable\r\n          prop=\"activityDate\"\r\n          label=\"特卖时间\"\r\n          width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"申请上首页\"\r\n          >\r\n          <template slot-scope=\"scope\">\r\n            <span v-show=\"scope.row.ishome === '1'\">已申请</span>\r\n            <span v-show=\"scope.row.ishome === '0'\">未申请</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"通过上首页\"\r\n          width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-show=\"scope.row.home === '1'\">是</span>\r\n            <span v-show=\"scope.row.home === '0'\">否</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"260\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleEdit(scope.row.id)\">编辑</el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\">删除</el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"success\"\r\n              @click=\"handleApply(scope.row)\">申请上首页</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </template>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.formInline.pageNo\"\r\n      :page-size=\"this.formInline.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 搜索条件\r\n      formInline: {\r\n        province: '',\r\n        area: '',\r\n        brand: '',\r\n        series: '',\r\n        pageNo: 1,\r\n        pageSize: 5\r\n      },\r\n      // 总数\r\n      total: 0,\r\n      // 省份\r\n      provinceAry: [],\r\n      // 城市\r\n      cityAry: [],\r\n      // 品牌\r\n      brandAry: [],\r\n      // 车系\r\n      seriesAry: [],\r\n      // 表格\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取特卖列表\r\n    async getSpecialSale () {\r\n      const { data: res } = await this.$http.post('/sale/showSale', this.formInline)\r\n      console.log(res)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 新增特卖\r\n    add () {\r\n      this.$router.push('/specialSale/add')\r\n    },\r\n    // 获取省份\r\n    async dropDownProvince (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getProvince')\r\n        this.provinceAry = res\r\n      }\r\n    },\r\n    // 获取城市\r\n    async dropDownCity (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/area/getCityByPid', {\r\n          provinceid: this.formInline.province\r\n        })\r\n        this.cityAry = res\r\n      }\r\n    },\r\n    // 获取品牌\r\n    async dropDownBrand (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleBrand')\r\n        this.brandAry = res\r\n      }\r\n    },\r\n    // 获取车系\r\n    async dropDownSeries (callback) {\r\n      if (callback === true) {\r\n        const { data: res } = await this.$http.post('/sale/showSaleSeries', {\r\n          brand: this.formInline.brand\r\n        })\r\n        this.seriesAry = res\r\n      }\r\n    },\r\n    // 查询\r\n    submit () {\r\n      this.getSpecialSale()\r\n    },\r\n    // 表格操作按钮\r\n    handleEdit (id) { // 编辑\r\n      this.$router.push({\r\n        path: '/information/edit',\r\n        query: { id: id }\r\n      })\r\n    },\r\n    async handleDelete (row) { // 删除\r\n      const res = await this.$http.post('/Infor/delInfor', {\r\n        id: row.id\r\n      })\r\n      if (res.status === 200 && res.data === true) {\r\n        this.$router.go(0)\r\n      } else {\r\n        this.$message({\r\n          message: '删除失败',\r\n          type: 'error'\r\n        })\r\n      }\r\n    },\r\n    async handleApply (row) { // 申请上首页\r\n      console.log(row)\r\n      if (row.ishome === '1') {\r\n        return this.$message({\r\n          message: '您已经申请过了哦',\r\n          type: 'warning'\r\n        })\r\n      }\r\n      const res = await this.$http.post('/Infor/ishome', {\r\n        id: row.id\r\n      })\r\n      if (res.data === true && res.status === 200) {\r\n        this.$router.go(0)\r\n      } else {\r\n        this.$message({\r\n          message: '申请失败',\r\n          type: 'error'\r\n        })\r\n      }\r\n    },\r\n    // 切换页数\r\n    changePage (index) {\r\n      this.formInline.pageNo = index\r\n      this.submit()\r\n    }\r\n  },\r\n  computed: {\r\n    // 判断城市下拉框\r\n    disabledCity () {\r\n      if (this.formInline.province !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    // 判断车系下拉框\r\n    disabledSeries () {\r\n      if (this.formInline.brand !== '') {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n  },\r\n  created () {\r\n    // 获取特卖列表\r\n    this.getSpecialSale()\r\n  },\r\n  mounted () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n// 新建资讯\r\n.addInformation {\r\n  margin-bottom: 20px;\r\n  border-radius: 10px;\r\n}\r\n// 搜索卡片区域\r\n.el-card {\r\n  margin-bottom: 15px;\r\n  .el-form-item {\r\n    margin-right: 20px;\r\n    margin-bottom: 0;\r\n  }\r\n  .el-button {\r\n    width: 150px;\r\n  }\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/specialSale"}]}