AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ForumTitle.vue","sourceRoot":"src/views/forum","sourcesContent":["<template>\r\n  <div class=\"w all\">\r\n    <!-- 广告位 -->\r\n    <div class=\"advertisement w\" v-show=\"this.obj1.pic\">\r\n      <img :src=\"this.obj1.pic ? this.obj1.pic : ''\" alt=\"\" @click=\"foreign1\">\r\n    </div>\r\n    <!-- tab栏 -->\r\n    <el-tabs class=\"w\" v-model=\"activeName\" @tab-click=\"handleClick\">\r\n      <el-tab-pane label=\"热门车系论坛\" name=\"热门车系论坛\">\r\n        <div class=\"items\" @click=\"jumpForum(item.id, item.name)\" v-for=\"item in hotSeries\" :key=\"item.id\">{{item.name}}</div>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"主题论坛\" name=\"主题论坛\">\r\n        <div class=\"items\" @click=\"jumpForum(item.id, item.name)\" v-for=\"item in theme\" :key=\"item.id\">{{item.name}}</div>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"地区论坛\" name=\"地区论坛\">\r\n        <div class=\"items\" @click=\"jumpForum(item.id, item.name)\" v-for=\"item in country\" :key=\"item.id\">{{item.name}}</div>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n    <!-- 主体内容 -->\r\n    <div class=\"main w\">\r\n      <div class=\"left\">\r\n        <div class=\"top\">\r\n          <!-- 轮播图 -->\r\n          <div class=\"block\" style=\"width: 400px\">\r\n            <el-carousel height=\"200px\">\r\n              <el-carousel-item v-if=\"this.obj2.pic\">\r\n                <img :src=\"this.obj2.pic ? this.obj2.pic : ''\" alt=\"\" @click=\"foreign2\">\r\n              </el-carousel-item>\r\n              <el-carousel-item v-for=\"item in swiperList\" :key=\"item.id\">\r\n                <img style=\"height: 100%\" :src=\"item.coverPic\" alt=\"\" @click=\"jumpPost(item.id)\">\r\n              </el-carousel-item>\r\n            </el-carousel>\r\n          </div>\r\n          <!-- 文字帖子 -->\r\n          <div class=\"word\">\r\n            <h3 @click=\"jumpPost(wordObj.id)\">{{wordObj.title}}</h3>\r\n            <ul>\r\n              <li @click=\"jumpPost(item.id)\" v-for=\"(item, i) in wordList\" :key=\"i\">\r\n                <div>\r\n                  <em></em>\r\n                  <span>{{item.title}}</span>\r\n                </div>\r\n                <i>{{item.nikename}}</i>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n        <div class=\"bottom\">\r\n          <h3>精选</h3>\r\n          <!-- 分割线 -->\r\n          <el-divider></el-divider>\r\n          <!-- 精选列表 -->\r\n          <ul>\r\n            <li @click=\"jumpPost(item.id)\" v-for=\"(item, i) in selectList\" :key=\"i\">\r\n              <img :src=\"item.coverPic\" alt=\"\">\r\n              <label>{{item.title}}</label>\r\n              <div>\r\n                <span>用户 : </span>\r\n                <em>{{item.nikename}}</em>\r\n              </div>\r\n              <div>\r\n                <span>论坛 : </span>\r\n                <em>{{item.forumName}}</em>\r\n              </div>\r\n              <div>\r\n                <span>浏览 : </span>\r\n                <em>{{item.clickNum}}</em>\r\n              </div>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n      <div class=\"right\">\r\n        <h3>热门论坛</h3>\r\n        <!-- 论坛 -->\r\n        <ul>\r\n          <li @click=\"jumpForum(item.id,item.name)\" v-for=\"(item, i) in hotForum\" :key=\"i\">\r\n            <img :src=\"item.pic\" alt=\"\">\r\n            <div>\r\n              <div style=\"color: #0777DD\">{{item.name}}</div>\r\n              <div>人气指数: <span>{{item.ivNum}}</span></div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 广告\r\n      advertiseList: [],\r\n      obj1: {},\r\n      obj2: {},\r\n      // 论坛\r\n      activeName: '热门车系论坛',\r\n      // 热门车系\r\n      hotSeries: [],\r\n      // 主题论坛\r\n      theme: [],\r\n      // 地区论坛\r\n      country: [],\r\n      // 轮播图\r\n      swiperList: [],\r\n      // 文字帖子1\r\n      wordObj: {},\r\n      // 文字帖子2-7\r\n      wordList: [],\r\n      // 精选列表\r\n      selectList: [],\r\n      // 热门论坛\r\n      hotForum: []\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getAdvertisement()\r\n  },\r\n  methods: {\r\n    // 广告\r\n    async getAdvertisement () {\r\n      const { data: res } = await this.$http.post('/adv/getAdv', {\r\n        name: [\r\n          { name: '论坛顶部通栏', area: sessionStorage.getItem('LocationProvince') },\r\n          { name: '论坛焦点图', area: sessionStorage.getItem('LocationCity') }\r\n        ]\r\n      })\r\n      this.advertiseList = res\r\n      this.obj1 = this.advertiseList[0] || {}\r\n      this.obj2 = this.advertiseList[1] || {}\r\n    },\r\n    foreign1 () {\r\n      window.open(`http://${this.obj1.url}`)\r\n    },\r\n    foreign2 () {\r\n      window.open(`http://${this.obj2.url}`)\r\n    },\r\n    // tab标签切换\r\n    async handleClick (index) {\r\n      if (index.label === '主题论坛') {\r\n        const { data: res } = await this.$http.post('/Invit/getForumByType', {\r\n          type: index.label\r\n        })\r\n        this.theme = res\r\n      } else if (index.label === '地区论坛') {\r\n        const { data: res } = await this.$http.post('/Invit/getForumByType', {\r\n          type: index.label\r\n        })\r\n        this.country = res\r\n      }\r\n    },\r\n    // 热门车系\r\n    async getHotSeries () {\r\n      const { data: res } = await this.$http.post('/Invit/getForumByType', {\r\n        type: '车型论坛'\r\n      })\r\n      this.hotSeries = res\r\n    },\r\n    // 点击论坛事件\r\n    jumpForum (id, name) {\r\n      // const obj = {}\r\n      // obj.id = id\r\n      // obj.name = name\r\n      const { href } = this.$router.resolve({\r\n        path: '/forum/post/list',\r\n        query: { id: id, name: name }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 轮播图\r\n    async getSwiper () {\r\n      const { data: res } = await this.$http.post('/Invit/getLb')\r\n      this.swiperList = res\r\n    },\r\n    // 点击轮播图事件\r\n    jumpPost (id) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/forum/post/details',\r\n        query: { id: id }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 文字帖子\r\n    async getWord () {\r\n      const { data: res } = await this.$http.post('/Invit/getJd')\r\n      this.wordObj = res[0]\r\n      this.wordList = res.slice(1, 6)\r\n    },\r\n    // 精选列表\r\n    async getSelectList () {\r\n      const { data: res } = await this.$http.post('/Invit/getJx')\r\n      this.selectList = res\r\n    },\r\n    // 热门论坛\r\n    async getHotForum () {\r\n      const { data: res } = await this.$http.post('/Invit/hotForum')\r\n      this.hotForum = res\r\n    }\r\n  },\r\n  created () {\r\n    // 获取论坛数据\r\n    this.getHotSeries()\r\n    // 轮播图\r\n    this.getSwiper()\r\n    // 文字帖子\r\n    this.getWord()\r\n    // 精选列表\r\n    this.getSelectList()\r\n    // 热门论坛\r\n    this.getHotForum()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.w {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n// 广告位\r\n.advertisement {\r\n  height: 100px;\r\n  margin-bottom: 20px;\r\n}\r\n// tab栏\r\n.el-tabs {\r\n  height: 350px;\r\n  .el-tab-pane {\r\n    flex-wrap: wrap;\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    .items {\r\n      width: 200px;\r\n      margin-bottom: 10px;\r\n    }\r\n    .items:hover {\r\n      color: rgb(43, 121, 255);\r\n      cursor: pointer;\r\n    }\r\n  }\r\n}\r\n// 主体\r\n.main {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  .left .top {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    .word {\r\n      margin: 0 30px;\r\n      h3 {\r\n        color: #EB3030;\r\n        margin-bottom: 20px;\r\n        width: 540px;\r\n        text-align: center;\r\n        cursor: pointer;\r\n      }\r\n      li {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        margin-bottom: 10px;\r\n        font-size: 12px;\r\n        cursor: pointer;\r\n        em {\r\n          display: inline-block;\r\n          vertical-align: middle;\r\n          width: 5px;\r\n          height: 5px;\r\n          border-radius: 50%;\r\n          background-color: #bbb;\r\n          margin-right: 7px;\r\n        }\r\n        span {\r\n          letter-spacing: 2px;\r\n        }\r\n        i {\r\n          font-style: normal;\r\n          color: #777;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .left .bottom {\r\n    width: 990px;\r\n    margin-bottom: 20px;\r\n    h3 {\r\n      font-weight: 400;\r\n      height: 70px;\r\n      line-height: 90px;\r\n    }\r\n    ul {\r\n      display: flex;\r\n      justify-content: flex-start;\r\n      flex-wrap: wrap;\r\n      li:nth-of-type(3n+2) {\r\n        margin: 0 5%;\r\n      }\r\n    }\r\n    li {\r\n      width: 30%;\r\n      cursor: pointer;\r\n      img {\r\n        display: block;\r\n        width: 200px;\r\n        height: 100px;\r\n        margin: 15px 0;\r\n      }\r\n      label {\r\n        display: block;\r\n        color: #333;\r\n        font-size: 10px;\r\n        margin-bottom: 15px;\r\n      }\r\n      div {\r\n        span {\r\n          color: #333;\r\n          font-size: 10px;\r\n        }\r\n        em {\r\n          color: #0777DD;\r\n          font-size: 10px;\r\n          font-style: normal;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .right {\r\n    width: 180px;\r\n    background-color: #F4F4F4;\r\n    padding: 10px 20px;\r\n    box-sizing: border-box;\r\n    h3 {\r\n      font-weight: 400;\r\n      color: #333333;\r\n      margin-bottom: 10px;\r\n    }\r\n    li {\r\n      display: flex;\r\n      justify-content: flex-start;\r\n      margin-bottom: 20px;\r\n      cursor: pointer;\r\n      >div {\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: space-around;\r\n        font-size: 10px;\r\n        padding: 5px 0;\r\n      }\r\n      img {\r\n        width: 40px;\r\n        height: 48px;\r\n        margin-right: 15px;\r\n      }\r\n      span {\r\n        color: #FF0D2F;\r\n      }\r\n    }\r\n  }\r\n}\r\n// 轮播图\r\n.el-carousel {\r\n  img {\r\n    cursor: pointer;\r\n  }\r\n}\r\n</style>\r\n"]}]}