{"remainingRequest":"D:\\myCompany\\zhongdianqiche\\car\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\myCompany\\zhongdianqiche\\car\\src\\views\\automobile\\DetailsHome.vue?vue&type=style&index=0&id=5a88ae99&lang=less&scoped=true&","dependencies":[{"path":"D:\\myCompany\\zhongdianqiche\\car\\src\\views\\automobile\\DetailsHome.vue","mtime":1589179731977},{"path":"D:\\myCompany\\zhongdianqiche\\car\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\myCompany\\zhongdianqiche\\car\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"D:\\myCompany\\zhongdianqiche\\car\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"D:\\myCompany\\zhongdianqiche\\car\\node_modules\\less-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\myCompany\\zhongdianqiche\\car\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\myCompany\\zhongdianqiche\\car\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["DetailsHome.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkaA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"DetailsHome.vue","sourceRoot":"src/views/automobile","sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <!-- 详细介绍 -->\r\n    <div class=\"automobile_details w\">\r\n      <!-- 展示图片区域 -->\r\n      <el-carousel trigger=\"click\" height=\"350px\" class=\"left\" indicator-position='none'>\r\n        <el-carousel-item v-for=\"item in swiperList\" :key=\"item\">\r\n          <img :src=\"item\" alt=\"\" style=\"height: 100%\">\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n      <div class=\"right\">\r\n        <div class=\"official_price\">\r\n          <label>官方指导价格：</label>\r\n          <h2>{{this.detailsList.guide}} 万元</h2>\r\n        </div>\r\n        <div class=\"distributor_price\">\r\n          <label>经销商报价：</label>\r\n          <span>{{this.detailsList.off}}</span>\r\n        </div>\r\n        <!-- <div class=\"distributor_price\">\r\n          <label>分期: </label>\r\n          <span></span>\r\n        </div> -->\r\n        <div class=\"everyone\">\r\n          <label for=\"\">排量：</label>\r\n          <span>{{this.detailsList.energy}}</span>\r\n        </div>\r\n        <div class=\"everyone\">\r\n          <label for=\"\">级别：</label>\r\n          <span>{{this.detailsList.level}}</span>\r\n        </div>\r\n        <div class=\"everyone\">\r\n          <label for=\"\">变速箱：</label>\r\n          <span>{{this.detailsList.gearBox}}</span>\r\n        </div>\r\n        <div class=\"everyone\">\r\n          <label for=\"\">车身类型：</label>\r\n          <span>{{this.detailsList.carType}}</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div style=\"clear: both\"></div>\r\n    <!-- 最新上市 -->\r\n    <div class=\"w latest_listing\">\r\n      <el-row class=\"left\">\r\n        <!-- 标题 -->\r\n        <div class=\"title\">\r\n          <ul>\r\n            <li @click=\"latestListingChange1(index,item.name)\" :class=\"currentIndex1==index?'selectedItem':''\" v-for=\"(item,index) in latest_listing\" :key=\"index\">{{item.name}}</li>\r\n          </ul>\r\n        </div>\r\n        <!-- 详情 -->\r\n        <div class=\"details\" v-for=\"item in latestListingAry\" :key=\"item.id\">\r\n          <div @click=\"informationTitle(item.id)\">\r\n            <span>{{item.type}}</span>\r\n            <span style=\"white-space: nowrap; overflow: hidden; text-overflow: ellipsis;\">{{item.title}}</span>\r\n            <span style=\"margin-left: 20px\">{{item.issueDate}}</span>\r\n          </div>\r\n        </div>\r\n      </el-row>\r\n    </div>\r\n    <!-- 车型报价 -->\r\n    <div class=\"w model_quotation\">\r\n      <!-- 标题 -->\r\n      <div class=\"title\">\r\n        <span class=\"selectedItem\">车型报价</span>\r\n      </div>\r\n      <!-- 分割线 -->\r\n      <el-divider></el-divider>\r\n      <!-- 详情 -->\r\n      <div>\r\n        <el-table\r\n          :data=\"tableData\"\r\n          style=\"width: 100%\"\r\n          :header-cell-style=\"{'background-color': '#ddd', 'text-align': 'center'}\"\r\n          :cell-style=\"{'text-align': 'center'}\">\r\n          <el-table-column\r\n            prop=\"type\"\r\n            label=\"款式\"\r\n            width=\"400\">\r\n          </el-table-column>\r\n          <!-- <el-table-column prop=\"information\" label=\"车型信息\" width=\"180\">\r\n            <template>\r\n              <el-button type=\"text\" size=\"small\">参配</el-button>\r\n              <el-button type=\"text\" size=\"small\">图片</el-button>\r\n              <el-button type=\"text\" size=\"small\">点评</el-button>\r\n            </template>\r\n          </el-table-column> -->\r\n          <el-table-column\r\n            prop=\"guidePrice\"\r\n            label=\"官方价(万元)\"\r\n            width=\"250\">\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"offer\"\r\n            label=\"本地最低报价\"\r\n            width=\"250\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"button\" label=\"\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"danger\" size=\"mini\" @click=\"table_bottom(scope.row.id)\">获取底价</el-button>\r\n              <el-button size=\"mini\" @click=\"table_buy(scope.row.id)\">分期购车</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </div>\r\n    <!-- 询问低价 -->\r\n    <div class=\"w asking_reserve_price\">\r\n      <el-row :gutter=\"20\">\r\n        <!-- Tab栏 -->\r\n        <Tab></Tab>\r\n      </el-row>\r\n    </div>\r\n    <!-- 推荐经销商 -->\r\n    <div class=\"w recommend_dealers\">\r\n      <!-- 标题 -->\r\n      <el-row :gutter=\"20\" class=\"title\">\r\n        <el-col :span=\"15\" class=\"left\">\r\n          <h4>推荐经销商</h4>\r\n        </el-col>\r\n        <el-col :span=\"2\" style=\"visibility: hidden;\">重点汽车网</el-col>\r\n        <el-col :span=\"7\">\r\n          <h4>推荐</h4>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 详情 -->\r\n      <el-row :gutter=\"20\" class=\"details\">\r\n        <el-col :span=\"15\" v-show=\"this.flag\">\r\n          <div v-for=\"item in dealersAry\" :key=\"item.index\">\r\n            <div class=\"left\">\r\n              <div>{{item.name4s}}</div>\r\n              <div>\r\n                <label>电话：</label>\r\n                <span style=\"color: red;\">{{item.tel}}</span>\r\n              </div>\r\n              <div>\r\n                <label>地址：</label>\r\n                <span>{{item.address}}</span>\r\n              </div>\r\n              <div>\r\n                <el-button type=\"success\" @click=\"True(item.id)\" size=\"mini\">获取底价</el-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"15\" v-show=\"!this.flag\">\r\n          <h2>暂无推荐经销商,先看看别的车系吧！</h2>\r\n        </el-col>\r\n        <el-col :span=\"2\" style=\"visibility: hidden;\">1</el-col>\r\n        <el-col :span=\"7\">\r\n          <ul>\r\n            <li @click=\"jump_series(item.series)\" v-for=\"item in dealersRecommendAry\" :key=\"item.index\">\r\n              <h4>{{item.series}}</h4>\r\n              <span>{{item.guidePrice}}</span>\r\n            </li>\r\n          </ul>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n    <!-- 对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"dialogFormVisible\" :carInfo=\"mag\" :info=\"msg\">\r\n      <el-form :model=\"form\">\r\n        <el-form-item label=\"尊称\" :label-width=\"formLabelWidth\">\r\n          <el-input style=\"width: 300px\" v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机号\" :label-width=\"formLabelWidth\">\r\n          <el-input style=\"width: 300px\" v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"TrueDialog\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport BMap from 'BMap'\r\nimport Tab from '../../components/Tab.vue'\r\nlet id = window.sessionStorage.getItem('carDetails')\r\nexport default {\r\n  components: {\r\n    Tab\r\n  },\r\n  data () {\r\n    return {\r\n      // 定位\r\n      locationProvince: '',\r\n      locationCity: '',\r\n      // 汽车详细信息集合\r\n      detailsList: {},\r\n      DisplacementList: [],\r\n      GearboxList: [],\r\n      // 轮播图\r\n      swiperList: [],\r\n      // 最新上市\r\n      latestListingAry: [],\r\n      // 城市\r\n      provinceAry: [], // 省\r\n      cityAry: [], // 市\r\n      value: '',\r\n      // 最新上市\r\n      currentIndex1: 0,\r\n      // 最新上市\r\n      latest_listing: [\r\n        { name: '最新上市' },\r\n        { name: '车讯' },\r\n        { name: '评测' },\r\n        { name: '导购' }\r\n      ],\r\n      // 车型报价表格\r\n      tableData: [],\r\n      // 推荐经销商\r\n      dealersAry: [],\r\n      flag: true,\r\n      dealersRecommendAry: [],\r\n      // 对话框\r\n      dialogFormVisible: false,\r\n      title: '',\r\n      msg: 0, // 经销商id\r\n      mag: 0, // 汽车id\r\n      form: {\r\n        name: '',\r\n        phone: ''\r\n      },\r\n      formLabelWidth: '120px'\r\n    }\r\n  },\r\n  methods: {\r\n    // 百度定位\r\n    ready () {\r\n      var geolocation = new BMap.Geolocation()\r\n      geolocation.getCurrentPosition(r => {\r\n        this.locationProvince = r.address.province\r\n        this.locationCity = r.address.city\r\n        // 获取经销商列表\r\n        this.getDistributor()\r\n      }, { enableHighAccuracy: true })\r\n    },\r\n    // 轮播图\r\n    async getSwiperList () {\r\n      const { data: res } = await this.$http.post('/Pic/getCarHome', {\r\n        series: id\r\n      })\r\n      this.swiperList = res\r\n    },\r\n    // 获取汽车详细信息\r\n    async getDetailsList () {\r\n      const { data: res } = await this.$http.post(`/car/carDetails`, {\r\n        series: id\r\n      })\r\n      // console.log(res)\r\n      this.detailsList = res\r\n    },\r\n    // 排量\r\n    async getDisplacement1 () {\r\n      const { data: res } = await this.$http.post(`/car/engine`, {\r\n        series: id\r\n      })\r\n      // console.log(res)\r\n      this.DisplacementList = res\r\n    },\r\n    // 变速箱\r\n    async getGearbox1 () {\r\n      const { data: res } = await this.$http.post(`/car/gearBox`, {\r\n        series: id\r\n      })\r\n      // console.log(res)\r\n      this.GearboxList = res\r\n    },\r\n    // 切换图片区域\r\n    changePic () {},\r\n    // 最新上市\r\n    async getLatestListing () {\r\n      const { data: res } = await this.$http.post('/Infor/MostInforzx', {\r\n        series: id\r\n      })\r\n      this.latestListingAry = res\r\n    },\r\n    // 切换最新上市小标题\r\n    async latestListingChange1 (index, name) {\r\n      this.currentIndex1 = index\r\n      const { data: res } = await this.$http.post(`/Infor/MostInforcx`, {\r\n        type: name,\r\n        series: id\r\n      })\r\n      this.latestListingAry = res\r\n      if (index === 0) {\r\n        this.getLatestListing()\r\n      }\r\n    },\r\n    informationTitle (id) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/automotivelnformation/details',\r\n        query: { id: id }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 车型报价\r\n    async getModelQuotation () {\r\n      const { data: res } = await this.$http.post(`/car/carOffer`, {\r\n        series: id,\r\n        city: window.sessionStorage.getItem('selectCity') || window.sessionStorage.getItem('LocationCity')\r\n      })\r\n      // console.log(res)\r\n      this.tableData = res\r\n    },\r\n    // 获取底价 分期购车\r\n    table_bottom (id) {\r\n      this.title = '获取底价'\r\n      this.dialogFormVisible = true\r\n      this.mag = id\r\n    },\r\n    table_buy (id) {\r\n      this.title = '分期购车'\r\n      this.dialogFormVisible = true\r\n      this.mag = id\r\n    },\r\n    // 车型报价排量\r\n    async getDisplacement () {\r\n      const { data: res } = await this.$http.post(`/car/engine`, {\r\n        id: id\r\n      })\r\n      // console.log(res)\r\n      this.model_quotation_displacement = res\r\n    },\r\n    // 车型报价变速箱\r\n    async getTransmissioncase () {\r\n      const { data: res } = await this.$http.post(`/car/gearBox`, {\r\n        id: id\r\n      })\r\n      this.model_quotation_transmission = res\r\n      // console.log(res)\r\n    },\r\n    // 推荐经销商列表\r\n    async getDistributor () {\r\n      const { data: res } = await this.$http.post('/dealer/getTjD', {\r\n        series: id,\r\n        province: this.locationProvince,\r\n        city: this.locationCity\r\n      })\r\n      this.dealersAry = res\r\n      if (this.dealersAry.length === 0) {\r\n        this.flag = false\r\n      }\r\n    },\r\n    // 推荐车系\r\n    async getRecommendSeries () {\r\n      const { data: res } = await this.$http.post('/car/getGuide', {\r\n        series: id\r\n      })\r\n      this.dealersRecommendAry = res.l\r\n    },\r\n    // 获取底价\r\n    True (disId) {\r\n      this.title = '获取底价'\r\n      this.msg = disId\r\n      this.dialogFormVisible = true\r\n    },\r\n    // 对话框\r\n    async TrueDialog () {\r\n      if (this.form.phone === '' || this.form.name === '') {\r\n        return this.$message.error('请输入手机号和姓名')\r\n      }\r\n      const mobile = /^1[3456789]\\d{9}$/\r\n      if (!mobile.test(this.form.phone)) {\r\n        return this.$message.error('手机号不合法')\r\n      }\r\n      const res = await this.$http.post(`/dealer/addXs`, {\r\n        type: '获取底价',\r\n        dealerId: this.msg,\r\n        series: id,\r\n        name: this.form.name,\r\n        phone: this.form.phone,\r\n        carId: this.mag\r\n      })\r\n      // console.log(res)\r\n      if (res.data === true && res.status === 200) {\r\n        this.$message.success('提交成功')\r\n        this.dialogFormVisible = false\r\n      } else {\r\n        return this.$message.success('提交失败')\r\n      }\r\n    },\r\n    // 推荐车系\r\n    jump_series (series) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/automobile/details/detailshome'\r\n      })\r\n      window.sessionStorage.setItem('carDetails', series)\r\n      window.open(href, '_blank')\r\n    }\r\n  },\r\n  mounted () {\r\n    this.ready()\r\n  },\r\n  created () {\r\n    // 轮播图\r\n    this.getSwiperList()\r\n    // 获取汽车详细详细\r\n    this.getDetailsList()\r\n    // 最新上市\r\n    this.getLatestListing()\r\n    // 车型报价\r\n    this.getModelQuotation()\r\n    // 车型报价排量\r\n    this.getDisplacement()\r\n    // 车型报价变速箱\r\n    this.getTransmissioncase()\r\n    // 推荐车系\r\n    this.getRecommendSeries()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 版心1200px\r\n.w {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n// 被选中项\r\n.selectedItem {\r\n  font-weight: 700!important;\r\n  color: orange!important;\r\n  font-size: 16px!important;\r\n}\r\n.selectedItem1 {\r\n  color: #FF6B09;\r\n}\r\n.selectedItem2 {\r\n  background-color: skyblue!important;\r\n}\r\n.selectedItem3 {\r\n  background-color: red!important;\r\n  color: #fff!important;\r\n}\r\n// 详情介绍\r\n.automobile_details {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  .left {\r\n    min-width: 450px;\r\n    margin-right: 30px;\r\n  }\r\n  .right {\r\n    span {\r\n      font-size: 20px!important;\r\n    }\r\n    >div {\r\n      margin-top: 15px;\r\n    }\r\n    label {\r\n      color: #bbb;\r\n      font-size: 16px;\r\n    }\r\n    .official_price {\r\n      h2 {\r\n        color: red;\r\n        display: inline;\r\n      }\r\n    }\r\n  }\r\n}\r\n// 最新上市\r\n.latest_listing {\r\n  margin-top: 20px;\r\n  .left,.right {\r\n    padding-right: 30px;\r\n    .title {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      color: #ccc;\r\n      height: 40px;\r\n      line-height: 40px;\r\n      li {\r\n        float: left;\r\n        margin-right: 30px;\r\n        font-size: 16px;\r\n        cursor: pointer;\r\n      }\r\n      div {\r\n        font-size: 12px;\r\n      }\r\n    }\r\n    .details {\r\n      margin-to