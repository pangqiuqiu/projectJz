orum>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"我的回帖\" name=\"second\">\r\n        <h3 v-show=\"!count2\">暂无回复的帖子</h3>\r\n        <MyReply @myReply=\"myReply\" v-show=\"count2\"></MyReply>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"我的精华帖\" name=\"third\">\r\n        <h3 v-show=\"!count3\">暂无精华帖</h3>\r\n        <MyEssences @myEssences=\"myEssences\" v-show=\"count3\"></MyEssences>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookie from 'js-cookie'\r\nimport MyForum from '../../components/user/MyForum'\r\nimport MyReply from '../../components/user/MyReply'\r\nimport MyEssences from '../../components/user/MyEssences'\r\nconst ID = Cookie.get('ID')\r\nexport default {\r\n  components: {\r\n    MyForum,\r\n    MyEssences,\r\n    MyReply\r\n  },\r\n  data () {\r\n    return {\r\n      // 数目\r\n      obj: {\r\n        tz: '',\r\n        jht: '',\r\n        ht: ''\r\n      },\r\n      // tab默认值\r\n      activeName: 'first',\r\n      // 是否隐藏\r\n      count1: false,\r\n      count2: false,\r\n      count3: false\r\n    }\r\n  },\r\n  methods: {\r\n    // 帖子数量\r\n    async getCount () {\r\n      const { data: res } = await this.$http.post('/Invit/Count', {\r\n        userId: ID\r\n      })\r\n      this.obj = res\r\n    },\r\n    // 判断显示与隐藏\r\n    myPost (data) {\r\n      if (data !== 0) {\r\n        this.count1 = true\r\n      }\r\n    },\r\n    myReply (data) {\r\n      if (data !== 0) {\r\n        this.count2 = true\r\n      }\r\n    },\r\n    myEssences (data) {\r\n      if (data !== 0) {\r\n        this.count3 = true\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getCount()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.title {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  margin: 10px 0;\r\n  div {\r\n    margin-right: 30px;\r\n    font-size: 14px;\r\n    color: #aaa;\r\n    label {\r\n      margin-right: 5px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}