IAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,WAAA,KAAA,GAAA,CAAA,GAAA;AACA,OAFA,MAEA;AACA,QAAA,MAAA,CAAA,IAAA,kBAAA,KAAA,GAAA,CAAA,GAAA;AACA;AACA,KAlBA;AAmBA;AACA,IAAA,SApBA,uBAoBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,cAAA,EAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAIA,gBAAA,MAAA,CAAA,UAAA,GAAA,GAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAzBA;AA0BA;AACA,IAAA,kBA3BA,gCA2BA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,GADA,GACA,EADA;AAEA,oBAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,UAAA,EAAA,GAAA,GAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA;AAFA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,IAAA,8BAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,MAAA,EAAA,MAAA,CAAA,MAFA;AAGA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAHA;AAIA,kBAAA,KAAA,EAAA,GAAA,CAAA,KAJA;AAKA,kBAAA,MAAA,EAAA,GAAA,CAAA;AALA,iBAAA,CAHA;;AAAA;AAAA;AAGA,gBAAA,GAHA,yBAGA,IAHA;AAUA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,GAAA;AACA,gBAAA,MAAA,CAAA,eAAA,GAAA,GAAA,CAAA,CAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAvCA;AAwCA;AACA,IAAA,MAzCA,kBAyCA,KAzCA,EAyCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,8BAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,MAAA,EAAA,KAFA;AAGA,kBAAA,QAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAMA,gBAAA,MAAA,CAAA,eAAA,GAAA,GAAA,CAAA,CAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAjDA;AAkDA;AACA,IAAA,OAnDA,mBAmDA,EAnDA,EAmDA;AAAA,kCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,gCADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAFA,OAAA,CADA;AAAA,UACA,IADA,yBACA,IADA;;AAKA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA;AAzDA,GAzBA;AAoFA,EAAA,OApFA,qBAoFA;AACA;AACA,SAAA,kBAAA,GAFA,CAGA;;AACA,SAAA,SAAA;AACA;AAzFA,CAAA","sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <div class=\"w main\">\r\n      <!-- 左侧资讯 -->\r\n      <div class=\"left\">\r\n        <!-- 轮播图 -->\r\n        <div class=\"block\">\r\n          <el-carousel height=\"400px\">\r\n            <el-carousel-item v-if=\"this.obj.pic\">\r\n              <img :src=\"this.obj.pic ? this.obj.pic : ''\" alt=\"\" @click=\"foreign1\">\r\n            </el-carousel-item>\r\n            <el-carousel-item v-for=\"item in swiperList\" :key=\"item.id\">\r\n              <img :src=\"item.coverPic\" alt=\"\" @click=\"details(item.id)\">\r\n            </el-carousel-item>\r\n          </el-carousel>\r\n        </div>\r\n        <div v-for=\"item in informationList\" :key=\"item.id\">\r\n          <div class=\"contain\">\r\n            <div>\r\n              <img :src=\"item.coverPic\" alt=\"\" style=\"width: 200px; height: 100px\">\r\n            </div>\r\n            <div>\r\n              <!-- 标题 -->\r\n              <h3 style=\"cursor: pointer;\" @click=\"details(item.id)\">{{item.title}}</h3>\r\n              <div class=\"user\">\r\n                <!-- 用户 -->\r\n                <div>\r\n                  <span class=\"iconfont icon-yonghu\"></span>\r\n                  <span>{{item.username}}</span>\r\n                </div>\r\n                <!-- 时间 -->\r\n                <div>\r\n                  <span class=\"iconfont icon-shijian\"></span>\r\n                  <span>{{item.issueDate}}</span>\r\n                </div>\r\n                <!-- 信息 -->\r\n                <div>\r\n                  <span class=\"iconfont icon-xinxi\"></span>\r\n                  <span>{{item.comNum}}</span>\r\n                </div>\r\n              </div>\r\n              <!-- 文章详情 -->\r\n              <p>{{item.intro}}</p>\r\n              <!-- 阅读全文 -->\r\n              <div class=\"reading\" @click=\"details(item.id)\">阅读全文>></div>\r\n            </div>\r\n          </div>\r\n          <el-divider></el-divider>\r\n        </div>\r\n        <!-- 分页 -->\r\n        <el-pagination\r\n          background\r\n          layout=\"prev, pager, next\"\r\n          hide-on-single-page\r\n          @current-change=\"change\"\r\n          :page-size=\"pageSize\"\r\n          :current-page=\"pageNo\"\r\n          :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n      <!-- 右侧 -->\r\n      <TyreComponent class=\"right\" :title=\"title\"></TyreComponent>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TyreComponent from './TyreComponent.vue'\r\nexport default {\r\n  props: ['title', 'adv'],\r\n  components: {\r\n    TyreComponent\r\n  },\r\n  data () {\r\n    return {\r\n      // 广告\r\n      advertiseList: [],\r\n      obj: {},\r\n      // 轮播图\r\n      swiperList: [],\r\n      // 资讯条数\r\n      total: 0,\r\n      // 页数\r\n      pageNo: 1,\r\n      // 每一页条数\r\n      pageSize: 10,\r\n      // 列表\r\n      informationList: []\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getAdvertisement()\r\n  },\r\n  methods: {\r\n    // 广告\r\n    async getAdvertisement () {\r\n      const { data: res } = await this.$http.post('/adv/getAdv', {\r\n        name: [{\r\n          name: this.adv,\r\n          area: sessionStorage.getItem('LocationCity')\r\n        }]\r\n      })\r\n      this.advertiseList = res\r\n      this.obj = this.advertiseList[0] || {}\r\n    },\r\n    foreign1 () {\r\n      if (this.obj.url.indexOf('https://') > -1) {\r\n        window.open(`${this.obj.url}`)\r\n      } else {\r\n        window.open(`http://${this.obj.url}`)\r\n      }\r\n    },\r\n    // 获取轮播\r\n    async getSwiper () {\r\n      const { data: res } = await this.$http.post('/Infor/getLb', {\r\n        home: this.title\r\n      })\r\n      this.swiperList = res\r\n    },\r\n    // 获取资讯列表\r\n    async getInformationList () {\r\n      let obj = {}\r\n      if (this.$route.query.formAligin) obj = JSON.parse(this.$route.query.formAligin)\r\n      const { data: res } = await this.$http.post(`/Infor/getInforListBySort`, {\r\n        type: this.title,\r\n        pageNo: this.pageNo,\r\n        pageSize: this.pageSize,\r\n        brand: obj.brand,\r\n        series: obj.series\r\n      })\r\n      this.total = res.num\r\n      this.informationList = res.l\r\n    },\r\n    // 改变页数\r\n    async change (index) {\r\n      const { data: res } = await this.$http.post(`/Infor/getInforListBySort`, {\r\n        type: this.title,\r\n        pageNo: index,\r\n        pageSize: this.pageSize\r\n      })\r\n      this.informationList = res.l\r\n      this.total = res.num\r\n    },\r\n    // 跳转资讯详情页\r\n    details (id) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/automotivelnformation/details',\r\n        query: { id: id }\r\n      })\r\n      window.open(href, '_blank')\r\n    }\r\n  },\r\n  created () {\r\n    // 获取资讯列表\r\n    this.getInformationList()\r\n    // 获取轮播\r\n    this.getSwiper()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.w {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n.selected {\r\n  background-color: #EB3030!important;\r\n}\r\n.main {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  .left {\r\n    width: 800px;\r\n    .el-carousel {\r\n      img {\r\n        height: 100%;\r\n        width: 100%;\r\n      }\r\n    }\r\n    .contain {\r\n      display: flex;\r\n      justify-content: flex-start;\r\n      padding: 20px 0;\r\n      img {\r\n        width: 300px!important;\r\n        height: 150px!important;\r\n        margin-right: 20px;\r\n      }\r\n      .user {\r\n        div {\r\n          float: left;\r\n          margin: 15px 20px 15px 0;\r\n          color: #999999;\r\n          font-size: 15px;\r\n          span {\r\n            margin-right: 5px;\r\n          }\r\n        }\r\n      }\r\n      p {\r\n        font-size: 14px;\r\n        color: #999999;\r\n        width: 430px;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        display: -webkit-box;\r\n        -webkit-box-orient: vertical;\r\n        -webkit-line-clamp: 3;\r\n      }\r\n      .reading {\r\n        color: #0777DD;\r\n        font-size: 14px;\r\n        cursor: pointer;\r\n      }\r\n    }\r\n    // 分页\r\n    .el-pagination {\r\n      margin: 30px 0;\r\n      text-align: center;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}