AA,IAAA,EAAA,GAAA,CAAA,IAFA;AAGA,kBAAA,GAAA,EAAA,GAAA,CAAA;AAHA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAMA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IANA;AAAA;AAAA;AAAA;;AAAA,kDAMA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CANA;;AAAA;AAOA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA,EAPA,CAQA;;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAvBA;AAwBA;AACA,IAAA,QAzBA,oBAyBA,GAzBA,EAyBA;AAAA,kCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,aADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA,CAAA,MAAA,CAAA,GAAA,CAAA;AAAA;AAFA,OAAA,CADA;AAAA,UACA,IADA,yBACA,IADA;;AAKA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,KA/BA;AAgCA;AACA,IAAA,SAjCA,qBAiCA,GAjCA,EAiCA;AACA,UAAA,GAAA,CAAA,IAAA,KAAA,OAAA,EAAA;AAAA,qCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,sBADA;AAEA,UAAA,KAAA,EAAA;AAAA,YAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,SAAA,CADA;AAAA,YACA,IADA,0BACA,IADA;;AAKA,QAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,OANA,MAMA;AAAA,qCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,gBADA;AAEA,UAAA,KAAA,EAAA;AAAA,YAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,SAAA,CADA;AAAA,YACA,KADA,0BACA,IADA;;AAKA,QAAA,MAAA,CAAA,IAAA,CAAA,KAAA,EAAA,QAAA;AACA;AACA;AA/CA;AAnBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-table\r\n      :data=\"tableData\"\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '16px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      border\r\n      style=\"width: 100%\">\r\n      <el-table-column label=\"发布人\" width=\"130\">\r\n        <template slot-scope=\"scope\">\r\n          <img style=\"cursor: pointer;\" @click=\"userMain(scope.row)\" :src=\"scope.row.object.key.headPic\" alt=\"\">\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"标题\">\r\n        <template slot-scope=\"scope\">\r\n          <span style=\"cursor: pointer;\" @click=\"userVideo(scope.row)\">{{scope.row.object.title}}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"mini\" type=\"danger\" v-show=\"scope.row.object.collect\" @click=\"deleted(scope.row)\">取消收藏</el-button>\r\n          <el-button size=\"mini\" type=\"primary\" v-show=\"!scope.row.object.collect\" @click=\"deleted(scope.row)\">收藏</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n      background\r\n      hide-on-single-page\r\n      @current-change=\"changePage\"\r\n      layout=\"prev, pager, next\"\r\n      :page-size=\"this.formLine.pageSize\"\r\n      :current-page=\"this.formLine.pageSize\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookie from 'js-cookie'\r\n// import Follow from '../media/Follow'\r\nexport default {\r\n  // components: {\r\n  //   Follow\r\n  // },\r\n  data () {\r\n    return {\r\n      tableData: [],\r\n      formLine: {\r\n        pageNo: 1,\r\n        pageSize: 5,\r\n        userId: Cookie.get('ID'),\r\n        type: '视频'\r\n      },\r\n      total: 0\r\n    }\r\n  },\r\n  created () {\r\n    this.getTable()\r\n  },\r\n  methods: {\r\n    // 获取表格\r\n    async getTable () {\r\n      const { data: res } = await this.$http.post('/user/colList', this.formLine)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n    },\r\n    // 分页\r\n    changePage (index) {\r\n      this.formLine.pageNo = index\r\n      this.getTable()\r\n    },\r\n    // 取消收藏\r\n    async deleted (row) {\r\n      const res = await this.$http.post('/key/col', {\r\n        userId: Cookie.get('ID'),\r\n        type: row.type,\r\n        nId: row.nId\r\n      })\r\n      if (res.status !== 200 || res.data !== true) return this.$message.error('操作失败')\r\n      this.$message.success('操作成功')\r\n      // this.getTable()\r\n      this.$router.go(0)\r\n    },\r\n    // 自媒体个人主页面\r\n    userMain (row) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/media/user',\r\n        query: { id: row.object.key.id }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 详情\r\n    userVideo (row) {\r\n      if (row.type === '重点号视频') {\r\n        const { href } = this.$router.resolve({\r\n          path: '/media/details/infor',\r\n          query: { id: row.nId }\r\n        })\r\n        window.open(href, '_blank')\r\n      } else {\r\n        const { href } = this.$router.resolve({\r\n          path: '/video/details',\r\n          query: { id: row.nId }\r\n        })\r\n        window.open(href, '_blank')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-pagination {\r\n  margin-top: 10px;\r\n  text-align: center;\r\n}\r\n\r\nimg {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/user"}]}