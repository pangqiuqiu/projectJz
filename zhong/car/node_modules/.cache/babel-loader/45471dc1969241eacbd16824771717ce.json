ACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,qBADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,OAAA,CADA;AAAA,UACA,IADA,yBACA,IADA;;AAKA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,KArBA;AAsBA;AACA,IAAA,YAvBA,wBAuBA,GAvBA,EAuBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,EAAA,EAAA,GAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAAA,sBAIA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IAJA;AAAA;AAAA;AAAA;;AAAA,kDAKA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CALA;;AAAA;AAOA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAhCA;AAiCA;AACA,IAAA,UAlCA,sBAkCA,GAlCA,EAkCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,YADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,OAAA;AAIA;AAvCA;AAjBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-table\r\n      :header-cell-style=\"{ 'text-align': 'center', 'font-size': '16px', 'color': '#000' }\"\r\n      :cell-style=\"{ 'text-align': 'center' }\"\r\n      :data=\"tableData\"\r\n      style=\"width: 100%; border: 0\">\r\n      <el-table-column label=\"标题\" width=\"600\">\r\n        <template slot-scope=\"scope\">\r\n          <span @click=\"postTitle(scope.row)\">{{scope.row.title}}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"postDate\"\r\n        label=\"发布时间\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button v-show=\"scope.row.state !== '精华帖'\" @click=\"handleEdit(scope.row)\" type=\"text\" size=\"small\">编辑</el-button>\r\n          <el-button @click=\"handleDetele(scope.row)\" type=\"text\" size=\"small\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      @current-change=\"changePage\"\r\n      hide-on-single-page\r\n      :current-page=\"this.form.pageNo\"\r\n      :page-size=\"this.form.pageSize\"\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookie from 'js-cookie'\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 表单\r\n      form: {\r\n        userId: '',\r\n        pageSize: 6,\r\n        pageNo: 1\r\n      },\r\n      total: 0,\r\n      tableData: []\r\n    }\r\n  },\r\n  created () {\r\n    // 获取列表数据\r\n    this.getTableList()\r\n  },\r\n  methods: {\r\n    // 获取列表数据\r\n    async getTableList () {\r\n      this.form.userId = Cookie.get('ID')\r\n      const { data: res } = await this.$http.post('/Invit/getMyInvit', this.form)\r\n      this.tableData = res.l\r\n      this.total = res.num\r\n      this.$emit('myPost', this.total)\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.form.pageNo = index\r\n      this.getTableList()\r\n    },\r\n    // 帖子详情页\r\n    postTitle (row) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/forum/post/details',\r\n        query: { id: row.id }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 删除\r\n    async handleDetele (row) {\r\n      const res = await this.$http.post('/Invit/delInvit', {\r\n        id: row.id\r\n      })\r\n      if (res.status !== 200 || res.data !== true) {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.$router.go(0)\r\n    },\r\n    // 编辑\r\n    handleEdit (row) {\r\n      this.$router.push({\r\n        path: '/user/edit',\r\n        query: { id: row.id }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-pagination {\r\n  margin-top: 10px;\r\n  text-align: center;\r\n}\r\nspan {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/user"}]}