Mseeage () {\r\n      window.sessionStorage.setItem('photo', this.message)\r\n      this.getPhoto()\r\n    },\r\n    // 切换标题\r\n    changeIndex (item, index) {\r\n      this.currentIndex = index\r\n      this.formIndex.title = item\r\n      this.getPhoto()\r\n    }\r\n  },\r\n  created () {\r\n    this.getPhoto()\r\n  },\r\n  watch: {\r\n    // message (val) {\r\n    //   if (this.message === '') {\r\n    //     return\r\n    //   }\r\n    //   this.getMseeage()\r\n    // },\r\n    // 监听路由\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.w {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/picture"}]}