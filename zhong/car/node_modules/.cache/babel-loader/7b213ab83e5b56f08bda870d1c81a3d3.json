A,EAAA,gBADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAFA,OAAA,CADA;AAAA,UACA,IADA,yBACA,IADA;;AAKA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,KAbA;AAcA;AACA,IAAA,IAfA,gBAeA,KAfA,EAeA;AACA,UAAA,GAAA,GAAA,KAAA,QAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,KAAA,KAAA;AACA,OAFA,CAAA;AAGA,UAAA,IAAA,GAAA,GAAA,CAAA,CAAA,CAAA,CAAA,IAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AACA,KArBA;AAsBA;AACA,IAAA,WAvBA,yBAuBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,gBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,gBAAA,MAAA,CAAA,OAAA,GAAA,GAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KA1BA;AA2BA;AACA,IAAA,YA5BA,0BA4BA;AACA,WAAA,SAAA,GAAA,MAAA,CAAA,WAAA,IAAA,QAAA,CAAA,eAAA,CAAA,SAAA,IAAA,QAAA,CAAA,IAAA,CAAA,SAAA,CADA,CAEA;;AACA,UAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAAA;AACA,UAAA,UAAA,GAAA,CAAA,WAAA,GAAA,IAAA,IAAA,CAAA,CAJA,CAKA;;AACA,UAAA,KAAA,SAAA,GAAA,GAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,QAAA,GAAA,UAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,GAAA,UAAA,GAAA,IAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,GAAA,GAAA,KAAA,SAAA,GAAA,IAAA;AACA,OAJA,MAIA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,QAAA,GAAA,UAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,GAAA,CAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,GAAA,GAAA,CAAA;AACA;AACA;AA3CA,GAvBA;AAoEA,EAAA,OApEA,qBAoEA;AACA,IAAA,MAAA,CAAA,gBAAA,CAAA,QAAA,EAAA,KAAA,YAAA;AACA,GAtEA;AAuEA,EAAA,aAvEA,2BAuEA;AACA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,QAAA,EAAA,KAAA,YAAA;AACA;AA1EA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app\">\r\n    <!-- 左侧列表 -->\r\n    <ul class=\"left\">\r\n      <li v-for=\"(item,index) in list\" :key=\"index\">\r\n        <div class=\"title\">\r\n          <h3>{{item.label}}</h3>\r\n          <span @click=\"more(item.label)\">查看更多>></span>\r\n        </div>\r\n        <el-divider></el-divider>\r\n        <div class=\"item\" :class=\"index === 2 || index === 5 ? 'current' : ''\" @click=\"details(items.id)\" v-for=\"(items,index) in item.children\" :key=\"items.id\">\r\n          <img :src=\"items.coverPic\" alt=\"\">\r\n          <h5>{{items.title}}</h5>\r\n          <div class=\"subsidiary\">\r\n            <div>\r\n              <span class=\"iconfont icon-shijian\"></span>\r\n              <span>{{items.sc}}</span>\r\n            </div>\r\n            <div>\r\n              <span style=\"font-size: 18px\" class=\"iconfont icon-bofang\"></span>\r\n              <span>{{items.clickNum}}</span>\r\n            </div>\r\n            <div>\r\n              <span class=\"iconfont icon-xinxi\"></span>\r\n              <span>{{items.comNum}}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <!-- 右侧导航 -->\r\n    <ul class=\"right\" ref=\"right\" style=\"position: relative; top: 0; right: 0\">\r\n      <h4>热播排行榜</h4>\r\n      <li v-for=\"(item,i) in hotList\" :key=\"i\" @click=\"details(item.id)\">\r\n        <img :src=\"item.coverPic\" alt=\"\">\r\n        <div>\r\n          <div class=\"title\">{{item.title}}</div>\r\n          <div class=\"small\">\r\n            <i style=\"font-size: 12px\" class=\"iconfont icon-bofang\"></i>\r\n            <span>{{item.clickNum}}</span>\r\n            <i style=\"font-size: 12px\" class=\"iconfont icon-xinxi\"></i>\r\n            <span>{{item.comNum}}</span>\r\n          </div>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      list: [],\r\n      moreList: [\r\n        { label: '原创视频', path: '/video/original' },\r\n        { label: '技术视频', path: '/video/technology' },\r\n        { label: '新车视频', path: '/video/newcar' },\r\n        { label: '车展视频', path: '/video/autoshow' },\r\n        { label: '试驾视频', path: '/video/testdrive' }\r\n      ],\r\n      // 热门视频\r\n      hotList: [],\r\n      // 滚动条高度\r\n      scrollTop: 0\r\n    }\r\n  },\r\n  created () {\r\n    // 获取首页视频列表\r\n    this.getVideoHomeList()\r\n    // 获取热门视频\r\n    this.getHotVideo()\r\n  },\r\n  methods: {\r\n    // 获取首页视频列表\r\n    async getVideoHomeList () {\r\n      const { data: res } = await this.$http.post('/video/homeList')\r\n      this.list = res\r\n    },\r\n    // 跳转详情页\r\n    details (id) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/video/details',\r\n        query: { id: id }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 跳转列表页面\r\n    more (index) {\r\n      const ary = this.moreList.filter(item => {\r\n        return item.label === index\r\n      })\r\n      const path = ary[0].path\r\n      this.$router.push(path)\r\n    },\r\n    // 热门视频\r\n    async getHotVideo () {\r\n      const { data: res } = await this.$http.post('/video/hotRank')\r\n      this.hotList = res\r\n    },\r\n    // 滚动条事件\r\n    handleScroll () {\r\n      this.scrollTop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop\r\n      // 品牌宽度\r\n      let clientWidth = document.body.clientWidth\r\n      let rightLevel = (clientWidth - 1200) / 2\r\n      // 滚动条高度\r\n      if (this.scrollTop > 150) {\r\n        this.$refs.right.style.position = 'absolute'\r\n        this.$refs.right.style.right = rightLevel + 'px'\r\n        this.$refs.right.style.top = this.scrollTop + 'px'\r\n      } else {\r\n        this.$refs.right.style.position = 'relative'\r\n        this.$refs.right.style.right = 0\r\n        this.$refs.right.style.top = 0\r\n      }\r\n    }\r\n  },\r\n  mounted () {\r\n    window.addEventListener('scroll', this.handleScroll)\r\n  },\r\n  beforeDestroy () {\r\n    // 离开该页面需要移除这个监听的事件，不然会报错\r\n    window.removeEventListener('scroll', this.handleScroll)\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.app {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n}\r\n.left {\r\n  width: 975px;\r\n  margin-right: 15px;\r\n  li {\r\n    margin-bottom: 30px;\r\n    overflow: hidden;\r\n    img {\r\n      width: 300px;\r\n      height: 150px;\r\n    }\r\n    .title {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      margin-bottom: 10px;\r\n      span {\r\n        color: #999;\r\n        font-size: 13px;\r\n        cursor: pointer;\r\n      }\r\n    }\r\n    .item {\r\n      float: left;\r\n      margin-top: 20px;\r\n      cursor: pointer;\r\n      margin-right: 30px;\r\n      h5 {\r\n        width: 220px;\r\n        text-align: center;\r\n        white-space: nowrap;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        line-height: 30px;\r\n      }\r\n      .subsidiary {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        padding: 0 10px;\r\n        color: #999999;\r\n        font-size: 12px;\r\n        margin-bottom: 15px;\r\n        span {\r\n          margin-left: 2px;\r\n        }\r\n        .iconfont {\r\n          font-size: 20px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.current {\r\n  margin-right: 0!important;\r\n}\r\n.right {\r\n  box-sizing: border-box;\r\n  width: 220px;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  height: 650px;\r\n  li {\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    margin-top: 15px;\r\n    cursor: pointer;\r\n    img {\r\n      width: 70px;\r\n      margin-right: 15px;\r\n    }\r\n    .title {\r\n      overflow: hidden;\r\n      text-overflow: ellipsis;\r\n      display: -webkit-box;\r\n      -webkit-box-orient: vertical;\r\n      -webkit-line-clamp: 2;\r\n      font-size: 10px;\r\n      color: #333333;\r\n    }\r\n    span {\r\n      font-size: 12px;\r\n      margin-right: 30px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/video"}]}