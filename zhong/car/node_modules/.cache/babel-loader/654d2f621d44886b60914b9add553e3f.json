age.success('修改成功')\r\n      this.$router.push('/user/forum')\r\n    },\r\n    // 编辑后的内容\r\n    getMsg (data) {\r\n      this.form.content = data\r\n    }\r\n  },\r\n  created () {\r\n    this.getPost()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-row {\r\n  display: flex;\r\n  align-items: center;\r\n  padding-top: 20px;\r\n  margin-bottom: 20px;\r\n}\r\n.el-button {\r\n  width: 100%;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/user"}]}