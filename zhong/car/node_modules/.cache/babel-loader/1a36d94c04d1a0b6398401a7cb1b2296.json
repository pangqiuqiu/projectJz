A;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KARA;AASA;AACA,IAAA,OAVA,qBAUA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,wBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,gBAAA,MAAA,CAAA,OAAA,GAAA,GAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAbA;AAcA;AACA,IAAA,MAfA,oBAeA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,KAAA,CAAA,CAAA,MAAA,SADA;AAAA;AAAA;AAAA;;AAEA,gBAAA,MAAA,CAAA,cAAA;;AAFA;AAAA;;AAAA;AAIA,gBAAA,QAJA,GAIA,IAAA,QAAA,EAJA;AAKA,gBAAA,QAAA,CAAA,MAAA,CAAA,YAAA,EAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AALA;AAAA,uBAMA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA,QAAA,EAAA;AACA,kBAAA,OAAA,EAAA;AACA,oCAAA;AADA;AADA,iBAAA,CANA;;AAAA;AAMA,gBAAA,GANA;;AAAA,sBAWA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,SAXA;AAAA;AAAA;AAAA;;AAAA,kDAYA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA,CAZA;;AAAA;AAcA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,cAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA,KAhCA;AAiCA,IAAA,cAjCA,4BAiCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,EAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,MAAA,GAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,UAAA,GAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AAHA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,MAAA,CAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,GAJA;;AAAA,sBAKA,GAAA,CAAA,IAAA,KAAA,IAAA,IAAA,GAAA,CAAA,MAAA,KAAA,GALA;AAAA;AAAA;AAAA;;AAAA,kDAMA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CANA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AA3CA,GAvBA;AAoEA,EAAA,OApEA,qBAoEA;AACA;AACA,SAAA,WAAA,GAFA,CAGA;;AACA,SAAA,OAAA;AACA;AAzEA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h4>个人信息</h4>\r\n    <ul>\r\n      <li>\r\n        <label class=\"title\">用户名:</label>\r\n        <span>{{this.formJava.username}}</span>\r\n      </li>\r\n      <li>\r\n        <label class=\"title\">昵称:</label>\r\n        <el-input size=\"mini\" v-model=\"formJava.nikename\" placeholder=\"请输入昵称\"></el-input>\r\n      </li>\r\n      <li>\r\n        <label class=\"title\">性别:</label>\r\n        <el-radio v-model=\"formJava.sex\" label=\"男\">男</el-radio>\r\n        <el-radio v-model=\"formJava.sex\" label=\"女\">女</el-radio>\r\n        <el-radio v-model=\"formJava.sex\" label=\"未知\">未知</el-radio>\r\n      </li>\r\n      <li>\r\n        <label class=\"title\">生日:</label>\r\n        <div class=\"block\" style=\"display: inline-block\">\r\n          <el-date-picker\r\n            size=\"mini\"\r\n            v-model=\"formJava.birthday\"\r\n            type=\"date\"\r\n            placeholder=\"选择日期\">\r\n          </el-date-picker>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <h4>联系方式</h4>\r\n    <ul>\r\n      <li>\r\n        <label class=\"title\">真实姓名:</label>\r\n        <el-input size=\"mini\" v-model=\"formJava.name\" placeholder=\"请输入姓名\"></el-input>\r\n      </li>\r\n      <li>\r\n        <label class=\"title\">手机号:</label>\r\n        <el-input size=\"mini\" v-model=\"formJava.phone\" placeholder=\"请输入手机号\"></el-input>\r\n      </li>\r\n      <li>\r\n        <label class=\"title\">所在地区:</label>\r\n        <el-cascader size=\"mini\" :props=\"{ value: 'id', label: 'label'}\" v-model=\"value\" :options=\"options\"></el-cascader>\r\n      </li>\r\n      <li>\r\n        <label class=\"title\">街道地址:</label>\r\n        <el-input size=\"mini\" v-model=\"formJava.street\" placeholder=\"请输入地址\"></el-input>\r\n      </li>\r\n    </ul>\r\n    <h4>头像设置</h4>\r\n    <img :src=\"formJava.headPic\" alt=\"\" style=\"margin-right: 50px\">\r\n    <input type=\"file\" id=\"saveImage\" name=\"myphoto\" accept=\"image/png,image/gif,image/jpeg\" ref=\"new_image\">\r\n    <!-- 提交 -->\r\n    <div>\r\n      <el-button type=\"primary\" @click=\"submit\" style=\"margin-top: 10px\">保存资料</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookie from 'js-cookie'\r\nconst ID = Cookie.get('ID')\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 省份城市\r\n      options: [],\r\n      value: [],\r\n      formJava: {\r\n        id: '',\r\n        username: '',\r\n        nikename: '',\r\n        birthday: '',\r\n        provinceId: '',\r\n        cityId: '',\r\n        province: '',\r\n        city: '',\r\n        sex: '',\r\n        name: '',\r\n        phone: '',\r\n        street: '',\r\n        headPic: ''\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    // 获取用户信息\r\n    async getUserinfo () {\r\n      const { data: res } = await this.$http.post('/user/getUser', {\r\n        id: ID\r\n      })\r\n      this.formJava = res\r\n      this.value = [this.formJava.provinceId, this.formJava.cityId]\r\n    },\r\n    // 省份城市\r\n    async getCity () {\r\n      const { data: res } = await this.$http.post('/area/showProvinceCity')\r\n      this.options = res\r\n    },\r\n    // 提交\r\n    async submit () {\r\n      if (this.$refs.new_image.files[0] === undefined) {\r\n        this.submitUserInfo()\r\n      } else {\r\n        const formData = new FormData()\r\n        formData.append('image_data', this.$refs.new_image.files[0])\r\n        const res = await this.$http.post('/upload/upload2', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        })\r\n        if (res.status !== 200 || res.data === '图片上传失败！') {\r\n          return this.$message.success('头像上传失败')\r\n        }\r\n        this.formJava.headPic = res.data\r\n        this.submitUserInfo()\r\n      }\r\n    },\r\n    async submitUserInfo () {\r\n      this.formJava.id = ID\r\n      this.formJava.cityId = this.value[1]\r\n      this.formJava.provinceId = this.value[0]\r\n      const res = await this.$http.post('/user/updateUser', this.formJava)\r\n      if (res.data !== true || res.status !== 200) {\r\n        return this.$message.error('保存失败')\r\n      }\r\n      this.$message.success('保存成功')\r\n      this.$router.go(0)\r\n    }\r\n  },\r\n  created () {\r\n    // 用户信息\r\n    this.getUserinfo()\r\n    // 省份\r\n    this.getCity()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.w {\r\n  width: 1100px;\r\n  margin: 0 auto;\r\n}\r\nh4 {\r\n  margin-bottom: 13px;\r\n}\r\nul {\r\n  li {\r\n    margin-bottom: 15px;\r\n    .title {\r\n      display: inline-block;\r\n      text-align: right;\r\n      padding-right: 10px;\r\n      width: 80px;\r\n    }\r\n  }\r\n}\r\nimg {\r\n  width: 120px;\r\n  height: 120px;\r\n}\r\n.el-input, .el-cascader {\r\n  width: 300px;\r\n}\r\n.el-select {\r\n  margin-right: 20px;\r\n}\r\n.el-button {\r\n  width: 400px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/user"}]}