,EADA;AAEA,gBAAA,IAFA,GAEA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAFA;AAAA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,OAAA,EAAA,EADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,MAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CAHA;;AAAA;AAAA;AAGA,gBAAA,GAHA,yBAGA,IAHA;AAQA,gBAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,CAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA1BA;AA2BA;AACA,IAAA,UA5BA,sBA4BA,KA5BA,EA4BA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,OAAA;AACA,KA/BA;AAgCA;AACA,IAAA,IAjCA,gBAiCA,EAjCA,EAiCA;AAAA,kCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,qBADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAFA,OAAA,CADA;AAAA,UACA,IADA,yBACA,IADA;;AAKA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,KAvCA;AAwCA;AACA,IAAA,GAzCA,iBAyCA;AACA,UAAA,KAAA,GAAA,MAAA,CAAA,GAAA,CAAA,OAAA,CAAA;AACA,UAAA,EAAA,GAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA;;AACA,UAAA,CAAA,KAAA,IAAA,CAAA,EAAA,EAAA;AACA,eAAA,KAAA,QAAA,CAAA,KAAA,CAAA,OAAA,CAAA;AACA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,qBAAA;AACA;AAhDA,GAnBA;AAqEA,EAAA,OArEA,qBAqEA;AACA;AACA,SAAA,OAAA;AACA;AAxEA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"w\">\r\n      <img :src=\"this.obj1.pic ? this.obj1.pic : ''\" alt=\"\" @click=\"foreign1\">\r\n    </div>\r\n    <div class=\"w top\">\r\n      <h2>{{this.title}}</h2>\r\n      <el-button type=\"primary\" @click=\"btn\">我要发帖</el-button>\r\n    </div>\r\n    <!-- 列表 -->\r\n    <el-card class=\"box-card w\" shadow=\"never\" style=\"min-height: 350px\">\r\n      <ul>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"16\"><h3>帖子标题</h3></el-col>\r\n          <el-col :span=\"5\"><h3>发帖时间</h3></el-col>\r\n          <el-col :span=\"3\"><h3>回复数</h3></el-col>\r\n        </el-row>\r\n        <li v-for=\"item in postList\" :key=\"item.id\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"16\"><div class=\"title\" @click=\"jump(item.id)\">{{item.title}}</div></el-col>\r\n            <el-col :span=\"5\"><div class=\"time\">{{item.postDate}}</div></el-col>\r\n            <el-col :span=\"3\"><div class=\"reply\">{{item.replyNum}}</div></el-col>\r\n          </el-row>\r\n        </li>\r\n      </ul>\r\n      <!-- 分页 -->\r\n      <el-pagination\r\n        background\r\n        hide-on-single-page\r\n        @current-change=\"changePage\"\r\n        layout=\"prev, pager, next\"\r\n        :page-size=\"pageSize\"\r\n        :current-page=\"pageNo\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookie from 'js-cookie'\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 广告\r\n      advertiseList: [],\r\n      obj1: {},\r\n      // 标题\r\n      title: '',\r\n      // 帖子列表\r\n      postList: [],\r\n      // 分页\r\n      total: 0,\r\n      pageNo: 1,\r\n      pageSize: 20\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getAdvertisement()\r\n  },\r\n  methods: {\r\n    // 广告\r\n    async getAdvertisement () {\r\n      const { data: res } = await this.$http.post('/adv/getAdv', {\r\n        name: [{\r\n          name: '帖子列表顶部通栏', area: sessionStorage.getItem('LocationProvince')\r\n        }]\r\n      })\r\n      this.advertiseList = res\r\n      this.obj1 = this.advertiseList[0] || {}\r\n    },\r\n    foreign1 () {\r\n      window.open(`http://${this.obj1.url}`)\r\n    },\r\n    // 帖子列表\r\n    async getPost () {\r\n      const id = this.$route.query.id\r\n      const name = this.$route.query.name\r\n      const { data: res } = await this.$http.post('/Invit/getInvitL', {\r\n        forumId: id,\r\n        pageSize: this.pageSize,\r\n        pageNo: this.pageNo\r\n      })\r\n      this.title = name\r\n      this.postList = res.l\r\n      this.total = res.num\r\n    },\r\n    // 改变分页\r\n    changePage (index) {\r\n      this.pageNo = index\r\n      this.getPost()\r\n    },\r\n    // 详情页\r\n    jump (id) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/forum/post/details',\r\n        query: { id: id }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 发帖\r\n    btn () {\r\n      const token = Cookie.get('token')\r\n      const ID = Cookie.get('ID')\r\n      if (!token || !ID) {\r\n        return this.$message.error('请先登录哦')\r\n      }\r\n      this.$router.push('/forum/post/release')\r\n    }\r\n  },\r\n  created () {\r\n    // 获取帖子列表\r\n    this.getPost()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.w {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n.top {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  h2 {\r\n    height: 60px;\r\n    line-height: 60px;\r\n  }\r\n  .el-button {\r\n    margin: 7px 0;\r\n  }\r\n}\r\nli {\r\n  .title {\r\n    font-size: 20px;\r\n  }\r\n  .title:hover {\r\n    cursor: pointer;\r\n    color: rgb(43, 121, 255);\r\n  }\r\n}\r\n.el-row {\r\n  height: 50px;\r\n}\r\n// 分页\r\n.el-pagination {\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/forum/post"}]}