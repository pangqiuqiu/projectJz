pe=\"primary\" size=\"mini\" v-show=\"this.demo === false\" @click=\"clickFollow\">+ 关注</el-button>\r\n    <el-button type=\"success\" size=\"mini\" v-show=\"this.demo === true\" @click=\"clickFollow\">已关注</el-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookie from 'js-cookie'\r\nexport default {\r\n  props: ['btn', 'others'],\r\n  data () {\r\n    return {\r\n      // 按钮状态\r\n      demo: this.btn\r\n    }\r\n  },\r\n  methods: {\r\n    async clickFollow () {\r\n      if (Cookie.get('ID') === undefined) return this.$message.error('请先登录哦')\r\n      const res = await this.$http.post('/key/gz', {\r\n        keyId: this.others,\r\n        userId: Cookie.get('ID')\r\n      })\r\n      if (res.status !== 200 || res.data !== true) {\r\n        return this.$message.error('操作失败')\r\n      }\r\n      this.$message.success('操作成功')\r\n      this.demo = !this.demo\r\n    }\r\n  },\r\n  created () {\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-button {\r\n  display: block;\r\n  margin: 0 auto;\r\n  padding: 5px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/media"}]}