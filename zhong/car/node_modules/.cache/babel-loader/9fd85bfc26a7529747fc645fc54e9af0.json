AAAA,kDACA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA,CADA;;AAAA;AAAA,sBAEA,MAAA,CAAA,QAAA,CAAA,MAAA,KAAA,CAFA;AAAA;AAAA;AAAA;;AAAA,kDAEA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,WAAA,CAFA;;AAAA;AAAA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAAA,EAAA;AACA,kBAAA,MAAA,EAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CADA;AAEA,kBAAA,IAAA,EAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,aAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAHA,iBAAA,CAHA;;AAAA;AAGA,gBAAA,GAHA;;AAAA,sBAQA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IARA;AAAA;AAAA;AAAA;;AAAA,kDAQA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CARA;;AAAA;AASA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,EAAA;;AACA,gBAAA,MAAA,CAAA,UAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KA9DA;AA+DA;AACA,IAAA,UAhEA,sBAgEA,KAhEA,EAgEA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,UAAA;AACA,KAnEA;AAoEA;AACA,IAAA,QArEA,oBAqEA,EArEA,EAqEA;AAAA,kCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,aADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAFA,OAAA,CADA;AAAA,UACA,IADA,yBACA,IADA;;AAKA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,KA3EA;AA4EA;AACA,IAAA,WA7EA,yBA6EA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,CAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,CAAA,GAAA,CAAA,OAAA,CADA;AAAA;AAAA;AAAA;;AAAA,kDACA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA,CADA;;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,EAAA;AACA,kBAAA,MAAA,EAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CADA;AAEA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAAA,sBAMA,GAAA,CAAA,MAAA,KAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,IANA;AAAA;AAAA;AAAA;;AAAA,kDAOA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAPA;;AAAA;AASA,gBAAA,MAAA,CAAA,QAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAvFA;AAwFA;AACA,IAAA,WAzFA,yBAyFA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,kBAAA,QAAA,EAAA,CAFA;AAGA,kBAAA,QAAA,EAAA;AAHA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAMA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAhGA;AAiGA;AACA,IAAA,UAlGA,sBAkGA,EAlGA,EAkGA,IAlGA,EAkGA;AACA,UAAA,IAAA,KAAA,OAAA,EAAA;AAAA,qCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,sBADA;AAEA,UAAA,KAAA,EAAA;AAAA,YAAA,EAAA,EAAA;AAAA;AAFA,SAAA,CADA;AAAA,YACA,IADA,0BACA,IADA;;AAKA,QAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,OANA,MAMA;AAAA,qCACA,KAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,sBADA;AAEA,UAAA,KAAA,EAAA;AAAA,YAAA,EAAA,EAAA;AAAA;AAFA,SAAA,CADA;AAAA,YACA,KADA,0BACA,IADA;;AAKA,QAAA,MAAA,CAAA,IAAA,CAAA,KAAA,EAAA,QAAA;AACA;AACA;AAhHA,GAjCA;AAmJA,EAAA,OAnJA,qBAmJA;AACA;AACA;AACA;AACA,SAAA,QAAA,GAJA,CAKA;;AACA,SAAA,UAAA;AACA,GA1JA;AA2JA,EAAA,OA3JA,qBA2JA;AACA,IAAA,QAAA,CAAA,aAAA,CAAA,MAAA,EAAA,YAAA,CAAA,OAAA,EAAA,2BAAA;AACA,GA7JA;AA8JA,EAAA,aA9JA,2BA8JA;AACA,IAAA,QAAA,CAAA,aAAA,CAAA,MAAA,EAAA,eAAA,CAAA,OAAA;AACA;AAhKA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 广告位 -->\r\n    <!-- <header>\r\n      <img :src=\"this.advObj ? this.advObj.pic : ''\" alt=\"\" @click=\"foreign\">\r\n    </header> -->\r\n    <!-- 主体区域 -->\r\n    <div class=\"all_infor\">\r\n      <!-- 左侧 -->\r\n      <div class=\"left\">\r\n        <!-- 资讯内容 -->\r\n        <div class=\"infor\">\r\n          <h2>{{this.inforObj.title}}</h2>\r\n          <div class=\"vice\">\r\n            <span>{{this.inforObj.label}}</span>\r\n            <span class=\"time\">{{this.inforObj.issueDate}}</span>\r\n            <span>收藏 <img @click=\"collectInfor\" :src=\"this.inforObj.collect ? require('../../../assets/media/yes.png'): require('../../../assets/media/no.png')\" alt=\"\"></span>\r\n          </div>\r\n          <div v-html=\"this.inforObj.text\" id=\"image\" style=\"width: 100%\"></div>\r\n          <div style=\"height: 10px\"></div>\r\n        </div>\r\n        <!-- 评论 -->\r\n        <footer>\r\n          <!-- 发布评论 -->\r\n          <div class=\"title\">\r\n            <h3>车友评论</h3>\r\n            <div>注册 / 登录</div>\r\n          </div>\r\n          <div class=\"myComment\">\r\n            <el-input v-model=\"textarea\" placeholder=\"请输入评论内容\"></el-input>\r\n            <el-button type=\"primary\" @click=\"submit\">提交评论</el-button>\r\n          </div>\r\n          <!-- 评论内容 -->\r\n          <ul>\r\n            <li v-for=\"(item,index) in commentList\" :key=\"index\">\r\n              <!-- 头像 -->\r\n              <img :src=\"item.headPic\" alt=\"\">\r\n              <div class=\"right\">\r\n                <!-- 姓名 时间 -->\r\n                <div>\r\n                  <span>{{item.nikename}}</span>\r\n                  <span>{{item.issueDate}}</span>\r\n                </div>\r\n                <!-- 评论正文 -->\r\n                <p>{{item.text}}</p>\r\n              </div>\r\n            </li>\r\n          </ul>\r\n          <!-- 分页 -->\r\n          <el-pagination\r\n            hide-on-single-page\r\n            background\r\n            @current-change=\"changePage\"\r\n            layout=\"prev, pager, next\"\r\n            :page-size=\"pageSize\"\r\n            :current-page=\"pageNo\"\r\n            :total=\"total\">\r\n          </el-pagination>\r\n        </footer>\r\n      </div>\r\n      <!-- 右侧 -->\r\n      <div class=\"right\">\r\n        <!-- 个人信息 -->\r\n        <div class=\"user\">\r\n          <div class=\"image\" @click=\"userMain(user.id)\"><img :src=\"this.user.headPic\" alt=\"\"></div>\r\n          <h3>{{this.user.name}}</h3>\r\n          <p>{{this.user.intro}}</p>\r\n          <div class=\"author\">\r\n            <label>粉丝: </label>\r\n            <span>{{this.user.fansNum}}</span>\r\n            <label>作品: </label>\r\n            <span>{{this.user.worksNum}}</span>\r\n          </div>\r\n          <div class=\"btn\">\r\n            <el-button type=\"primary\" @click=\"clickFollow\" size=\"mini\" v-show=\"this.user.bgz === false\">+ 关注</el-button>\r\n            <el-button type=\"success\" style=\"margin-left: 0;\" @click=\"clickFollow\" size=\"mini\" v-show=\"this.user.bgz === true\">已关注</el-button>\r\n          </div>\r\n        </div>\r\n        <el-divider></el-divider>\r\n        <!-- 相关作品 -->\r\n        <div class=\"works\">\r\n          <h4>他的作品</h4>\r\n          <ul>\r\n            <li @click=\"clickWorks(item.id, item.type)\" v-for=\"(item,index) in userWorks\" :key=\"index\">\r\n              <img :src=\"item.object.coverPic\" alt=\"\">\r\n              <div>{{item.object.title}}</div>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n        <!-- 热门车型 -->\r\n        <Weekhot></Weekhot>\r\n        <!-- 热门内容 -->\r\n        <Hotcontent></Hotcontent>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Weekhot from '../../../components/media/Weekhot'\r\nimport Hotcontent from '../../../components/media/Hotcontent'\r\nimport Cookie from 'js-cookie'\r\nexport default {\r\n  components: {\r\n    Weekhot,\r\n    Hotcontent\r\n  },\r\n  data () {\r\n    return {\r\n      // // 广告位\r\n      // advObj: {},\r\n      // 资讯详情\r\n      inforObj: {\r\n        title: '',\r\n        text: '',\r\n        label: '',\r\n        issueDate: '',\r\n        collect: '',\r\n        nId: '',\r\n        keyId: ''\r\n      },\r\n      // 用户信息\r\n      user: {},\r\n      isfollow: '',\r\n      // 个人作品推荐\r\n      userWorks: [],\r\n      // 评论列表\r\n      commentList: [],\r\n      textarea: '',\r\n      // 分页\r\n      pageSize: 10,\r\n      pageNo: 1,\r\n      total: 0 // 总数\r\n    }\r\n  },\r\n  methods: {\r\n    // // 广告位\r\n    // async getAdv () {\r\n    //   const { data: res } = await this.$http.post('/adv/getAdv', {\r\n    //     name: ['自媒体文章详情']\r\n    //   })\r\n    //   this.advObj = res[0]\r\n    // },\r\n    // foreign () {\r\n    //   window.open(`http://${this.advObj.url}`)\r\n    // },\r\n    // 资讯详情\r\n    async getInfor () {\r\n      const { data: res } = await this.$http.post('/key/getKeyN', {\r\n        id: this.$route.query.id,\r\n        userId: Cookie.get('ID') || ''\r\n      })\r\n      this.inforObj.issueDate = res.object.issueDate\r\n      this.inforObj.label = res.label\r\n      this.inforObj.title = res.object.title\r\n      this.inforObj.text = res.object.text\r\n      this.inforObj.collect = res.collect\r\n      this.inforObj.nId = res.nId\r\n      this.inforObj.keyId = res.keyId\r\n      this.user = res.key\r\n      // 获取个作品\r\n      this.getUserWork()\r\n    },\r\n    // 点击收藏\r\n    async collectInfor () {\r\n      if (!Cookie.get('ID') || !Cookie.get('token')) return this.$message.error('请先登录哦')\r\n      const res = await this.$http.post('/key/col', {\r\n        userId: Cookie.get('ID'),\r\n        nId: this.$route.query.id,\r\n        type: '重点号资讯'\r\n      })\r\n      if (res.data !== true || res.status !== 200) return this.$message.error('操作失败')\r\n      this.getInfor()\r\n    },\r\n    // 评论列表\r\n    async getComment () {\r\n      const { data: res } = await this.$http.post('/key/showCom', {\r\n        pageSize: 10,\r\n        pageNo: this.pageNo,\r\n        id: this.$route.query.id\r\n      })\r\n      this.commentList = res.l\r\n      this.total = res.num\r\n    },\r\n    // 发布评论\r\n    async submit () {\r\n      if (!Cookie.get('ID') || !Cookie.get('token')) return this.$message.error('请先登录哦')\r\n      if (this.textarea.length === 0) return this.$message.error('评论内容不可以为空')\r\n      const res = await this.$http.post('/Infor/addComment', {\r\n        userId: Cookie.get('ID'),\r\n        text: this.textarea,\r\n        informationId: this.inforObj.nId\r\n      })\r\n      if (res.status !== 200 || res.data !== true) return this.$message.error('评论发布失败')\r\n      this.$message.success('评论发布成功')\r\n      this.textarea = ''\r\n      this.getComment()\r\n    },\r\n    // 分页\r\n    changePage (index) {\r\n      this.pageNo = index\r\n      this.getComment()\r\n    },\r\n    // 点击头像跳转个人主页\r\n    userMain (id) {\r\n      const { href } = this.$router.resolve({\r\n        path: '/media/user',\r\n        query: { id: id }\r\n      })\r\n      window.open(href, '_blank')\r\n    },\r\n    // 关注当前重点号\r\n    async clickFollow () {\r\n      if (!Cookie.get('ID') || !Cookie.get('token')) return this.$message.error('请先登录哦')\r\n      const res = await this.$http.post('/key/gz', {\r\n        userId: Cookie.get('ID'),\r\n        keyId: this.inforObj.keyId\r\n      })\r\n      if (res.status !== 200 || res.data !== true) {\r\n        return this.$message.error('操作失败')\r\n      }\r\n      this.getInfor()\r\n    },\r\n    // 获取个人作品\r\n    async getUserWork () {\r\n      const { data: res } = await this.$http.post('/key/KeyNList', {\r\n        keyId: this.inforObj.keyId,\r\n        pageSize: 8,\r\n        clickNum: 0\r\n      })\r\n      this.userWorks = res\r\n    },\r\n    // 点击个人作品\r\n    clickWorks (id, type) {\r\n      if (type === '重点号资讯') {\r\n        const { href } = this.$router.resolve({\r\n          path: '/media/details/infor',\r\n          query: { id: id }\r\n        })\r\n        window.open(href, '_blank')\r\n      } else {\r\n        const { href } = this.$router.resolve({\r\n          path: '/media/details/video',\r\n          query: { id: id }\r\n        })\r\n        window.open(href, '_blank')\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    // 广告位\r\n    // this.getAdv()\r\n    // 资讯内容\r\n    this.getInfor()\r\n    // 评论\r\n    this.getComment()\r\n  },\r\n  mounted () {\r\n    document.querySelector('body').setAttribute('style', 'background-color: #f5f5f5')\r\n  },\r\n  beforeDestroy () {\r\n    document.querySelector('body').removeAttribute('style')\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 广告\r\n// header {\r\n//   height: 100px;\r\n//   background-color: #fff;\r\n//   cursor: pointer;\r\n// }\r\n\r\n// 文章详情\r\n.all_infor {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  margin-top: 20px;\r\n  .left {\r\n    width: 830px;\r\n    margin-right: 20px;\r\n    .infor {\r\n      background-color: #fff;\r\n      padding: 0 20px;\r\n      h2 {\r\n        text-align: center;\r\n        height: 60px;\r\n        line-height: 60px;\r\n      }\r\n      .vice {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        font-size: 13px;\r\n        color: #999;\r\n        img {\r\n          cursor: pointer;\r\n          width: 13px;\r\n          height: 13px;\r\n        }\r\n      }\r\n    }\r\n    footer {\r\n      .title {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        height: 60px;\r\n        line-height: 60px;\r\n        h3 {\r\n          color: rgb(43, 121, 255);\r\n        }\r\n      }\r\n      .myComment {\r\n        width: 830px;\r\n        display: flex;\r\n        justify-content: flex-start;\r\n        margin-bottom: 20px;\r\n      }\r\n      ul {\r\n        width: 830px;\r\n        li {\r\n          display: flex;\r\n          justify-content: flex-start;\r\n          border-bottom: 1px dashed #ddd;\r\n          margin-top: 10px;\r\n          img {\r\n            width: 40px;\r\n            height: 40px;\r\n            border-radius: 50%;\r\n            margin-right: 20px;\r\n          }\r\n          span {\r\n            color: #999999;\r\n            font-size: 12px;\r\n            margin-right: 20px;\r\n          }\r\n          p {\r\n            color: #333333;\r\n            font-size: 12px;\r\n            width: 760px;\r\n          }\r\n        }\r\n      }\r\n      .el-pagination {\r\n        width: 830px;\r\n        text-align: center;\r\n        margin: 15px 0;\r\n      }\r\n    }\r\n  }\r\n  .right {\r\n    width: 350px;\r\n    // 个人\r\n    .user {\r\n      background-color: #fff;\r\n      padding: 15px 0;\r\n      .image {\r\n        padding: 15px 0;\r\n        text-align: center;\r\n        cursor: pointer;\r\n        img {\r\n          width: 70px;\r\n          height: 70px;\r\n          border-radius: 50%;\r\n        }\r\n      }\r\n      h3 {\r\n        text-align: center;\r\n      }\r\n      p {\r\n        text-align: center;\r\n        font-size: 13px;\r\n        color: #999;\r\n      }\r\n      .author {\r\n        text-align: center;\r\n        font-siz