A,CAAA;AAHA,SAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,cAAA;AACA,WAHA,MAGA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA;AACA,SAXA;AAYA,OAdA;AAeA,KA5CA;AA6CA;AACA,IAAA,SA9CA,uBA8CA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,WAAA;AACA,KAhDA;AAiDA;AACA,IAAA,IAlDA,kBAkDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,YAAA,CAAA,MAAA,KAAA,EAAA,IAAA,MAAA,CAAA,YAAA,CAAA,MAAA,CAAA,MAAA,KAAA,EAAA,IAAA,MAAA,CAAA,MAAA,KAAA,MAAA,CAAA,MADA;AAAA;AAAA;AAAA;;AAAA,kDACA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA,CADA;;AAAA;AAAA,sBAEA,MAAA,CAAA,QAAA,GAAA,CAFA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,EAAA;AACA,gBAAA,KAJA,GAIA,WAAA,CAAA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,sBAAA,MAAA,CAAA,QAAA,GAAA,CAAA,EAAA;AACA,oBAAA,aAAA,CAAA,KAAA,CAAA;AACA,oBAAA,MAAA,CAAA,QAAA,GAAA,CAAA;AACA;AACA,iBANA,EAMA,IANA,CAJA;AAAA;AAAA,uBAWA,MAAA,CAAA,KAAA,CAAA,IAAA,mBAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,YAAA,CAAA;AADA,iBAAA,CAXA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAhEA;AAiEA;AACA,IAAA,IAlEA,kBAkEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,cAAA;AACA,KApEA;AAqEA;AACA,IAAA,MAtEA,oBAsEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA;AAxEA;AAhEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"card\">\r\n      <el-card class=\"w\" :body-style=\"{ padding: '0px', height: '100%' }\">\r\n        <div class=\"left\">\r\n          <img src=\"../../assets/login/1.png\" alt=\"\">\r\n        </div>\r\n        <div class=\"right\">\r\n          <!-- 欢迎 -->\r\n          <h3>欢迎注册 重点汽车网 会员</h3>\r\n          <!-- 表单 -->\r\n          <el-form :model=\"registerForm\" status-icon :rules=\"registerRules\" ref=\"ruleFormRef\" class=\"demo-ruleForm\">\r\n            <!-- 手机号 -->\r\n            <el-form-item prop=\"mobile\">\r\n              <el-input placeholder=\"请输入手机号\" type=\"text\" @change=\"change\" v-model=\"registerForm.mobile\" autocomplete=\"off\" @blur=\"judgeMobile\"></el-input>\r\n            </el-form-item>\r\n            <!-- 验证码 -->\r\n            <div class=\"code\">\r\n              <el-form-item>\r\n                <el-input type=\"text\" v-model=\"writen\" autocomplete=\"off\" placeholder=\"请输入随机码\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item class=\"random\" style=\"width: 88px\">\r\n                <span @click=\"this.getRandom\">{{random}}</span>\r\n              </el-form-item>\r\n            </div>\r\n            <!-- 密码 -->\r\n            <el-form-item prop=\"psd1\">\r\n              <el-input placeholder=\"请输入密码\" type=\"password\" v-model=\"registerForm.psd1\" autocomplete=\"off\" :disabled=\"isDisabled\"></el-input>\r\n            </el-form-item>\r\n            <!-- 确认密码 -->\r\n            <el-form-item prop=\"psd2\">\r\n              <el-input placeholder=\"请再次输入密码\" type=\"password\" v-model=\"registerForm.psd2\" autocomplete=\"off\" :disabled=\"isDisabled\"></el-input>\r\n            </el-form-item>\r\n            <!-- 验证码 -->\r\n            <div class=\"code\">\r\n              <el-form-item prop=\"code\">\r\n                <el-input placeholder=\"请输入验证码\" type=\"type\" v-model=\"registerForm.code\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-button type=\"warning\" @click=\"code()\" :disabled=\"isDisabled\">{{!codetime?'发送验证码':codetime + 's后重新获取'}}</el-button>\r\n              </el-form-item>\r\n            </div>\r\n            <!-- 按钮区域 -->\r\n            <el-form-item class=\"btn\">\r\n              <el-button type=\"primary\" @click=\"back\" style=\"margin-right: 10px;\">返回登录</el-button>\r\n              <el-button type=\"primary\" @click=\"submitForm()\" style=\"margin-right: 10px;\">注册</el-button>\r\n              <el-button @click=\"resetForm\">重置</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n    <!-- 返回首页 -->\r\n    <div class=\"back\" @click=\"backed\">\r\n      <img src=\"../../assets/login/fanhui.png\" alt=\"\"> 返回首页\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    // 验证手机号正则\r\n    let mobile = /^[1]([3-9])[0-9]{9}$/\r\n    let isMobile = (rule, value, callback) => {\r\n      if (!mobile.test(value)) {\r\n        return callback(new Error('请输入正确的手机号'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    // 验证密码的正则\r\n    let psd = /^[\\d\\w]{6,12}$/\r\n    let isPsd = (rule, value, callback) => {\r\n      if (!psd.test(value)) {\r\n        return callback(new Error('请输入6~12以内数字、字母或组合密码'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    return {\r\n      isDisabled: false,\r\n      // 验证码倒计时\r\n      codetime: 0,\r\n      // 这是注册表单的数据绑定对象\r\n      registerForm: {\r\n        mobile: '', // 手机号\r\n        psd1: '', // 密码\r\n        psd2: '', // 确认密码\r\n        code: '' // 验证码\r\n      },\r\n      random: '', // 图片随机数\r\n      writen: '', // 输入的随机数\r\n      // 这是表单的验证规则对象\r\n      registerRules: {\r\n        // 验证手机号是否合法\r\n        mobile: [\r\n          { required: true, message: '请输入手机号', trigger: 'blur' },\r\n          { validator: isMobile }\r\n        ],\r\n        // 验证密码是否合法\r\n        psd1: [\r\n          { required: true, message: '请输入登录密码', trigger: 'blur' },\r\n          { min: 6, max: 15, message: '长度在 6 到 15 个字符', trigger: 'blur' },\r\n          { validator: isPsd }\r\n        ],\r\n        // 确认密码是否合法\r\n        psd2: [\r\n          { required: true, message: '请确认登录密码', trigger: 'blur' },\r\n          { min: 6, max: 15, message: '长度在 6 到 15 个字符', trigger: 'blur' },\r\n          { validator: isPsd }\r\n        ],\r\n        // 验证验证码是否合法\r\n        code: [\r\n          { required: true, message: '请输入验证码', trigger: 'blur' },\r\n          { min: 4, max: 4, message: '请输入正确的验证码', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    // 获取随机数\r\n    this.getRandom()\r\n  },\r\n  methods: {\r\n    // 获取随机数\r\n    getRandom () {\r\n      let chars = '0123456789'\r\n      let maxPos = chars.length\r\n      let code = ''\r\n      for (let i = 0; i < 6; i++) {\r\n        code += chars.charAt(Math.floor(Math.random() * maxPos))\r\n      }\r\n      this.random = code\r\n    },\r\n    // 判断手机号是否已经注册\r\n    async judgeMobile () {\r\n      const res = await this.$http.post(`/user/phoneYz`, {\r\n        phone: this.registerForm.mobile\r\n      })\r\n      if (res.data === false) {\r\n        this.$message.error('手机号已被注册')\r\n        this.isDisabled = true\r\n      } else {\r\n        this.isDisabled = false\r\n      }\r\n    },\r\n    change () {\r\n      this.judgeMobile()\r\n    },\r\n    // 点击按钮触发注册请求\r\n    submitForm () {\r\n      if (this.registerForm.psd1 !== this.registerForm.psd2) return this.$message.error('两次密码不一致')\r\n      this.$refs.ruleFormRef.validate((valid) => {\r\n        if (!valid) return\r\n        this.$http.post(`/user/addUser`, {\r\n          code: this.registerForm.code,\r\n          phone: this.registerForm.mobile,\r\n          password: this.registerForm.psd1\r\n        }).then((res) => {\r\n          if (res.data === true) {\r\n            this.$message.success('注册成功')\r\n            this.$router.push('/login/login')\r\n          } else {\r\n            this.$message.error('短信验证码错误')\r\n          }\r\n        })\r\n      })\r\n    },\r\n    // 点击按钮触发重置按钮\r\n    resetForm () {\r\n      this.$refs.ruleFormRef.resetFields()\r\n    },\r\n    // 获取短信验证码\r\n    async code () {\r\n      if (this.registerForm.mobile === '' || this.registerForm.mobile.length !== 11 || this.writen !== this.random) return this.$message.error('请输入手机号和随机码')\r\n      if (this.codetime > 0) return\r\n      this.codetime = 60\r\n      let timer = setInterval(() => {\r\n        this.codetime--\r\n        if (this.codetime < 1) {\r\n          clearInterval(timer)\r\n          this.codetime = 0\r\n        }\r\n      }, 1000)\r\n      await this.$http.post(`/user/sendCode`, {\r\n        phone: this.registerForm.mobile\r\n      })\r\n    },\r\n    // 返回登录页面\r\n    back () {\r\n      this.$router.push('/login/login')\r\n    },\r\n    // 返回首页\r\n    backed () {\r\n      this.$router.push('/home')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\nhtml, body {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n.w {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n.container {\r\n  height: 100%;\r\n  .card {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    height: 100%;\r\n  }\r\n  .el-card {\r\n    height: 600px;\r\n    width: 1200px;\r\n    box-sizing: border-box;\r\n    .left {\r\n      box-sizing: border-box;\r\n      padding: 30px;\r\n      float: left;\r\n      width: 800px;\r\n      height: 100%;\r\n      background-color: #f3f5ff;\r\n      img {\r\n        height: 100%;\r\n      }\r\n    }\r\n    .right {\r\n      float: left;\r\n      width: 398px;\r\n      box-sizing: border-box;\r\n      padding-left: 50px;\r\n      padding-top: 50px;\r\n      h3 {\r\n        color: #2B79FF;\r\n        margin-top: 35px;\r\n      }\r\n      .el-form {\r\n        margin-top: 40px;\r\n        padding-right: 15px;\r\n        .code {\r\n          display: flex;\r\n          justify-content: space-between;\r\n          .random {\r\n            background-color: #ddd;\r\n            padding: 0 10px;\r\n            letter-spacing: 2px;\r\n            font-weight: 700;\r\n            text-align: center;\r\n            cursor: pointer;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n// 返回首页\r\n.back {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  letter-spacing: 2px;\r\n  cursor: pointer;\r\n  color: #888888;\r\n  font-size: 14px;\r\n  img {\r\n    vertical-align: middle;\r\n    width: 15px;\r\n    height: 15px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/login"}]}